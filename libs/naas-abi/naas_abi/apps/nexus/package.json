{
    "name": "nexus",
    "version": "0.0.1",
    "private": true,
    "workspaces": [
      "apps/*",
      "packages/*"
    ],
    "scripts": {
      "dev": "turbo run dev",
      "build": "turbo run build",
      "lint": "turbo run lint",
      "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
      "test": "turbo run test",
      "clean": "turbo run clean && rm -rf node_modules",
      "typecheck": "turbo run typecheck"
    },
    "devDependencies": {
      "@playwright/test": "^1.58.2",
      "@types/node": "^20.11.0",
      "prettier": "^3.2.4",
      "turbo": "^2.0.0",
      "typescript": "^5.3.3"
    },
    "packageManager": "pnpm@8.15.0",
    "engines": {
      "node": ">=18.0.0"
    },
    "dependencies": {
      "vis-data": "^8.0.3",
      "vis-network": "^10.0.2"
    }
  }
  