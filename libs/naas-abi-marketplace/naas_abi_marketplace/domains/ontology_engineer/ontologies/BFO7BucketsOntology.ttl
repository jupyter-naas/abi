@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix dc:    <http://purl.org/dc/terms/> .
@prefix bfo:   <http://purl.obolibrary.org/obo/> .
@prefix abi:   <http://ontology.naas.ai/abi/> .

#################################################################
# Ontology
#################################################################

abi:BFO_7Buckets a owl:Ontology ;
  dc:title "BFO 7 Buckets Ontology"@en ;
  dc:description "Ontology for the BFO 7 buckets process"@en ;
  dc:license <https://creativecommons.org/licenses/by/4.0/> ;
  rdfs:comment "Ontology for the BFO 7 buckets process"@en .

#################################################################
# BFO classes with restrictions
#################################################################

# Process boundary (WHAT)
<http://purl.obolibrary.org/obo/BFO_0000035> a owl:Class ;
    rdfs:label "process boundary"@en ;
    rdfs:subClassOf <http://purl.obolibrary.org/obo/BFO_0000003> ,  # occurrent
        [ a owl:Restriction ;
          # occupies temporal region (BFO_0000199) → temporal instant (BFO_0000203)
          owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000199> ;
          owl:allValuesFrom <http://purl.obolibrary.org/obo/BFO_0000203>  # temporal instant
        ] .

# Process (WHAT)
<http://purl.obolibrary.org/obo/BFO_0000015> a owl:Class ;
    rdfs:label "process"@en ;
    rdfs:subClassOf <http://purl.obolibrary.org/obo/BFO_0000003> ,  # occurrent
        [ a owl:Restriction ;
          # has occurrent part (BFO_0000117) → process (BFO_0000015) or process boundary (BFO_0000035)
          owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000117> ;
          owl:allValuesFrom [ a owl:Class ;
              owl:unionOf ( <http://purl.obolibrary.org/obo/BFO_0000015>  # process
                          <http://purl.obolibrary.org/obo/BFO_0000035>  # process boundary
                          )
          ]
        ] ,
        [ a owl:Restriction ;
          # precedes (BFO_0000062) → process (BFO_0000015)
          owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000062> ;
          owl:allValuesFrom <http://purl.obolibrary.org/obo/BFO_0000015>  # process
        ] ,
        [ a owl:Restriction ;
          # preceded by (BFO_0000063) → process (BFO_0000015)
          owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000063> ;
          owl:allValuesFrom <http://purl.obolibrary.org/obo/BFO_0000015>  # process
        ] ,
        [ a owl:Restriction ;
          # occupies temporal region (BFO_0000199) → temporal region (BFO_0000008)
          owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000199> ;
          owl:allValuesFrom <http://purl.obolibrary.org/obo/BFO_0000008>  # temporal region
        ] ,
        [ a owl:Restriction ;
          # has participant (BFO_0000057) → material entity (BFO_0000040) or quality (BFO_0000019)
          owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000057> ;
          owl:allValuesFrom [ a owl:Class ;
              owl:unionOf ( <http://purl.obolibrary.org/obo/BFO_0000040>  # material entity
                          <http://purl.obolibrary.org/obo/BFO_0000019>  # quality
                          )
          ]
        ] ,
        [ a owl:Restriction ;
          # concretizes (BFO_0000059) → generically dependent continuant (BFO_0000031)
          owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000059> ;
          owl:someValuesFrom <http://purl.obolibrary.org/obo/BFO_0000031>  # generically dependent continuant
        ] ,
        [ a owl:Restriction ;
          # occurs in (BFO_0000066) → site (BFO_0000029) or material entity (BFO_0000040)
          owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000066> ;
          owl:allValuesFrom [ a owl:Class ;
              owl:unionOf ( <http://purl.obolibrary.org/obo/BFO_0000029>  # site
                          <http://purl.obolibrary.org/obo/BFO_0000040>  # material entity
                          )
          ]
        ] ,
        [ a owl:Restriction ;
          # realizes (BFO_0000055) → role (BFO_0000023) or disposition (BFO_0000016)
          owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000055> ;
          owl:someValuesFrom [ a owl:Class ;
              owl:unionOf ( <http://purl.obolibrary.org/obo/BFO_0000023>  # role
                            <http://purl.obolibrary.org/obo/BFO_0000016>  # disposition
                          )
          ]
        ] .

# Temporal region (WHEN)
<http://purl.obolibrary.org/obo/BFO_0000008> a owl:Class ;
    rdfs:label "temporal region"@en ;
    rdfs:subClassOf <http://purl.obolibrary.org/obo/BFO_0000003> .

# Material entity (WHO)
<http://purl.obolibrary.org/obo/BFO_0000040> a owl:Class ;
    rdfs:label "material entity"@en ;
    rdfs:subClassOf <http://purl.obolibrary.org/obo/BFO_0000004> ,  # independent continuant
        [ a owl:Restriction ;
          # located in (BFO_0000171) → site (BFO_0000029)
          owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000171> ;
          owl:allValuesFrom <http://purl.obolibrary.org/obo/BFO_0000029>  # site
        ] ,
        [ a owl:Restriction ;
          # is carrier of (BFO_0000101) → generically dependent continuant (BFO_0000031)
          owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000101> ;
          owl:allValuesFrom <http://purl.obolibrary.org/obo/BFO_0000031>  # generically dependent continuant
        ] ,
        [ a owl:Restriction ;
          # bearer of (BFO_0000196) → quality (BFO_0000019) or role (BFO_0000023) or disposition (BFO_0000016)
          owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000196> ;
          owl:allValuesFrom [ a owl:Class ;
              owl:unionOf ( <http://purl.obolibrary.org/obo/BFO_0000019>  # quality
                          <http://purl.obolibrary.org/obo/BFO_0000023>    # role
                          <http://purl.obolibrary.org/obo/BFO_0000016>    # disposition
                          )
          ]
        ] ,
        [ a owl:Restriction ;
          # material basis of (BFO_0000127) → disposition (BFO_0000016)
          owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000127> ;
          owl:allValuesFrom <http://purl.obolibrary.org/obo/BFO_0000016>  # disposition
        ] ,
        [ a owl:Restriction ;
          # participates in (BFO_0000055) → process (BFO_0000015)
          owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000055> ;
          owl:allValuesFrom <http://purl.obolibrary.org/obo/BFO_0000015>  # process
        ] .

# Site (WHERE)
<http://purl.obolibrary.org/obo/BFO_0000029> a owl:Class ;
    rdfs:label "site"@en ;
    rdfs:subClassOf 
        <http://purl.obolibrary.org/obo/BFO_0000141> ,  # immaterial entity
        [ a owl:Restriction ;
          # environs (BFO_0000183) → process (BFO_0000015)
          owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000183> ;
          owl:someValuesFrom <http://purl.obolibrary.org/obo/BFO_0000015>  # process
        ] .

# Generically dependent continuant / ICE (HOW WE KNOW)
<http://purl.obolibrary.org/obo/BFO_0000031> a owl:Class ;
    rdfs:label "generically dependent continuant"@en ;
    rdfs:subClassOf <http://purl.obolibrary.org/obo/BFO_0000002> ;  # continuant
    rdfs:subClassOf 
        [ a owl:Restriction ;
          # is concretized by (BFO_0000058) → process (BFO_0000015) or quality (BFO_0000019) or role (BFO_0000023) or disposition (BFO_0000016)
          owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000058> ;
          owl:someValuesFrom [ a owl:Class ;
              owl:unionOf ( <http://purl.obolibrary.org/obo/BFO_0000015>  # process
                            <http://purl.obolibrary.org/obo/BFO_0000019>  # quality
                            <http://purl.obolibrary.org/obo/BFO_0000023>  # role
                            <http://purl.obolibrary.org/obo/BFO_0000016>  # disposition
                          )
          ]
        ] ,
        [ a owl:Restriction ;
          # generically depends on (BFO_0000084) → material entity (BFO_0000040)
          owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000084> ;
          owl:someValuesFrom <http://purl.obolibrary.org/obo/BFO_0000040>  # material entity
        ] .

# Quality (HOW IT IS)
<http://purl.obolibrary.org/obo/BFO_0000019> a owl:Class ;
    rdfs:label "quality"@en ;
    rdfs:subClassOf <http://purl.obolibrary.org/obo/BFO_0000020> ,  # specifically dependent continuant
        [ a owl:Restriction ;
          # concretizes (BFO_0000059) → generically dependent continuant (BFO_0000031)
          owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000059> ;
          owl:someValuesFrom <http://purl.obolibrary.org/obo/BFO_0000031>  # Generically dependent continuant
        ] ,
        [ a owl:Restriction ;
          # participates in (BFO_0000055) → process (BFO_0000015)
          owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000055> ;
          owl:someValuesFrom <http://purl.obolibrary.org/obo/BFO_0000015>  # Process
        ] ,
        [ a owl:Restriction ;
          # inheres in (BFO_0000197) → material entity (BFO_0000040)
          owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000197> ;
          owl:someValuesFrom <http://purl.obolibrary.org/obo/BFO_0000040>  # Material Entity
        ] .

# Role (WHY)
<http://purl.obolibrary.org/obo/BFO_0000023> a owl:Class ;
    rdfs:label "role"@en ;
    rdfs:subClassOf <http://purl.obolibrary.org/obo/BFO_0000017> ,
        [ a owl:Restriction ;
          # has realization (BFO_0000054) → process (BFO_0000015)
          owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000054> ;
          owl:someValuesFrom <http://purl.obolibrary.org/obo/BFO_0000015>
        ] ,
        [ a owl:Restriction ;
          # concretizes (BFO_0000059) → generically dependent continuant (BFO_0000031)
          owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000059> ;
          owl:someValuesFrom <http://purl.obolibrary.org/obo/BFO_0000031>
        ] ,
        [ a owl:Restriction ;
          # inheres in (BFO_0000197) → material entity (BFO_0000040)
          owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000197> ;
          owl:someValuesFrom <http://purl.obolibrary.org/obo/BFO_0000040>
        ] .

# Disposition (WHY)
<http://purl.obolibrary.org/obo/BFO_0000016> a owl:Class ;
    rdfs:label "disposition"@en ;
    rdfs:subClassOf <http://purl.obolibrary.org/obo/BFO_0000017> ,  # realizable entity
        [ a owl:Restriction ;
          # has realization (BFO_0000054) → process (BFO_0000015)
          owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000054> ;
          owl:someValuesFrom <http://purl.obolibrary.org/obo/BFO_0000015>
        ] ,
        [ a owl:Restriction ;
          # concretizes (BFO_0000059) → generically dependent continuant (BFO_0000031)
          owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000059> ;
          owl:someValuesFrom <http://purl.obolibrary.org/obo/BFO_0000031>
        ] ,
        [ a owl:Restriction ;
          # has material basis (BFO_0000218) → material entity (BFO_0000040)
          owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000218> ;
          owl:someValuesFrom <http://purl.obolibrary.org/obo/BFO_0000040>
        ] .