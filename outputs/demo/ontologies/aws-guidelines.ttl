bfo:BFO_0000029 a owl:Class ;
    rdfs:label "site"@en ;
    dc11:identifier "034-BFO" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom [ a owl:Class ;
                    owl:unionOf ( bfo:BFO_0000029 bfo:BFO_0000040 ) ] ;
            owl:onProperty bfo:BFO_0000176 ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a owl:Class ;
                    owl:unionOf ( bfo:BFO_0000029 bfo:BFO_0000140 ) ] ;
