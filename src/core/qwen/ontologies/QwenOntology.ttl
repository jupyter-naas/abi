@prefix abi: <http://ontology.naas.ai/abi/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix capability: <http://ontology.naas.ai/abi/capability/> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
<http://ontology.naas.ai/abi/QwenOntology> a owl:Ontology ;
    owl:imports <http://ontology.naas.ai/abi/AIAgentOntology> ;
    dc:title "Qwen AI Agent Ontology"@en ;
    dc:description "BFO-grounded ontology for Qwen AI models and processes"@en ;
    dc:created "20250812T101446"@en .


#################################################################
#    BFO Bucket 1: Material Entities (WHAT/WHO)
#################################################################

# Material Infrastructure (BFO_0000040)
abi:QwenServerInfrastructure a bfo:BFO_0000040 ;
    rdfs:label "Qwen Server Infrastructure"@en ;
    rdfs:comment "Material infrastructure supporting Qwen systems for general AI processing"@en ;
    bfo:BFO_0000171 abi:QwenDataCenterRegion . # located in

#################################################################
#    BFO Bucket 2: Qualities (WHAT-LIKE)
#################################################################

# Qualities that inhere in material entities (BFO_0000019)
abi:QwenProcessingAccuracy a bfo:BFO_0000019 ;
    rdfs:label "Qwen Processing Accuracy"@en ;
    rdfs:comment "Quality representing accuracy of Qwen AI processing"@en ;
    bfo:BFO_0000197 abi:QwenServerInfrastructure . # inheres in

#################################################################
#    BFO Bucket 3: Realizable Entities (WHAT-CAN-BE)
#################################################################

# Capabilities that inhere in material entities (BFO_0000017)
abi:QwenGeneralProcessingCapability a bfo:BFO_0000017 ;
    rdfs:label "Qwen General Processing Capability"@en ;
    rdfs:comment "Capability for general AI processing and task execution"@en ;
    bfo:BFO_0000197 abi:QwenServerInfrastructure . # inheres in

#################################################################
#    BFO Bucket 7: Generically Dependent Continuants (INFORMATION)
#################################################################

# AI Agent as software pattern (BFO_0000031)
abi:QwenAgent a abi:AIAgent ;
    rdfs:label "Qwen AI Agent"@en ;
    rdfs:comment "AI Agent software specialized in general AI processing"@en ;
    abi:hasSpecializedRole "General AI processing"@en ;
    bfo:BFO_0000084 abi:QwenServerInfrastructure . # generically depends on

#################################################################
#    BFO Bucket 5: Immaterial Entities (WHERE)
#################################################################

# Spatial regions (BFO_0000141)
abi:QwenDataCenterRegion a bfo:BFO_0000141 ;
    rdfs:label "Qwen Data Center Region"@en ;
    rdfs:comment "Spatial region encompassing Qwen data center locations"@en .

#################################################################
#    BFO Bucket 6: Temporal Regions (WHEN)
#################################################################

# Temporal regions (BFO_0000008)
abi:QwenProcessingSession a bfo:BFO_0000008 ;
    rdfs:label "Qwen Processing Session"@en ;
    rdfs:comment "Temporal region during which Qwen processing occurs"@en .

#################################################################
#    BFO Bucket 4: Processes (HOW-IT-HAPPENS)
#################################################################

# Specialized processes that realize capabilities (BFO_0000015)
abi:QwenGeneralProcessingProcess a bfo:BFO_0000015 ;
    rdfs:label "Qwen General Processing Process"@en ;
    rdfs:comment "Process of general AI processing using Qwen"@en ;
    bfo:BFO_0000057 abi:QwenServerInfrastructure ; # has participant
    bfo:BFO_0000055 abi:QwenGeneralProcessingCapability ; # realizes
    bfo:BFO_0000108 abi:QwenProcessingSession . # exists at

# Note: Additional processes can be defined as needed, following the same BFO pattern

