    rdfs:comment "Material infrastructure supporting Claude systems"@en ;
    skos:example "Physical servers, GPU clusters, network hardware for Claude"@en ;
    bfo:BFO_0000171 abi:AnthropicDataCenterRegion . # located in

#################################################################
#    BFO Bucket 2: Qualities (WHAT-LIKE)
#################################################################

# Qualities that inhere in material entities (BFO_0000019)
abi:ClaudeProfessionalWritingQuality a bfo:BFO_0000019 ;
    rdfs:label "Claude ProfessionalWritingQuality"@en ;
    rdfs:comment "Quality representing excellence in professional writing"@en ;
    skos:example "Claude processing quality metrics"@en ;
    bfo:BFO_0000197 abi:AnthropicServerInfrastructure . # inheres in

#################################################################
#    BFO Bucket 3: Realizable Entities (WHAT-CAN-BE)
#################################################################

# Capabilities that inhere in material entities (BFO_0000017)
abi:ClaudeProfessionalWritingCapability a bfo:BFO_0000017 ;
    rdfs:label "Claude ProfessionalWritingCapability"@en ;
    rdfs:comment "Capability for professional writing and business communication"@en ;
    skos:example "Claude core capability"@en ;
    bfo:BFO_0000197 abi:AnthropicServerInfrastructure . # inheres in

#################################################################
#    BFO Bucket 7: Generically Dependent Continuants (INFORMATION)
#################################################################

# AI Models as software patterns (BFO_0000031) - CONCRETE INSTANCE
abi:Claude35SonnetModel a bfo:BFO_0000031 ;
    rdfs:label "claude-3-5-sonnet"@en ;
    rdfs:comment "Anthropic's most intelligent model, with best-in-class performance for most tasks including complex reasoning and analysis."@en ;
    skos:example "claude-3-5-sonnet neural network"@en ;
    abi:hasModelId "claude-3-5-sonnet-20241022"@en ;
    abi:hasContextWindow "200000"^^xsd:integer ;
    abi:hasOwner "anthropic"@en ;
    abi:hasDescription "Anthropic's most intelligent model, with best-in-class performance for most tasks including complex reasoning and analysis."@en ;
    bfo:BFO_0000084 abi:AnthropicServerInfrastructure . # generically depends on

# System Prompt as behavioral pattern (BFO_0000031) - CONCRETE INSTANCE
abi:ClaudeSystemPrompt a bfo:BFO_0000031 ;
    rdfs:label "Claude System Prompt"@en ;
    rdfs:comment "System prompt defining Claude's role, objectives, and operating guidelines"@en ;
    skos:example "Claude behavioral configuration"@en ;
    abi:hasRole "You are Claude, Anthropic's AI assistant specialized in professional writing and business communication."@en ;
    abi:hasObjective "Create high-quality professional documents with constitutional AI safety"@en ;
    abi:hasTasks "Professional writing; Business proposals; Document creation; Constitutional AI compliance"@en ;
    bfo:BFO_0000084 abi:AnthropicServerInfrastructure . # generically depends on

# Intent Mappings as routing patterns (BFO_0000031) - CONCRETE INSTANCE
abi:ClaudeIntentMappings a bfo:BFO_0000031 ;
    rdfs:label "Claude Intent Mappings"@en ;
    rdfs:comment "Intent-to-tool routing patterns for Claude agent"@en ;
    skos:example "Claude intent routing configuration"@en ;
    abi:hasIntentCount "18"^^xsd:integer ;
    bfo:BFO_0000084 abi:AnthropicServerInfrastructure . # generically depends on

# AI Agent as software pattern (BFO_0000031) - CONCRETE INSTANCE
abi:ClaudeAgent a abi:AIAgent ;
    rdfs:label "Claude"@en ;
    rdfs:comment "Anthropic's Claude AI agent specialized in professional writing and constitutional AI"@en ;
    skos:example "Complete Claude service: model + system prompt + intent mappings"@en ;
    abi:hasSpecializedRole "Professional writing and constitutional AI"@en ;
    abi:usesModel abi:Claude35SonnetModel ;
    abi:usesSystemPrompt abi:ClaudeSystemPrompt ;
    abi:usesIntentMappings abi:ClaudeIntentMappings ;
    bfo:BFO_0000084 abi:AnthropicServerInfrastructure . # generically depends on

