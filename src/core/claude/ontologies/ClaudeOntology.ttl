    rdfs:comment "Process of creating professional documents and business communications"@en ;
    skos:example "Claude executing its core process"@en ;
    bfo:BFO_0000057 abi:AnthropicServerInfrastructure ; # has participant
    bfo:BFO_0000055 abi:ClaudeProfessionalWritingCapability ; # realizes
    bfo:BFO_0000108 abi:ClaudeWritingSession . # exists at
