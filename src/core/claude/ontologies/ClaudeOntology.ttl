#################################################################

# Spatial regions (BFO_0000141)
abi:AnthropicDataCenterRegion a bfo:BFO_0000141 ;
    rdfs:label "Claude Data Center Region"@en ;
    rdfs:comment "Spatial region encompassing Claude data center locations"@en ;
    skos:example "Geographic region where Claude infrastructure is located"@en .

#################################################################
#    BFO Bucket 6: Temporal Regions (WHEN)
#################################################################

# Temporal regions (BFO_0000008)
abi:ClaudeWritingSession a bfo:BFO_0000008 ;
    rdfs:label "Claude WritingSession"@en ;
    rdfs:comment "Temporal region during which Claude processing occurs"@en ;
    skos:example "Claude processing time interval"@en .

#################################################################
#    BFO Bucket 4: Processes (HOW-IT-HAPPENS)
