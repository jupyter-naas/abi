###  http://ontology.naas.ai/abi/AIAgent
abi:AIAgent a owl:Class ;
    rdfs:subClassOf bfo:BFO_0000031 ;
    rdfs:label "AI Agent"@en ;
    skos:definition "A generically dependent continuant that is software capable of autonomous decision-making and task execution, which generically depends on material infrastructure."@en ;
    skos:example "ChatGPT software, Claude software, Gemini software - the information patterns that run on hardware infrastructure."@en .


#################################################################
#    General axioms
#################################################################

# Class Axioms
abi:AIModelInstance rdfs:subClassOf abi:AISystem ;
    rdfs:comment "Every AI model instance is a type of AI system."@en .

abi:TextGenerationProcess rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty abi:realizesCapability ;
    owl:someValuesFrom capability:TextGenerationCapability ;
    rdfs:comment "Every text generation process must realize a text generation capability."@en
] .

abi:ImageAnalysisProcess rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty abi:realizesCapability ;
    owl:someValuesFrom capability:ImageAnalysisCapability ;
    rdfs:comment "Every image analysis process must realize an image analysis capability."@en
] .

abi:CodeGenerationProcess rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty abi:realizesCapability ;
    owl:someValuesFrom capability:CodeGenerationCapability ;
    rdfs:comment "Every code generation process must realize a code generation capability."@en
] .

# Core Relationship Axioms
abi:AIAgent rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty bfo:BFO_0000084 ; # generically depends on
    owl:someValuesFrom bfo:BFO_0000040 ; # material entity
    rdfs:comment "Every AI agent must generically depend on some material infrastructure."@en
] .

# Model-Process Relationship Axioms
bfo:BFO_0000015 rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty abi:utilizesModel ;
    owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
    rdfs:comment "Each process instance utilizes exactly one AI model instance."@en
] .

# Core Property Axioms
abi:hasAIAgent owl:inverseOf abi:isAIAgentOf ;
    rdfs:comment "If an AI system has an AI agent, then that AI agent is an agent of that AI system."@en .

abi:realizesCapability owl:inverseOf [
    rdf:type owl:ObjectProperty ;
    rdfs:label "is realized by"@en ;
    rdfs:domain bfo:BFO_0000017 ;
    rdfs:range bfo:BFO_0000015 ;
    rdfs:comment "If a process realizes a capability, then that capability is realized by that process."@en
] .

# Disjoint Classes
capability:TextGenerationCapability owl:disjointWith capability:ImageAnalysisCapability .
capability:TextGenerationCapability owl:disjointWith capability:CodeGenerationCapability .
capability:ImageAnalysisCapability owl:disjointWith capability:CodeGenerationCapability .

abi:TextGenerationProcess owl:disjointWith abi:ImageAnalysisProcess .
abi:TextGenerationProcess owl:disjointWith abi:CodeGenerationProcess .
abi:ImageAnalysisProcess owl:disjointWith abi:CodeGenerationProcess .

abi:ModelAccuracy owl:disjointWith abi:ResponseLatency .
abi:ModelAccuracy owl:disjointWith abi:TokenCapacity .
abi:ResponseLatency owl:disjointWith abi:TokenCapacity .

abi:AISystem owl:disjointWith abi:LoadBalancer .

# Functional Properties
abi:modelName a owl:FunctionalProperty ;
    rdfs:comment "Each AI system can have only one model name."@en .

abi:providerName a owl:FunctionalProperty ;
    rdfs:comment "Each AI system can have only one provider name."@en .

abi:accuracyValue a owl:FunctionalProperty ;
    rdfs:comment "Each model accuracy quality can have only one numerical value."@en .

abi:latencyValue a owl:FunctionalProperty ;
    rdfs:comment "Each response latency quality can have only one numerical value."@en .

abi:tokenLimit a owl:FunctionalProperty ;
    rdfs:comment "Each token capacity quality can have only one numerical value."@en .

abi:trainingDataCutoff a owl:FunctionalProperty ;
    rdfs:comment "Each model training period can have only one cutoff date."@en .

abi:locationName a owl:FunctionalProperty ;
    rdfs:comment "Each spatial region can have only one location name."@en .

abi:collaborationPriority a owl:FunctionalProperty ;
    rdfs:comment "Each AI agent can have only one collaboration priority value."@en .

abi:systemComplexity a owl:FunctionalProperty ;
    rdfs:comment "Each AI system can have only one complexity level."@en .

abi:temporalSequence a owl:FunctionalProperty ;
    rdfs:comment "Each process can have only one sequence number in a workflow."@en .

