#################################################################
#    Data Property Axioms
#################################################################

# Data Property Axioms
abi:accuracyValue rdfs:subPropertyOf [
    rdf:type owl:DatatypeProperty ;
    rdfs:range [ rdf:type rdfs:Datatype ;
                 owl:onDatatype xsd:decimal ;
                 owl:withRestrictions ( [ xsd:minInclusive "0.0"^^xsd:decimal ;
                                         xsd:maxInclusive "1.0"^^xsd:decimal ] ) ] ;
    rdfs:comment "Accuracy values must be between 0.0 and 1.0 (0% to 100%)."@en
] .

abi:latencyValue rdfs:subPropertyOf [
    rdf:type owl:DatatypeProperty ;
    rdfs:range [ rdf:type rdfs:Datatype ;
                 owl:onDatatype xsd:integer ;
                 owl:withRestrictions ( [ xsd:minInclusive "0"^^xsd:integer ] ) ] ;
    rdfs:comment "Latency values must be non-negative integers (milliseconds)."@en
] .

abi:tokenLimit rdfs:subPropertyOf [
    rdf:type owl:DatatypeProperty ;
    rdfs:range [ rdf:type rdfs:Datatype ;
                 owl:onDatatype xsd:integer ;
                 owl:withRestrictions ( [ xsd:minInclusive "1"^^xsd:integer ] ) ] ;
    rdfs:comment "Token limits must be positive integers (at least 1 token)."@en
] .

abi:collaborationPriority rdfs:subPropertyOf [
    rdf:type owl:DatatypeProperty ;
    rdfs:range [ rdf:type rdfs:Datatype ;
                 owl:onDatatype xsd:integer ;
                 owl:withRestrictions ( [ xsd:minInclusive "1"^^xsd:integer ;
                                         xsd:maxInclusive "10"^^xsd:integer ] ) ] ;
    rdfs:comment "Collaboration priority must be between 1 (lowest) and 10 (highest)."@en
] .

abi:temporalSequence rdfs:subPropertyOf [
    rdf:type owl:DatatypeProperty ;
    rdfs:range [ rdf:type rdfs:Datatype ;
                 owl:onDatatype xsd:integer ;
                 owl:withRestrictions ( [ xsd:minInclusive "1"^^xsd:integer ] ) ] ;
    rdfs:comment "Temporal sequence numbers must be positive integers starting from 1."@en
] .


