@prefix abi: <http://ontology.naas.ai/abi/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix capability: <http://ontology.naas.ai/abi/capability/> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
<http://ontology.naas.ai/abi/LlamaOntology> a owl:Ontology ;
    owl:imports <http://ontology.naas.ai/abi/AIAgentOntology> ;
    dc:title "Llama AI Agent Ontology"@en ;
    dc:description "BFO-grounded ontology for Llama AI models and processes"@en ;
    dc:created "20250812T101446"@en .


#################################################################
#    BFO Bucket 1: Material Entities (WHAT/WHO)
#################################################################

# Material Infrastructure (BFO_0000040)
abi:LlamaServerInfrastructure a bfo:BFO_0000040 ;
    rdfs:label "Llama Server Infrastructure"@en ;
    rdfs:comment "Material infrastructure supporting Llama systems for general AI processing"@en ;
    bfo:BFO_0000171 abi:LlamaDataCenterRegion . # located in

#################################################################
#    BFO Bucket 2: Qualities (WHAT-LIKE)
#################################################################

# Qualities that inhere in material entities (BFO_0000019)
abi:LlamaProcessingAccuracy a bfo:BFO_0000019 ;
    rdfs:label "Llama Processing Accuracy"@en ;
    rdfs:comment "Quality representing accuracy of Llama AI processing"@en ;
    bfo:BFO_0000197 abi:LlamaServerInfrastructure . # inheres in

#################################################################
#    BFO Bucket 3: Realizable Entities (WHAT-CAN-BE)
#################################################################

# Capabilities that inhere in material entities (BFO_0000017)
abi:LlamaGeneralProcessingCapability a bfo:BFO_0000017 ;
    rdfs:label "Llama General Processing Capability"@en ;
    rdfs:comment "Capability for general AI processing and task execution"@en ;
    bfo:BFO_0000197 abi:LlamaServerInfrastructure . # inheres in

#################################################################
#    BFO Bucket 7: Generically Dependent Continuants (INFORMATION)
#################################################################

# AI Agent as software pattern (BFO_0000031)
abi:LlamaAgent a abi:AIAgent ;
    rdfs:label "Llama AI Agent"@en ;
    rdfs:comment "AI Agent software specialized in general AI processing"@en ;
    abi:hasSpecializedRole "General AI processing"@en ;
    bfo:BFO_0000084 abi:LlamaServerInfrastructure . # generically depends on

#################################################################
#    BFO Bucket 5: Immaterial Entities (WHERE)
#################################################################

# Spatial regions (BFO_0000141)
abi:LlamaDataCenterRegion a bfo:BFO_0000141 ;
    rdfs:label "Llama Data Center Region"@en ;
    rdfs:comment "Spatial region encompassing Llama data center locations"@en .

#################################################################
#    BFO Bucket 6: Temporal Regions (WHEN)
#################################################################

# Temporal regions (BFO_0000008)
abi:LlamaProcessingSession a bfo:BFO_0000008 ;
    rdfs:label "Llama Processing Session"@en ;
    rdfs:comment "Temporal region during which Llama processing occurs"@en .

#################################################################
#    BFO Bucket 4: Processes (HOW-IT-HAPPENS)
#################################################################

# Specialized processes that realize capabilities (BFO_0000015)
abi:LlamaGeneralProcessingProcess a bfo:BFO_0000015 ;
    rdfs:label "Llama General Processing Process"@en ;
    rdfs:comment "Process of general AI processing using Llama"@en ;
    bfo:BFO_0000057 abi:LlamaServerInfrastructure ; # has participant
    bfo:BFO_0000055 abi:LlamaGeneralProcessingCapability ; # realizes
    bfo:BFO_0000108 abi:LlamaProcessingSession . # exists at

# Note: Additional processes can be defined as needed, following the same BFO pattern

# Note: This ontology now contains only the essential BFO-compliant structure.
# Additional temporal regions, model instances, qualities, and specifications
# can be added as needed following the established BFO patterns above.
