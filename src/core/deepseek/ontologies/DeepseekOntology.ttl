#################################################################

# Spatial regions (BFO_0000141)
abi:EasternDataCenterRegion a bfo:BFO_0000141 ;
    rdfs:label "DeepSeek Data Center Region"@en ;
    rdfs:comment "Spatial region encompassing DeepSeek data center locations"@en ;
    skos:example "Geographic region where DeepSeek infrastructure is located"@en .

#################################################################
#    BFO Bucket 6: Temporal Regions (WHEN)
#################################################################

# Temporal regions (BFO_0000008)
abi:DeepSeekRegionalAnalysisSession a bfo:BFO_0000008 ;
    rdfs:label "DeepSeek RegionalAnalysisSession"@en ;
    rdfs:comment "Temporal region during which DeepSeek processing occurs"@en ;
    skos:example "DeepSeek processing time interval"@en .

#################################################################
#    BFO Bucket 4: Processes (HOW-IT-HAPPENS)
#################################################################

