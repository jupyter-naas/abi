    rdfs:label "DeepSeek Eastern Context Accuracy"@en ;
    rdfs:comment "Quality representing accuracy of Eastern context analysis"@en ;
    bfo:BFO_0000197 abi:DeepSeekServerInfrastructure . # inheres in

abi:DeepSeekRegionalRelevance a bfo:BFO_0000019 ;
    rdfs:label "DeepSeek Regional Relevance"@en ;
