abi:DeepSeekEasternContextAnalysisProcess a abi:RegionalContextAnalysisProcess ;
    rdfs:label "DeepSeek Eastern Context Analysis Process"@en ;
    rdfs:comment "Process of analyzing Eastern cultural and regional contexts using DeepSeek"@en ;
    bfo:BFO_0000057 abi:DeepSeekServerInfrastructure ; # has participant
    bfo:BFO_0000055 abi:DeepSeekEasternContextCapability ; # realizes
    bfo:BFO_0000108 abi:DeepSeekRegionalAnalysisSession . # exists at

