    rdfs:comment "Process of analyzing Eastern regional and cultural contexts"@en ;
    skos:example "DeepSeek executing its core process"@en ;
    bfo:BFO_0000057 abi:DeepSeekServerInfrastructure ; # has participant
    bfo:BFO_0000055 abi:DeepSeekRegionalContextAnalysisCapability ; # realizes
    bfo:BFO_0000108 abi:DeepSeekRegionalAnalysisSession . # exists at
