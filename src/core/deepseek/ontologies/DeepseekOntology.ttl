#################################################################
#    BFO Bucket 7: Generically Dependent Continuants (INFORMATION)
#################################################################

# AI Models as software patterns (BFO_0000031) - CONCRETE INSTANCE
abi:DeepseekR18BModel a bfo:BFO_0000031 ;
    rdfs:label "DeepSeek R1 8B"@en ;
    rdfs:comment "Advanced reasoning model with strong performance in mathematics and problem-solving, optimized for Eastern context"@en ;
    skos:example "DeepSeek-R1-8B neural network, 32K token context, Eastern regional optimization"@en ;
    abi:hasModelId "deepseek-r1:8b"@en ;
    abi:hasContextWindow "32768"^^xsd:integer ;
    abi:hasOwner "ollama"@en ;
    abi:hasDescription "Advanced reasoning model with strong performance in mathematics and problem-solving, optimized for Eastern context"@en ;
    bfo:BFO_0000084 abi:DeepSeekServerInfrastructure . # generically depends on

# System Prompt as behavioral pattern (BFO_0000031) - CONCRETE INSTANCE
abi:DeepseekSystemPrompt a bfo:BFO_0000031 ;
    rdfs:label "DeepSeek System Prompt"@en ;
    rdfs:comment "System prompt defining DeepSeek's role in Eastern regional context analysis"@en ;
    skos:example "Role: regional analyst, Tasks: Eastern context analysis, Guidelines: cultural understanding"@en ;
    abi:hasRole "You are DeepSeek, an AI assistant specialized in Eastern regional context analysis and advanced reasoning."@en ;
    abi:hasObjective "Provide Eastern regional context and advanced analytical reasoning"@en ;
    abi:hasTasks "Eastern context analysis; Regional research; Advanced reasoning; Cultural understanding"@en ;
    bfo:BFO_0000084 abi:DeepSeekServerInfrastructure . # generically depends on

# Intent Mappings as routing patterns (BFO_0000031) - CONCRETE INSTANCE
abi:DeepseekIntentMappings a bfo:BFO_0000031 ;
    rdfs:label "DeepSeek Intent Mappings"@en ;
    rdfs:comment "Intent-to-tool routing patterns for DeepSeek agent"@en ;
    skos:example "analyze Eastern context → deepseek_analyze, research region → deepseek_research"@en ;
    abi:hasIntentCount "10"^^xsd:integer ;
    bfo:BFO_0000084 abi:DeepSeekServerInfrastructure . # generically depends on

# AI Agent as software pattern (BFO_0000031) - CONCRETE INSTANCE
abi:DeepSeekAgent a abi:AIAgent ;
    rdfs:label "DeepSeek"@en ;
    rdfs:comment "DeepSeek AI agent specialized in Eastern regional context and advanced reasoning"@en ;
    skos:example "DeepSeek service using DeepSeek-R1-8B for Eastern context analysis"@en ;
    abi:hasSpecializedRole "Eastern context and regional analysis"@en ;
    abi:usesModel abi:DeepseekR18BModel ;
    abi:usesSystemPrompt abi:DeepseekSystemPrompt ;
    abi:usesIntentMappings abi:DeepseekIntentMappings ;
    bfo:BFO_0000084 abi:DeepSeekServerInfrastructure . # generically depends on

