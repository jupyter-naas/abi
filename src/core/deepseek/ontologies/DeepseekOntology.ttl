    dc:created "20250812T101446"@en .


#################################################################
#    BFO Bucket 1: Material Entities (WHAT/WHO)
#################################################################

# Material Infrastructure (BFO_0000040)
abi:DeepSeekServerInfrastructure a bfo:BFO_0000040 ;
    rdfs:label "DeepSeek Server Infrastructure"@en ;
    rdfs:comment "Material infrastructure supporting DeepSeek systems"@en ;
    skos:example "Physical servers, GPU clusters, network hardware for DeepSeek"@en ;
    bfo:BFO_0000171 abi:EasternDataCenterRegion . # located in

#################################################################
#    BFO Bucket 2: Qualities (WHAT-LIKE)
#################################################################

# Qualities that inhere in material entities (BFO_0000019)
abi:DeepSeekRegionalContextAccuracy a bfo:BFO_0000019 ;
    rdfs:label "DeepSeek RegionalContextAccuracy"@en ;
    rdfs:comment "Quality representing accuracy in Eastern regional context analysis"@en ;
    skos:example "DeepSeek processing quality metrics"@en ;
    bfo:BFO_0000197 abi:DeepSeekServerInfrastructure . # inheres in

#################################################################
#    BFO Bucket 3: Realizable Entities (WHAT-CAN-BE)
#################################################################

# Capabilities that inhere in material entities (BFO_0000017)
abi:DeepSeekRegionalContextAnalysisCapability a bfo:BFO_0000017 ;
    rdfs:label "DeepSeek RegionalContextAnalysisCapability"@en ;
    rdfs:comment "Capability for analyzing Eastern regional and cultural context"@en ;
    skos:example "DeepSeek core capability"@en ;
    bfo:BFO_0000197 abi:DeepSeekServerInfrastructure . # inheres in

#################################################################
#    BFO Bucket 7: Generically Dependent Continuants (INFORMATION)
#################################################################

# AI Models as software patterns (BFO_0000031) - CONCRETE INSTANCE
abi:DeepSeekV3Model a bfo:BFO_0000031 ;
    rdfs:label "deepseek-v3"@en ;
    rdfs:comment "DeepSeek's advanced model specialized in Eastern regional context and analytical reasoning."@en ;
    skos:example "deepseek-v3 neural network"@en ;
    abi:hasModelId "deepseek-chat"@en ;
    abi:hasContextWindow "65536"^^xsd:integer ;
    abi:hasOwner "deepseek"@en ;
    abi:hasDescription "DeepSeek's advanced model specialized in Eastern regional context and analytical reasoning."@en ;
    bfo:BFO_0000084 abi:DeepSeekServerInfrastructure . # generically depends on

# System Prompt as behavioral pattern (BFO_0000031) - CONCRETE INSTANCE
abi:DeepSeekSystemPrompt a bfo:BFO_0000031 ;
    rdfs:label "DeepSeek System Prompt"@en ;
    rdfs:comment "System prompt defining DeepSeek's role, objectives, and operating guidelines"@en ;
    skos:example "DeepSeek behavioral configuration"@en ;
    abi:hasRole "You are DeepSeek, an AI assistant specialized in Eastern regional context analysis and advanced reasoning."@en ;
    abi:hasObjective "Provide Eastern regional context and advanced analytical reasoning"@en ;
    abi:hasTasks "Eastern context analysis; Regional research; Cultural analysis; Advanced reasoning"@en ;
    bfo:BFO_0000084 abi:DeepSeekServerInfrastructure . # generically depends on

# Intent Mappings as routing patterns (BFO_0000031) - CONCRETE INSTANCE
abi:DeepSeekIntentMappings a bfo:BFO_0000031 ;
    rdfs:label "DeepSeek Intent Mappings"@en ;
    rdfs:comment "Intent-to-tool routing patterns for DeepSeek agent"@en ;
    skos:example "DeepSeek intent routing configuration"@en ;
    abi:hasIntentCount "14"^^xsd:integer ;
    bfo:BFO_0000084 abi:DeepSeekServerInfrastructure . # generically depends on

# AI Agent as software pattern (BFO_0000031) - CONCRETE INSTANCE
abi:DeepSeekAgent a abi:AIAgent ;
    rdfs:label "DeepSeek"@en ;
    rdfs:comment "DeepSeek agent specialized in Eastern regional context analysis and advanced reasoning"@en ;
    skos:example "Complete DeepSeek service: model + system prompt + intent mappings"@en ;
    abi:hasSpecializedRole "Eastern regional context and analytical reasoning"@en ;
    abi:usesModel abi:DeepSeekV3Model ;
    abi:usesSystemPrompt abi:DeepSeekSystemPrompt ;
    abi:usesIntentMappings abi:DeepSeekIntentMappings ;
    bfo:BFO_0000084 abi:DeepSeekServerInfrastructure . # generically depends on

