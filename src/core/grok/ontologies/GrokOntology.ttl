@prefix dc: <http://purl.org/dc/terms/> .

<http://naas.ai/ontology/abi/GrokOntology> a owl:Ontology ;
    owl:imports <http://ontology.naas.ai/abi/AIAgentOntology> ;
    dc:title "Grok AI Agent Ontology"@en ;
    dc:description "BFO-grounded ontology for Grok AI models and processes"@en ;
    dc:created "20250812T101446"@en .


#################################################################
#    BFO Bucket 1: Material Entities (WHAT/WHO)
#################################################################

abi:GrokAgent a abi:AIAgent ;
    rdfs:label "Grok AI Agent"@en ;
    rdfs:comment "AI Agent capable of utilizing Grok models"@en ;
    abi:hasSpecializedRole "Multi-purpose AI processing"@en .


#################################################################
#    BFO Bucket 4: Processes (HOW-IT-HAPPENS)
#################################################################

abi:GrokBusinessProposalProcess a abi:BusinessProposalCreationProcess ;
    rdfs:label "Grok Business Proposal Process"@en ;
    abi:hasParticipant abi:GrokAgent ;
    abi:realizesCapability capability:TextGenerationCapability ;
    abi:hasTemporalRegion abi:GrokBusinessProposalSession ;
    abi:hasQuality abi:GrokBusinessProposalQuality .

abi:GrokCreativeWritingProcess a abi:CreativeWritingProcess ;
    rdfs:label "Grok Creative Writing Process"@en ;
    abi:hasParticipant abi:GrokAgent ;
    abi:realizesCapability capability:TextGenerationCapability ;
    abi:hasTemporalRegion abi:GrokCreativeWritingSession ;
    abi:hasQuality abi:GrokCreativeWritingQuality .

abi:GrokCodeGenerationProcess a abi:CodeGenerationProcess ;
    rdfs:label "Grok Code Generation Process"@en ;
    abi:hasParticipant abi:GrokAgent ;
    abi:realizesCapability capability:CodeGenerationCapability ;
    abi:hasTemporalRegion abi:GrokCodeGenerationSession ;
    abi:hasQuality abi:GrokCodeGenerationQuality .

#################################################################
#    BFO Bucket 5: Temporal Regions (WHEN)
#################################################################

abi:GrokBusinessProposalSession a abi:InferenceSession ;
    rdfs:label "Grok Business Proposal Session"@en .

abi:GrokCreativeWritingSession a abi:InferenceSession ;
    rdfs:label "Grok Creative Writing Session"@en .

abi:GrokCodeGenerationSession a abi:InferenceSession ;
    rdfs:label "Grok Code Generation Session"@en .


