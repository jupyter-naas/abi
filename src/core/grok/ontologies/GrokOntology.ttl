@prefix abi: <http://naas.ai/ontology/abi#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix capability: <http://ontology.naas.ai/abi/capability/> .
@prefix dc: <http://purl.org/dc/terms/> .

<http://naas.ai/ontology/abi/GrokOntology> a owl:Ontology ;
    owl:imports <http://ontology.naas.ai/abi/AIAgentOntology> ;
    dc:title "Grok AI Agent Ontology"@en ;
    dc:description "BFO-grounded ontology for Grok AI models and processes"@en ;
    dc:created "20250812T101446"@en .


#################################################################
#    BFO Bucket 1: Material Entities (WHAT/WHO)
#################################################################

# Material Infrastructure (BFO_0000040)
abi:xAIServerInfrastructure a bfo:BFO_0000040 ;
    rdfs:label "xAI Server Infrastructure"@en ;
    rdfs:comment "Material infrastructure supporting Grok systems for trend monitoring and social sentiment analysis"@en ;
    bfo:BFO_0000171 abi:xAIDataCenterRegion . # located in

abi:RealTimeDataStreamingHardware a bfo:BFO_0000040 ;
    rdfs:label "Real-Time Data Streaming Hardware"@en ;
    rdfs:comment "Specialized hardware for real-time data streaming and trend analysis"@en ;
    bfo:BFO_0000171 abi:xAIDataCenterRegion . # located in

#################################################################
#    BFO Bucket 2: Qualities (WHAT-LIKE)
#################################################################

# Qualities that inhere in material entities (BFO_0000019)
abi:GrokTrendDetectionAccuracy a bfo:BFO_0000019 ;
    rdfs:label "Grok Trend Detection Accuracy"@en ;
    rdfs:comment "Quality representing accuracy of trend detection"@en ;
    bfo:BFO_0000197 abi:xAIServerInfrastructure . # inheres in

abi:GrokRealTimeProcessingSpeed a bfo:BFO_0000019 ;
    rdfs:label "Grok Real-Time Processing Speed"@en ;
    rdfs:comment "Quality representing speed of real-time processing"@en ;
    bfo:BFO_0000197 abi:RealTimeDataStreamingHardware . # inheres in

#################################################################
#    BFO Bucket 3: Realizable Entities (WHAT-CAN-BE)
#################################################################

# Capabilities that inhere in material entities (BFO_0000017)
abi:GrokTrendMonitoringCapability a bfo:BFO_0000017 ;
    rdfs:label "Grok Trend Monitoring Capability"@en ;
    rdfs:comment "Capability for monitoring trends and social sentiment analysis"@en ;
    bfo:BFO_0000197 abi:xAIServerInfrastructure ; # inheres in
    bfo:BFO_0000218 abi:RealTimeDataStreamingHardware . # has material basis

#################################################################
#    BFO Bucket 7: Generically Dependent Continuants (INFORMATION)
#################################################################

# AI Models as software patterns (BFO_0000031) - CONCRETE INSTANCE
abi:Grok4Model a bfo:BFO_0000031 ;
    rdfs:label "grok-4-latest"@en ;
    rdfs:comment "xAI's model with real-time information access and social media understanding capabilities"@en ;
    skos:example "Grok-4 neural network, 200K token context, real-time social data access"@en ;
    abi:hasModelId "grok-4-latest"@en ;
    abi:hasContextWindow "200000"^^xsd:integer ;
    abi:hasOwner "xai"@en ;
    abi:hasDescription "xAI's model with real-time information access and social media understanding capabilities"@en ;
    bfo:BFO_0000084 abi:xAIServerInfrastructure . # generically depends on

# System Prompt as behavioral pattern (BFO_0000031) - CONCRETE INSTANCE
abi:GrokSystemPrompt a bfo:BFO_0000031 ;
    rdfs:label "Grok System Prompt"@en ;
    rdfs:comment "System prompt defining Grok's role in trend monitoring and social sentiment analysis"@en ;
    skos:example "Role: trend monitor, Tasks: social sentiment analysis, Guidelines: real-time data"@en ;
    abi:hasRole "You are Grok, xAI's assistant specialized in monitoring global trends and analyzing social sentiment."@en ;
    abi:hasObjective "Monitor and analyze global trends and social conversations"@en ;
    abi:hasTasks "Trend monitoring; Social sentiment analysis; Real-time information; Cultural trends"@en ;
    bfo:BFO_0000084 abi:xAIServerInfrastructure . # generically depends on

# Intent Mappings as routing patterns (BFO_0000031) - CONCRETE INSTANCE
abi:GrokIntentMappings a bfo:BFO_0000031 ;
    rdfs:label "Grok Intent Mappings"@en ;
    rdfs:comment "Intent-to-tool routing patterns for Grok agent"@en ;
    skos:example "monitor trends → grok_monitor, analyze sentiment → grok_sentiment"@en ;
    abi:hasIntentCount "14"^^xsd:integer ;
    bfo:BFO_0000084 abi:xAIServerInfrastructure . # generically depends on

# AI Agent as software pattern (BFO_0000031) - CONCRETE INSTANCE
abi:GrokAgent a abi:AIAgent ;
    rdfs:label "Grok"@en ;
    rdfs:comment "xAI's Grok agent specialized in trend monitoring and social sentiment analysis"@en ;
    skos:example "Grok service using Grok-4 for global trend analysis"@en ;
    abi:hasSpecializedRole "Global trends and social sentiment analysis"@en ;
    abi:usesModel abi:Grok4Model ;
    abi:usesSystemPrompt abi:GrokSystemPrompt ;
    abi:usesIntentMappings abi:GrokIntentMappings ;
    bfo:BFO_0000084 abi:xAIServerInfrastructure . # generically depends on

#################################################################
#    BFO Bucket 5: Immaterial Entities (WHERE)
#################################################################

# Spatial regions (BFO_0000141)
abi:xAIDataCenterRegion a bfo:BFO_0000141 ;
    rdfs:label "xAI Data Center Region"@en ;
    rdfs:comment "Spatial region encompassing xAI data center locations"@en .

abi:GlobalNetworkBoundary a bfo:BFO_0000141 ;
    rdfs:label "Global Network Boundary"@en ;
    rdfs:comment "Immaterial boundary defining global social media and communication networks"@en .

#################################################################
#    BFO Bucket 6: Temporal Regions (WHEN)
#################################################################

# Temporal regions (BFO_0000008)
abi:GrokTrendMonitoringSession a bfo:BFO_0000008 ;
    rdfs:label "Grok Trend Monitoring Session"@en ;
    rdfs:comment "Temporal region during which trend monitoring occurs"@en .

#################################################################
#    BFO Bucket 4: Processes (HOW-IT-HAPPENS)
#################################################################

# Specialized processes that realize capabilities (BFO_0000015)
abi:GrokTrendMonitoringProcess a abi:TrendMonitoringProcess ;
    rdfs:label "Grok Trend Monitoring Process"@en ;
    rdfs:comment "Process of monitoring global trends and analyzing social sentiment using Grok"@en ;
    bfo:BFO_0000057 abi:xAIServerInfrastructure ; # has participant
    bfo:BFO_0000055 abi:GrokTrendMonitoringCapability ; # realizes
    bfo:BFO_0000108 abi:GrokTrendMonitoringSession . # exists at

