@prefix abi: <http://ontology.naas.ai/abi/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix capability: <http://ontology.naas.ai/abi/capability/> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
<http://ontology.naas.ai/abi/GrokOntology> a owl:Ontology ;
    owl:imports <http://ontology.naas.ai/abi/AIAgentOntology> ;
    dc:title "Grok AI Agent Ontology"@en ;
    dc:description "BFO-grounded ontology for Grok AI models and processes"@en ;
    dc:created "20250812T101446"@en .


#################################################################
#    BFO Bucket 1: Material Entities (WHAT/WHO)
#################################################################

# Material Infrastructure (BFO_0000040)
abi:XAIServerInfrastructure a bfo:BFO_0000040 ;
    rdfs:label "Grok Server Infrastructure"@en ;
    rdfs:comment "Material infrastructure supporting Grok systems"@en ;
    skos:example "Physical servers, GPU clusters, network hardware for Grok"@en ;
    bfo:BFO_0000171 abi:XAIDataCenterRegion . # located in

#################################################################
#    BFO Bucket 2: Qualities (WHAT-LIKE)
#################################################################

# Qualities that inhere in material entities (BFO_0000019)
abi:GrokTrendDetectionAccuracy a bfo:BFO_0000019 ;
    rdfs:label "Grok TrendDetectionAccuracy"@en ;
    rdfs:comment "Quality representing accuracy in detecting social trends"@en ;
    skos:example "Grok processing quality metrics"@en ;
    bfo:BFO_0000197 abi:XAIServerInfrastructure . # inheres in

#################################################################
#    BFO Bucket 3: Realizable Entities (WHAT-CAN-BE)
#################################################################

# Capabilities that inhere in material entities (BFO_0000017)
abi:GrokTrendMonitoringCapability a bfo:BFO_0000017 ;
    rdfs:label "Grok TrendMonitoringCapability"@en ;
    rdfs:comment "Capability for monitoring global trends and social sentiment"@en ;
    skos:example "Grok core capability"@en ;
    bfo:BFO_0000197 abi:XAIServerInfrastructure . # inheres in

#################################################################
#    BFO Bucket 7: Generically Dependent Continuants (INFORMATION)
#################################################################

# AI Models as software patterns (BFO_0000031) - CONCRETE INSTANCE
abi:Grok2Model a bfo:BFO_0000031 ;
    rdfs:label "grok-2"@en ;
    rdfs:comment "xAI's Grok-2 model specialized in real-time trend analysis and social sentiment monitoring."@en ;
    skos:example "grok-2 neural network"@en ;
    abi:hasModelId "grok-2"@en ;
    abi:hasContextWindow "32768"^^xsd:integer ;
    abi:hasOwner "xai"@en ;
    abi:hasDescription "xAI's Grok-2 model specialized in real-time trend analysis and social sentiment monitoring."@en ;
    bfo:BFO_0000084 abi:XAIServerInfrastructure . # generically depends on

# System Prompt as behavioral pattern (BFO_0000031) - CONCRETE INSTANCE
abi:GrokSystemPrompt a bfo:BFO_0000031 ;
    rdfs:label "Grok System Prompt"@en ;
    rdfs:comment "System prompt defining Grok's role, objectives, and operating guidelines"@en ;
    skos:example "Grok behavioral configuration"@en ;
    abi:hasRole "You are Grok, xAI's AI assistant specialized in monitoring global trends and analyzing social sentiment."@en ;
    abi:hasObjective "Provide real-time insights on global trends and social media sentiment"@en ;
    abi:hasTasks "Trend monitoring; Social sentiment analysis; Real-time data processing; Global event tracking"@en ;
    bfo:BFO_0000084 abi:XAIServerInfrastructure . # generically depends on

# Intent Mappings as routing patterns (BFO_0000031) - CONCRETE INSTANCE
abi:GrokIntentMappings a bfo:BFO_0000031 ;
    rdfs:label "Grok Intent Mappings"@en ;
    rdfs:comment "Intent-to-tool routing patterns for Grok agent"@en ;
    skos:example "Grok intent routing configuration"@en ;
    abi:hasIntentCount "12"^^xsd:integer ;
    bfo:BFO_0000084 abi:XAIServerInfrastructure . # generically depends on

# AI Agent as software pattern (BFO_0000031) - CONCRETE INSTANCE
abi:GrokAgent a abi:AIAgent ;
    rdfs:label "Grok"@en ;
    rdfs:comment "xAI's Grok agent specialized in real-time trend monitoring and social sentiment analysis"@en ;
    skos:example "Complete Grok service: model + system prompt + intent mappings"@en ;
    abi:hasSpecializedRole "Global trends and social sentiment monitoring"@en ;
    abi:usesModel abi:Grok2Model ;
    abi:usesSystemPrompt abi:GrokSystemPrompt ;
    abi:usesIntentMappings abi:GrokIntentMappings ;
    bfo:BFO_0000084 abi:XAIServerInfrastructure . # generically depends on

#################################################################
#    BFO Bucket 5: Immaterial Entities (WHERE)
#################################################################

# Spatial regions (BFO_0000141)
abi:XAIDataCenterRegion a bfo:BFO_0000141 ;
    rdfs:label "Grok Data Center Region"@en ;
    rdfs:comment "Spatial region encompassing Grok data center locations"@en ;
    skos:example "Geographic region where Grok infrastructure is located"@en .

#################################################################
#    BFO Bucket 6: Temporal Regions (WHEN)
#################################################################

# Temporal regions (BFO_0000008)
abi:GrokTrendMonitoringSession a bfo:BFO_0000008 ;
    rdfs:label "Grok TrendMonitoringSession"@en ;
    rdfs:comment "Temporal region during which Grok processing occurs"@en ;
    skos:example "Grok processing time interval"@en .

#################################################################
#    BFO Bucket 4: Processes (HOW-IT-HAPPENS)
#################################################################

# Specialized processes that realize capabilities (BFO_0000015)
abi:GrokTrendMonitoringProcess a abi:TrendMonitoringProcess ;
    rdfs:label "Grok TrendMonitoringProcess"@en ;
    rdfs:comment "Process of monitoring and analyzing social trends and global sentiment"@en ;
    skos:example "Grok executing its core process"@en ;
    bfo:BFO_0000057 abi:XAIServerInfrastructure ; # has participant
    bfo:BFO_0000055 abi:GrokTrendMonitoringCapability ; # realizes
    bfo:BFO_0000108 abi:GrokTrendMonitoringSession . # exists at
