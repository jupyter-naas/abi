@prefix abi: <http://ontology.naas.ai/abi/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix capability: <http://ontology.naas.ai/abi/capability/> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
<http://ontology.naas.ai/abi/MistralOntology> a owl:Ontology ;
    owl:imports <http://ontology.naas.ai/abi/AIAgentOntology> ;
    dc:title "Mistral AI Agent Ontology"@en ;
    dc:description "BFO-grounded ontology for Mistral AI models and processes"@en ;
    dc:created "20250812T101446"@en .


#################################################################
#    BFO Bucket 1: Material Entities (WHAT/WHO)
#################################################################

# Material Infrastructure (BFO_0000040)
abi:MistralServerInfrastructure a bfo:BFO_0000040 ;
    rdfs:label "Mistral Server Infrastructure"@en ;
    rdfs:comment "Material infrastructure supporting Mistral systems"@en ;
    skos:example "Physical servers, GPU clusters, network hardware for Mistral"@en ;
    bfo:BFO_0000171 abi:MistralDataCenterRegion . # located in

#################################################################
#    BFO Bucket 2: Qualities (WHAT-LIKE)
#################################################################

# Qualities that inhere in material entities (BFO_0000019)
abi:MistralMathematicalPrecision a bfo:BFO_0000019 ;
    rdfs:label "Mistral MathematicalPrecision"@en ;
    rdfs:comment "Quality representing precision in mathematical calculations"@en ;
    skos:example "Mistral processing quality metrics"@en ;
    bfo:BFO_0000197 abi:MistralServerInfrastructure . # inheres in

#################################################################
#    BFO Bucket 3: Realizable Entities (WHAT-CAN-BE)
#################################################################

# Capabilities that inhere in material entities (BFO_0000017)
abi:MistralMathematicalReasoningCapability a bfo:BFO_0000017 ;
    rdfs:label "Mistral MathematicalReasoningCapability"@en ;
    rdfs:comment "Capability for mathematical reasoning and problem solving"@en ;
    skos:example "Mistral core capability"@en ;
    bfo:BFO_0000197 abi:MistralServerInfrastructure . # inheres in

#################################################################
#    BFO Bucket 7: Generically Dependent Continuants (INFORMATION)
#################################################################

# AI Models as software patterns (BFO_0000031) - CONCRETE INSTANCE
abi:MistralLarge2Model a bfo:BFO_0000031 ;
    rdfs:label "mistral-large-2"@en ;
    rdfs:comment "Mistral's large model optimized for mathematics, coding, and logical reasoning tasks."@en ;
    skos:example "mistral-large-2 neural network"@en ;
    abi:hasModelId "mistral-large-2407"@en ;
    abi:hasContextWindow "131072"^^xsd:integer ;
    abi:hasOwner "mistral"@en ;
    abi:hasDescription "Mistral's large model optimized for mathematics, coding, and logical reasoning tasks."@en ;
    bfo:BFO_0000084 abi:MistralServerInfrastructure . # generically depends on

# System Prompt as behavioral pattern (BFO_0000031) - CONCRETE INSTANCE
abi:MistralSystemPrompt a bfo:BFO_0000031 ;
    rdfs:label "Mistral System Prompt"@en ;
    rdfs:comment "System prompt defining Mistral's role, objectives, and operating guidelines"@en ;
    skos:example "Mistral behavioral configuration"@en ;
    abi:hasRole "You are Mistral, a technical AI assistant specialized in mathematics and logical reasoning."@en ;
    abi:hasObjective "Provide precise solutions to mathematical and logical problems"@en ;
    abi:hasTasks "Mathematical calculations; Logical reasoning; Problem solving; Technical analysis"@en ;
    bfo:BFO_0000084 abi:MistralServerInfrastructure . # generically depends on

# Intent Mappings as routing patterns (BFO_0000031) - CONCRETE INSTANCE
abi:MistralIntentMappings a bfo:BFO_0000031 ;
    rdfs:label "Mistral Intent Mappings"@en ;
    rdfs:comment "Intent-to-tool routing patterns for Mistral agent"@en ;
    skos:example "Mistral intent routing configuration"@en ;
    abi:hasIntentCount "15"^^xsd:integer ;
    bfo:BFO_0000084 abi:MistralServerInfrastructure . # generically depends on

# AI Agent as software pattern (BFO_0000031) - CONCRETE INSTANCE
abi:MistralAgent a abi:AIAgent ;
    rdfs:label "Mistral"@en ;
    rdfs:comment "Mistral agent specialized in mathematical reasoning and logical problem solving"@en ;
    skos:example "Complete Mistral service: model + system prompt + intent mappings"@en ;
    abi:hasSpecializedRole "Mathematics and logical reasoning"@en ;
    abi:usesModel abi:MistralLarge2Model ;
    abi:usesSystemPrompt abi:MistralSystemPrompt ;
    abi:usesIntentMappings abi:MistralIntentMappings ;
    bfo:BFO_0000084 abi:MistralServerInfrastructure . # generically depends on

#################################################################
#    BFO Bucket 5: Immaterial Entities (WHERE)
#################################################################

# Spatial regions (BFO_0000141)
abi:MistralDataCenterRegion a bfo:BFO_0000141 ;
    rdfs:label "Mistral Data Center Region"@en ;
    rdfs:comment "Spatial region encompassing Mistral data center locations"@en ;
    skos:example "Geographic region where Mistral infrastructure is located"@en .

#################################################################
#    BFO Bucket 6: Temporal Regions (WHEN)
#################################################################

# Temporal regions (BFO_0000008)
abi:MistralMathematicalSession a bfo:BFO_0000008 ;
    rdfs:label "Mistral MathematicalSession"@en ;
    rdfs:comment "Temporal region during which Mistral processing occurs"@en ;
    skos:example "Mistral processing time interval"@en .

#################################################################
#    BFO Bucket 4: Processes (HOW-IT-HAPPENS)
#################################################################

# Specialized processes that realize capabilities (BFO_0000015)
abi:MistralMathematicalReasoningProcess a abi:MathematicalReasoningProcess ;
    rdfs:label "Mistral MathematicalReasoningProcess"@en ;
    rdfs:comment "Process of performing mathematical calculations and logical reasoning"@en ;
    skos:example "Mistral executing its core process"@en ;
    bfo:BFO_0000057 abi:MistralServerInfrastructure ; # has participant
    bfo:BFO_0000055 abi:MistralMathematicalReasoningCapability ; # realizes
    bfo:BFO_0000108 abi:MistralMathematicalSession . # exists at
