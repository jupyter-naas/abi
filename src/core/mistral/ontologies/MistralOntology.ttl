@prefix abi: <http://naas.ai/ontology/abi#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix capability: <http://ontology.naas.ai/abi/capability/> .
@prefix dc: <http://purl.org/dc/terms/> .

<http://naas.ai/ontology/abi/MistralOntology> a owl:Ontology ;
    owl:imports <http://ontology.naas.ai/abi/AIAgentOntology> ;
    dc:title "Mistral AI Agent Ontology"@en ;
    dc:description "BFO-grounded ontology for Mistral AI models and processes"@en ;
    dc:created "20250812T101446"@en .


#################################################################
#    BFO Bucket 1: Material Entities (WHAT/WHO)
#################################################################

# Material Infrastructure (BFO_0000040)
abi:MistralServerInfrastructure a bfo:BFO_0000040 ;
    rdfs:label "Mistral Server Infrastructure"@en ;
    rdfs:comment "Material infrastructure supporting Mistral systems for mathematics and logical reasoning"@en ;
    bfo:BFO_0000171 abi:EuropeanDataCenterRegion . # located in

abi:MathematicalProcessingHardware a bfo:BFO_0000040 ;
    rdfs:label "Mathematical Processing Hardware"@en ;
    rdfs:comment "Specialized hardware for mathematical computations and logical reasoning"@en ;
    bfo:BFO_0000171 abi:EuropeanDataCenterRegion . # located in

#################################################################
#    BFO Bucket 2: Qualities (WHAT-LIKE)
#################################################################

# Qualities that inhere in material entities (BFO_0000019)
abi:MistralMathematicalAccuracy a bfo:BFO_0000019 ;
    rdfs:label "Mistral Mathematical Accuracy"@en ;
    rdfs:comment "Quality representing accuracy of mathematical computations"@en ;
    bfo:BFO_0000197 abi:MistralServerInfrastructure . # inheres in

abi:MistralLogicalConsistency a bfo:BFO_0000019 ;
    rdfs:label "Mistral Logical Consistency"@en ;
    rdfs:comment "Quality representing consistency of logical reasoning"@en ;
    bfo:BFO_0000197 abi:MathematicalProcessingHardware . # inheres in

#################################################################
#    BFO Bucket 3: Realizable Entities (WHAT-CAN-BE)
#################################################################

# Capabilities that inhere in material entities (BFO_0000017)
abi:MistralMathematicalReasoningCapability a bfo:BFO_0000017 ;
    rdfs:label "Mistral Mathematical Reasoning Capability"@en ;
    rdfs:comment "Capability for mathematical computation and logical analysis"@en ;
    bfo:BFO_0000197 abi:MistralServerInfrastructure ; # inheres in
    bfo:BFO_0000218 abi:MathematicalProcessingHardware . # has material basis

#################################################################
#    BFO Bucket 7: Generically Dependent Continuants (INFORMATION)
#################################################################

# AI Models as software patterns (BFO_0000031) - CONCRETE INSTANCE
abi:MistralLarge2Model a bfo:BFO_0000031 ;
    rdfs:label "mistral-large-2"@en ;
    rdfs:comment "Mistral's flagship model with enhanced code generation, mathematics, and reasoning capabilities"@en ;
    skos:example "Mistral-Large-2 neural network, 128K token context, enhanced math capabilities"@en ;
    abi:hasModelId "mistral-large-2407"@en ;
    abi:hasContextWindow "128000"^^xsd:integer ;
    abi:hasOwner "mistral"@en ;
    abi:hasDescription "Mistral's flagship model with enhanced code generation, mathematics, and reasoning capabilities."@en ;
    bfo:BFO_0000084 abi:MistralServerInfrastructure . # generically depends on

# System Prompt as behavioral pattern (BFO_0000031) - CONCRETE INSTANCE
abi:MistralSystemPrompt a bfo:BFO_0000031 ;
    rdfs:label "Mistral System Prompt"@en ;
    rdfs:comment "System prompt defining Mistral's role in mathematical reasoning"@en ;
    skos:example "Role: mathematical reasoner, Tasks: math/logic problems, Guidelines: precise computation"@en ;
    abi:hasRole "You are Mistral, an AI assistant specialized in mathematical reasoning and logical problem-solving."@en ;
    abi:hasObjective "Solve complex mathematical and logical problems with precision"@en ;
    abi:hasTasks "Mathematical calculations; Logical reasoning; Code generation; Problem solving"@en ;
    bfo:BFO_0000084 abi:MistralServerInfrastructure . # generically depends on

# Intent Mappings as routing patterns (BFO_0000031) - CONCRETE INSTANCE
abi:MistralIntentMappings a bfo:BFO_0000031 ;
    rdfs:label "Mistral Intent Mappings"@en ;
    rdfs:comment "Intent-to-tool routing patterns for Mistral agent"@en ;
    skos:example "solve math → mistral_calculate, generate code → mistral_code"@en ;
    abi:hasIntentCount "12"^^xsd:integer ;
    bfo:BFO_0000084 abi:MistralServerInfrastructure . # generically depends on

# AI Agent as software pattern (BFO_0000031) - CONCRETE INSTANCE
abi:MistralAgent a abi:AIAgent ;
    rdfs:label "Mistral"@en ;
    rdfs:comment "Mistral AI agent specialized in mathematical reasoning and logical problem-solving"@en ;
    skos:example "Mistral service using Mistral-Large-2 for math and logic"@en ;
    abi:hasSpecializedRole "Mathematics and logical reasoning"@en ;
    abi:usesModel abi:MistralLarge2Model ;
    abi:usesSystemPrompt abi:MistralSystemPrompt ;
    abi:usesIntentMappings abi:MistralIntentMappings ;
    bfo:BFO_0000084 abi:MistralServerInfrastructure . # generically depends on

#################################################################
#    BFO Bucket 5: Immaterial Entities (WHERE)
#################################################################

# Spatial regions (BFO_0000141)
abi:EuropeanDataCenterRegion a bfo:BFO_0000141 ;
    rdfs:label "European Data Center Region"@en ;
    rdfs:comment "Spatial region encompassing European data center locations"@en .

#################################################################
#    BFO Bucket 6: Temporal Regions (WHEN)
#################################################################

# Temporal regions (BFO_0000008)
abi:MistralMathematicalReasoningSession a bfo:BFO_0000008 ;
    rdfs:label "Mistral Mathematical Reasoning Session"@en ;
    rdfs:comment "Temporal region during which mathematical reasoning occurs"@en .

#################################################################
#    BFO Bucket 4: Processes (HOW-IT-HAPPENS)
#################################################################

# Specialized processes that realize capabilities (BFO_0000015)
abi:MistralMathematicalReasoningProcess a abi:MathematicalReasoningProcess ;
    rdfs:label "Mistral Mathematical Reasoning Process"@en ;
    rdfs:comment "Process of mathematical computation and logical analysis using Mistral"@en ;
    bfo:BFO_0000057 abi:MistralServerInfrastructure ; # has participant
    bfo:BFO_0000055 abi:MistralMathematicalReasoningCapability ; # realizes
    bfo:BFO_0000108 abi:MistralMathematicalReasoningSession . # exists at

# Note: Additional processes can be defined as needed, following the same BFO pattern

