@prefix abi: <http://ontology.naas.ai/abi/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix capability: <http://ontology.naas.ai/abi/capability/> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
<http://ontology.naas.ai/abi/GeminiOntology> a owl:Ontology ;
    owl:imports <http://ontology.naas.ai/abi/AIAgentOntology> ;
    dc:title "Gemini AI Agent Ontology"@en ;
    dc:description "BFO-grounded ontology for Gemini AI models and processes"@en ;
    dc:created "20250812T101446"@en .


#################################################################
#    BFO Bucket 1: Material Entities (WHAT/WHO)
#################################################################

# Material Infrastructure (BFO_0000040)
abi:GoogleServerInfrastructure a bfo:BFO_0000040 ;
    rdfs:label "Gemini Server Infrastructure"@en ;
    rdfs:comment "Material infrastructure supporting Gemini systems"@en ;
    skos:example "Physical servers, GPU clusters, network hardware for Gemini"@en ;
    bfo:BFO_0000171 abi:GoogleDataCenterRegion . # located in

#################################################################
#    BFO Bucket 2: Qualities (WHAT-LIKE)
#################################################################

# Qualities that inhere in material entities (BFO_0000019)
abi:GeminiMultimodalProcessingQuality a bfo:BFO_0000019 ;
    rdfs:label "Gemini MultimodalProcessingQuality"@en ;
    rdfs:comment "Quality representing excellence in multimodal content processing"@en ;
    skos:example "Gemini processing quality metrics"@en ;
    bfo:BFO_0000197 abi:GoogleServerInfrastructure . # inheres in

#################################################################
#    BFO Bucket 3: Realizable Entities (WHAT-CAN-BE)
#################################################################

# Capabilities that inhere in material entities (BFO_0000017)
abi:GeminiLargeDocumentProcessingCapability a bfo:BFO_0000017 ;
    rdfs:label "Gemini LargeDocumentProcessingCapability"@en ;
    rdfs:comment "Capability for processing large documents and multimodal content"@en ;
    skos:example "Gemini core capability"@en ;
    bfo:BFO_0000197 abi:GoogleServerInfrastructure . # inheres in

#################################################################
#    BFO Bucket 7: Generically Dependent Continuants (INFORMATION)
#################################################################

# AI Models as software patterns (BFO_0000031) - CONCRETE INSTANCE
abi:Gemini20FlashModel a bfo:BFO_0000031 ;
    rdfs:label "gemini-2.0-flash"@en ;
    rdfs:comment "Google's most advanced, multimodal flagship model with enhanced reasoning capabilities, cheaper and faster than GPT-4 Turbo."@en ;
    skos:example "gemini-2.0-flash neural network"@en ;
    abi:hasModelId "gemini-2.0-flash"@en ;
    abi:hasContextWindow "1000000"^^xsd:integer ;
    abi:hasOwner "google"@en ;
    abi:hasDescription "Google's most advanced, multimodal flagship model with enhanced reasoning capabilities, cheaper and faster than GPT-4 Turbo."@en ;
    bfo:BFO_0000084 abi:GoogleServerInfrastructure . # generically depends on

# System Prompt as behavioral pattern (BFO_0000031) - CONCRETE INSTANCE
abi:GeminiSystemPrompt a bfo:BFO_0000031 ;
    rdfs:label "Gemini System Prompt"@en ;
    rdfs:comment "System prompt defining Gemini's role, objectives, and operating guidelines"@en ;
    skos:example "Gemini behavioral configuration"@en ;
    abi:hasRole "You are Gemini, Google's AI assistant specialized in processing large documents and multimodal content."@en ;
    abi:hasObjective "Analyze and process large documents with multimodal understanding"@en ;
    abi:hasTasks "Large document processing; Multimodal analysis; Image and text processing; Complex content synthesis"@en ;
    bfo:BFO_0000084 abi:GoogleServerInfrastructure . # generically depends on

# Intent Mappings as routing patterns (BFO_0000031) - CONCRETE INSTANCE
abi:GeminiIntentMappings a bfo:BFO_0000031 ;
    rdfs:label "Gemini Intent Mappings"@en ;
    rdfs:comment "Intent-to-tool routing patterns for Gemini agent"@en ;
    skos:example "Gemini intent routing configuration"@en ;
    abi:hasIntentCount "20"^^xsd:integer ;
    bfo:BFO_0000084 abi:GoogleServerInfrastructure . # generically depends on

# AI Agent as software pattern (BFO_0000031) - CONCRETE INSTANCE
abi:GeminiAgent a abi:AIAgent ;
    rdfs:label "Gemini"@en ;
    rdfs:comment "Google's Gemini AI agent specialized in large document processing and multimodal analysis"@en ;
    skos:example "Complete Gemini service: model + system prompt + intent mappings"@en ;
    abi:hasSpecializedRole "Large document and multimodal content processing"@en ;
    abi:usesModel abi:Gemini20FlashModel ;
    abi:usesSystemPrompt abi:GeminiSystemPrompt ;
    abi:usesIntentMappings abi:GeminiIntentMappings ;
    bfo:BFO_0000084 abi:GoogleServerInfrastructure . # generically depends on

#################################################################
#    BFO Bucket 5: Immaterial Entities (WHERE)
#################################################################

# Spatial regions (BFO_0000141)
abi:GoogleDataCenterRegion a bfo:BFO_0000141 ;
    rdfs:label "Gemini Data Center Region"@en ;
    rdfs:comment "Spatial region encompassing Gemini data center locations"@en ;
    skos:example "Geographic region where Gemini infrastructure is located"@en .

#################################################################
#    BFO Bucket 6: Temporal Regions (WHEN)
#################################################################

# Temporal regions (BFO_0000008)
abi:GeminiDocumentProcessingSession a bfo:BFO_0000008 ;
    rdfs:label "Gemini DocumentProcessingSession"@en ;
    rdfs:comment "Temporal region during which Gemini processing occurs"@en ;
    skos:example "Gemini processing time interval"@en .

#################################################################
#    BFO Bucket 4: Processes (HOW-IT-HAPPENS)
#################################################################

# Specialized processes that realize capabilities (BFO_0000015)
abi:GeminiLargeDocumentProcessingProcess a abi:LargeDocumentProcessingProcess ;
    rdfs:label "Gemini LargeDocumentProcessingProcess"@en ;
    rdfs:comment "Process of analyzing and processing large documents and multimodal content"@en ;
    skos:example "Gemini executing its core process"@en ;
    bfo:BFO_0000057 abi:GoogleServerInfrastructure ; # has participant
    bfo:BFO_0000055 abi:GeminiLargeDocumentProcessingCapability ; # realizes
    bfo:BFO_0000108 abi:GeminiDocumentProcessingSession . # exists at
