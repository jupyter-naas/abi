@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dc11: <http://purl.org/dc/elements/1.1/> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix cco: <https://www.commoncoreontologies.org/> .
@prefix abi: <http://ontology.naas.ai/abi/> .
@prefix llama: <http://ontology.naas.ai/abi/llama/> .

<http://ontology.naas.ai/abi/llama/LlamaInstances> rdf:type owl:Ontology ;
    owl:imports <http://ontology.naas.ai/abi/AIAgentOntology> ;
    dc:title "Llama Module Instances" ;
    dc:description "Concrete instances demonstrating Llama AI agents with open-source capabilities and cost-effectiveness."@en .

#################################################################
#    Llama AI System (Main System)
#################################################################

llama:LlamaSystem a abi:AISystem ;
    rdfs:label "Llama AI System"@en ;
    abi:providerName "Meta" ;
    abi:systemComplexity "complex" ;
    abi:hasLoadBalancer llama:LlamaLoadBalancer ;
    abi:hasSubsystem llama:OpenSourceSubsystem, llama:CostOptimizationSubsystem, llama:MassiveContextSubsystem ;
    rdfs:comment "Main Llama system that excels in open-source capabilities, cost optimization, and massive context processing with local deployment options."@en .

#################################################################
#    Llama Subsystems
#################################################################

llama:OpenSourceSubsystem a abi:AISystem ;
    rdfs:label "Llama Open Source Subsystem"@en ;
    abi:providerName "Meta" ;
    abi:systemComplexity "moderate" ;
    abi:isSubsystemOf llama:LlamaSystem ;
    abi:hasAIAgent llama:LlamaOpenSourceAgent, llama:LlamaLocalAgent ;
    rdfs:comment "Subsystem dedicated to open-source capabilities and local deployment."@en .

llama:CostOptimizationSubsystem a abi:AISystem ;
    rdfs:label "Llama Cost Optimization Subsystem"@en ;
    abi:providerName "Meta" ;
    abi:systemComplexity "moderate" ;
    abi:isSubsystemOf llama:LlamaSystem ;
    abi:hasAIAgent llama:LlamaCostAgent, llama:LlamaEfficiencyAgent ;
    rdfs:comment "Subsystem optimized for cost-effectiveness and resource efficiency."@en .

llama:MassiveContextSubsystem a abi:AISystem ;
    rdfs:label "Llama Massive Context Subsystem"@en ;
    abi:providerName "Meta" ;
    abi:systemComplexity "moderate" ;
    abi:isSubsystemOf llama:LlamaSystem ;
    abi:hasAIAgent llama:LlamaContextAgent, llama:LlamaMemoryAgent ;
    rdfs:comment "Subsystem specialized in massive context processing and memory management."@en .

#################################################################
#    Llama AI Agents
#################################################################

### Open Source Agents
llama:LlamaOpenSourceAgent a abi:AIAgent ;
    rdfs:label "Llama Open Source Agent"@en ;
    abi:isAIAgentOf llama:OpenSourceSubsystem ;
    abi:hasSpecializedRole abi:PrimaryResponderRole ;
    abi:collaborationPriority "9" ;
    abi:collaboratesWith llama:LlamaCostAgent, llama:LlamaContextAgent ;
    abi:hasFallbackAgent llama:LlamaLocalAgent ;
    rdfs:comment "Primary open-source agent that excels in local deployment and community-driven development."@en .

llama:LlamaLocalAgent a abi:AIAgent ;
    rdfs:label "Llama Local Agent"@en ;
    abi:isAIAgentOf llama:OpenSourceSubsystem ;
    abi:hasSpecializedRole abi:SpecializedTaskExecutorRole ;
    abi:collaborationPriority "7" ;
    abi:collaboratesWith llama:LlamaOpenSourceAgent, llama:LlamaEfficiencyAgent ;
    rdfs:comment "Local deployment agent that runs on-premises with full data privacy."@en .

### Cost Optimization Agents
llama:LlamaCostAgent a abi:AIAgent ;
    rdfs:label "Llama Cost Agent"@en ;
    abi:isAIAgentOf llama:CostOptimizationSubsystem ;
    abi:hasSpecializedRole abi:SpecializedTaskExecutorRole ;
    abi:collaborationPriority "8" ;
    abi:collaboratesWith llama:LlamaOpenSourceAgent, llama:LlamaEfficiencyAgent ;
    rdfs:comment "Cost optimization agent that minimizes resource usage and maximizes efficiency."@en .

llama:LlamaEfficiencyAgent a abi:AIAgent ;
    rdfs:label "Llama Efficiency Agent"@en ;
    abi:isAIAgentOf llama:CostOptimizationSubsystem ;
    abi:hasSpecializedRole abi:SpecializedTaskExecutorRole ;
    abi:collaborationPriority "7" ;
    abi:collaboratesWith llama:LlamaCostAgent, llama:LlamaLocalAgent ;
    rdfs:comment "Efficiency optimization agent that maximizes performance per resource unit."@en .

### Massive Context Agents
llama:LlamaContextAgent a abi:AIAgent ;
    rdfs:label "Llama Context Agent"@en ;
    abi:isAIAgentOf llama:MassiveContextSubsystem ;
    abi:hasSpecializedRole abi:SpecializedTaskExecutorRole ;
    abi:collaborationPriority "8" ;
    abi:collaboratesWith llama:LlamaOpenSourceAgent, llama:LlamaMemoryAgent ;
    rdfs:comment "Massive context processing agent that handles large document analysis."@en .

llama:LlamaMemoryAgent a abi:AIAgent ;
    rdfs:label "Llama Memory Agent"@en ;
    abi:isAIAgentOf llama:MassiveContextSubsystem ;
    abi:hasSpecializedRole abi:SpecializedTaskExecutorRole ;
    abi:collaborationPriority "7" ;
    abi:collaboratesWith llama:LlamaContextAgent, llama:LlamaEfficiencyAgent ;
    rdfs:comment "Memory management agent that optimizes context window utilization."@en .

#################################################################
#    Load Balancer
#################################################################

llama:LlamaLoadBalancer a abi:LoadBalancer ;
    rdfs:label "Llama Load Balancer"@en ;
    rdfs:comment "Intelligent load balancer that optimizes for cost-effectiveness and open-source capabilities across Llama agents."@en .

#################################################################
#    AI Model Instances
#################################################################

llama:LlamaOpenSourceModelInstance a abi:AIModelInstance ;
    rdfs:label "Llama Open Source Model Instance"@en ;
    abi:modelName "llama-3.1-8b" ;
    abi:providerName "Meta" ;
    abi:hasQuality llama:LlamaOpenSourceAccuracy, llama:LlamaOpenSourceLatency, llama:LlamaOpenSourceTokenCapacity ;
    rdfs:comment "Specific instance of Llama 3.1 8B model optimized for open-source deployment."@en .

llama:LlamaMassiveContextModelInstance a abi:AIModelInstance ;
    rdfs:label "Llama Massive Context Model Instance"@en ;
    abi:modelName "llama-3.1-70b" ;
    abi:providerName "Meta" ;
    abi:hasQuality llama:LlamaMassiveContextAccuracy, llama:LlamaMassiveContextLatency, llama:LlamaMassiveContextTokenCapacity ;
    rdfs:comment "Specific instance of Llama 3.1 70B model for massive context processing."@en .

#################################################################
#    Qualities
#################################################################

### Llama Open Source Qualities
llama:LlamaOpenSourceAccuracy a abi:ModelAccuracy ;
    rdfs:label "Llama Open Source Accuracy"@en ;
    abi:accuracyValue "0.43"^^xsd:decimal ;
    rdfs:comment "Llama open source model accuracy quality with 43% performance."@en .

llama:LlamaOpenSourceLatency a abi:ResponseLatency ;
    rdfs:label "Llama Open Source Latency"@en ;
    abi:latencyValue "1753"^^xsd:integer ;
    rdfs:comment "Llama open source response latency quality of 1753ms."@en .

llama:LlamaOpenSourceTokenCapacity a abi:TokenCapacity ;
    rdfs:label "Llama Open Source Token Capacity"@en ;
    abi:tokenLimit "8192"^^xsd:integer ;
    rdfs:comment "Llama open source token capacity quality of 8,192 tokens."@en .

### Llama Massive Context Qualities
llama:LlamaMassiveContextAccuracy a abi:ModelAccuracy ;
    rdfs:label "Llama Massive Context Accuracy"@en ;
    abi:accuracyValue "0.43"^^xsd:decimal ;
    rdfs:comment "Llama massive context model accuracy quality with 43% performance."@en .

llama:LlamaMassiveContextLatency a abi:ResponseLatency ;
    rdfs:label "Llama Massive Context Latency"@en ;
    abi:latencyValue "1753"^^xsd:integer ;
    rdfs:comment "Llama massive context response latency quality of 1753ms."@en .

llama:LlamaMassiveContextTokenCapacity a abi:TokenCapacity ;
    rdfs:label "Llama Massive Context Token Capacity"@en ;
    abi:tokenLimit "32768"^^xsd:integer ;
    rdfs:comment "Llama massive context token capacity quality of 32,768 tokens."@en .

#################################################################
#    Processes with Cost Optimization Workflow
#################################################################

llama:LocalDeploymentProcess a abi:TextGenerationProcess ;
    rdfs:label "Local Deployment Process"@en ;
    abi:realizesCapability abi:TextGenerationCapability ;
    abi:hasParticipant llama:LlamaOpenSourceModelInstance ;
    abi:hasTemporalRegion llama:LocalDeploymentSession ;
    abi:temporalSequence "1" ;
    abi:triggersProcess llama:CostOptimizationProcess ;
    rdfs:comment "Process that handles local deployment and on-premises processing."@en .

llama:CostOptimizationProcess a abi:TextGenerationProcess ;
    rdfs:label "Cost Optimization Process"@en ;
    abi:realizesCapability abi:TextGenerationCapability ;
    abi:hasParticipant llama:LlamaOpenSourceModelInstance ;
    abi:hasTemporalRegion llama:CostOptimizationSession ;
    abi:temporalSequence "2" ;
    abi:precedesInTime llama:ContextProcessingProcess ;
    abi:triggersProcess llama:ContextProcessingProcess ;
    rdfs:comment "Process that optimizes costs and resource usage."@en .

llama:ContextProcessingProcess a abi:TextGenerationProcess ;
    rdfs:label "Context Processing Process"@en ;
    abi:realizesCapability abi:TextGenerationCapability ;
    abi:hasParticipant llama:LlamaMassiveContextModelInstance ;
    abi:hasTemporalRegion llama:ContextProcessingSession ;
    abi:temporalSequence "3" ;
    abi:precedesInTime llama:EfficiencyValidationProcess ;
    abi:triggersProcess llama:EfficiencyValidationProcess ;
    rdfs:comment "Process that handles massive context processing and analysis."@en .

llama:EfficiencyValidationProcess a abi:TextGenerationProcess ;
    rdfs:label "Efficiency Validation Process"@en ;
    abi:realizesCapability abi:TextGenerationCapability ;
    abi:hasParticipant llama:LlamaOpenSourceModelInstance ;
    abi:hasTemporalRegion llama:EfficiencyValidationSession ;
    abi:temporalSequence "4" ;
    abi:precedesInTime llama:ResponseDeliveryProcess ;
    abi:triggersProcess llama:ResponseDeliveryProcess ;
    rdfs:comment "Process that validates efficiency and cost-effectiveness."@en .

llama:ResponseDeliveryProcess a abi:TextGenerationProcess ;
    rdfs:label "Response Delivery Process"@en ;
    abi:realizesCapability abi:TextGenerationCapability ;
    abi:hasParticipant llama:LlamaOpenSourceModelInstance ;
    abi:hasTemporalRegion llama:ResponseDeliverySession ;
    abi:temporalSequence "5" ;
    rdfs:comment "Process that delivers cost-optimized responses."@en .

#################################################################
#    Temporal Regions
#################################################################

llama:LocalDeploymentSession a abi:InferenceSession ;
    rdfs:label "Local Deployment Session"@en ;
    rdfs:comment "Temporal region during which local deployment occurs."@en .

llama:CostOptimizationSession a abi:InferenceSession ;
    rdfs:label "Cost Optimization Session"@en ;
    rdfs:comment "Temporal region during which cost optimization occurs."@en .

llama:ContextProcessingSession a abi:InferenceSession ;
    rdfs:label "Context Processing Session"@en ;
    rdfs:comment "Temporal region during which context processing occurs."@en .

llama:EfficiencyValidationSession a abi:InferenceSession ;
    rdfs:label "Efficiency Validation Session"@en ;
    rdfs:comment "Temporal region during which efficiency validation occurs."@en .

llama:ResponseDeliverySession a abi:InferenceSession ;
    rdfs:label "Response Delivery Session"@en ;
    rdfs:comment "Temporal region during which response delivery occurs."@en .

#################################################################
#    Information Content Entities
#################################################################

llama:LlamaOpenSourceSpecification a abi:ModelSpecification ;
    rdfs:label "Llama Open Source Model Specification"@en ;
    abi:specificationContent "Llama 3.1 8B is an open-source model optimized for local deployment, cost-effectiveness, and community-driven development with 8K context window." ;
    rdfs:comment "Information content describing Llama open source model parameters and capabilities."@en .

llama:LlamaMassiveContextSpecification a abi:ModelSpecification ;
    rdfs:label "Llama Massive Context Model Specification"@en ;
    abi:specificationContent "Llama 3.1 70B is a massive context model with 32K context window, optimized for large document analysis and processing." ;
    rdfs:comment "Information content describing Llama massive context model parameters and capabilities."@en .

llama:OpenSourceCapabilitiesSpecification a abi:ModelSpecification ;
    rdfs:label "Open Source Capabilities Specification"@en ;
    abi:specificationContent "Open-source capabilities include local deployment, community-driven development, cost optimization, massive context processing, and full data privacy." ;
    rdfs:comment "Information content describing Llama's open-source capabilities and features."@en .
