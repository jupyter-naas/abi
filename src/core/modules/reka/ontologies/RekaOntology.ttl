@prefix abi: <http://naas.ai/ontology/abi#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix capability: <http://ontology.naas.ai/abi/capability/> .
@prefix dc: <http://purl.org/dc/terms/> .

<http://naas.ai/ontology/abi/RekaOntology> a owl:Ontology ;
    owl:imports <http://ontology.naas.ai/abi/AIAgentOntology> ;
    dc:title "Reka AI Agent Ontology"@en ;
    dc:description "BFO-grounded ontology for Reka AI models and processes"@en ;
    dc:created "20250812T101446"@en .


#################################################################
#    BFO Bucket 1: Material Entities (WHAT/WHO)
#################################################################

abi:RekaAgent a abi:AIAgent ;
    rdfs:label "Reka AI Agent"@en ;
    rdfs:comment "AI Agent capable of utilizing Reka models"@en ;
    abi:hasSpecializedRole "Multi-purpose AI processing"@en .


#################################################################
#    BFO Bucket 4: Processes (HOW-IT-HAPPENS)
#################################################################

abi:RekaBusinessProposalProcess a abi:BusinessProposalCreationProcess ;
    rdfs:label "Reka Business Proposal Process"@en ;
    abi:hasParticipant abi:RekaAgent ;
    abi:realizesCapability capability:TextGenerationCapability ;
    abi:hasTemporalRegion abi:RekaBusinessProposalSession ;
    abi:hasQuality abi:RekaBusinessProposalQuality .

abi:RekaCreativeWritingProcess a abi:CreativeWritingProcess ;
    rdfs:label "Reka Creative Writing Process"@en ;
    abi:hasParticipant abi:RekaAgent ;
    abi:realizesCapability capability:TextGenerationCapability ;
    abi:hasTemporalRegion abi:RekaCreativeWritingSession ;
    abi:hasQuality abi:RekaCreativeWritingQuality .

abi:RekaCodeGenerationProcess a abi:CodeGenerationProcess ;
    rdfs:label "Reka Code Generation Process"@en ;
    abi:hasParticipant abi:RekaAgent ;
    abi:realizesCapability capability:CodeGenerationCapability ;
    abi:hasTemporalRegion abi:RekaCodeGenerationSession ;
    abi:hasQuality abi:RekaCodeGenerationQuality .

#################################################################
#    BFO Bucket 5: Temporal Regions (WHEN)
#################################################################

abi:RekaBusinessProposalSession a abi:InferenceSession ;
    rdfs:label "Reka Business Proposal Session"@en .

abi:RekaCreativeWritingSession a abi:InferenceSession ;
    rdfs:label "Reka Creative Writing Session"@en .

abi:RekaCodeGenerationSession a abi:InferenceSession ;
    rdfs:label "Reka Code Generation Session"@en .


#################################################################
#    BFO Bucket 1: Material Entity - Reka Flash 3
#################################################################

abi:Reka_Flash_3 a abi:AIModelInstance ;
    rdfs:label "Reka Flash 3"@en ;
    abi:modelSlug "reka-flash-3"@en ;
    abi:provider "Reka AI"@en ;
    abi:sourceAPI "artificial_analysis"@en .

#################################################################
#    BFO Bucket 2: Qualities - Performance & Cost Metrics
#################################################################

abi:Reka_Flash_3 abi:inputTokenCost 0.2 ;
    abi:inputTokenCostCurrency "USD"@en ;
    abi:outputTokenCost 0.8 ;
    abi:outputTokenCostCurrency "USD"@en ;
    abi:blendedCost 0.35 ;
    abi:blendedCostCurrency "USD"@en ;
    abi:outputSpeed 55.778 ;
    abi:outputSpeedUnit "tokens_per_second"@en ;
    abi:timeToFirstToken 1.269 ;
    abi:timeToFirstTokenUnit "seconds"@en ;
    abi:timeToFirstAnswerToken 37.125 ;
    abi:timeToFirstAnswerTokenUnit "seconds"@en ;
    abi:intelligenceIndex 36.3 ;
    abi:codingIndex 35.1 ;
    abi:mathIndex 70.1 .

#################################################################
#    Relationships - Agent/Process/Model Network
#################################################################

abi:RekaAgent abi:canUtilizeModel abi:Reka_Flash_3 .
abi:RekaBusinessProposalProcess abi:utilizesModel abi:Reka_Flash_3 .
abi:RekaCreativeWritingProcess abi:utilizesModel abi:Reka_Flash_3 .
abi:RekaCodeGenerationProcess abi:utilizesModel abi:Reka_Flash_3 .

