@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dc11: <http://purl.org/dc/elements/1.1/> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix cco: <https://www.commoncoreontologies.org/> .
@prefix abi: <http://ontology.naas.ai/abi/> .
@prefix chatgpt: <http://ontology.naas.ai/abi/chatgpt/> .

<http://ontology.naas.ai/abi/chatgpt/ChatGPTInstances> rdf:type owl:Ontology ;
    owl:imports <http://ontology.naas.ai/abi/AIAgentOntology> ;
    dc:title "ChatGPT Module Instances" ;
    dc:description "Concrete instances demonstrating ChatGPT AI agents with advanced relationships."@en .

#################################################################
#    ChatGPT AI System (Main System)
#################################################################

chatgpt:ChatGPTSystem a abi:AISystem ;
    rdfs:label "ChatGPT AI System"@en ;
    abi:providerName "OpenAI" ;
    abi:systemComplexity "complex" ;
    abi:hasLoadBalancer chatgpt:ChatGPTLoadBalancer ;
    abi:hasSubsystem chatgpt:TextGenerationSubsystem, chatgpt:ImageGenerationSubsystem, chatgpt:ValidationSubsystem ;
    rdfs:comment "Main ChatGPT system that orchestrates multiple specialized subsystems for comprehensive AI capabilities."@en .

#################################################################
#    ChatGPT Subsystems
#################################################################

chatgpt:TextGenerationSubsystem a abi:AISystem ;
    rdfs:label "ChatGPT Text Generation Subsystem"@en ;
    abi:providerName "OpenAI" ;
    abi:systemComplexity "moderate" ;
    abi:isSubsystemOf chatgpt:ChatGPTSystem ;
    abi:hasAIAgent chatgpt:GPT4TextAgent, chatgpt:GPT35TextAgent ;
    rdfs:comment "Subsystem dedicated to text generation tasks with multiple model variants for different use cases."@en .

chatgpt:ImageGenerationSubsystem a abi:AISystem ;
    rdfs:label "ChatGPT Image Generation Subsystem"@en ;
    abi:providerName "OpenAI" ;
    abi:systemComplexity "moderate" ;
    abi:isSubsystemOf chatgpt:ChatGPTSystem ;
    abi:hasAIAgent chatgpt:DALLE3Agent, chatgpt:DALLE2Agent ;
    rdfs:comment "Subsystem dedicated to image generation and manipulation tasks."@en .

chatgpt:ValidationSubsystem a abi:AISystem ;
    rdfs:label "ChatGPT Validation Subsystem"@en ;
    abi:providerName "OpenAI" ;
    abi:systemComplexity "simple" ;
    abi:isSubsystemOf chatgpt:ChatGPTSystem ;
    abi:hasAIAgent chatgpt:ContentValidatorAgent ;
    rdfs:comment "Subsystem responsible for content validation and safety checks."@en .

#################################################################
#    ChatGPT AI Agents
#################################################################

### Text Generation Agents
chatgpt:GPT4TextAgent a abi:AIAgent ;
    rdfs:label "GPT-4 Text Agent"@en ;
    abi:isAIAgentOf chatgpt:TextGenerationSubsystem ;
    abi:hasSpecializedRole abi:PrimaryResponderRole ;
    abi:collaborationPriority "9" ;
    abi:collaboratesWith chatgpt:DALLE3Agent, chatgpt:ContentValidatorAgent ;
    abi:hasFallbackAgent chatgpt:GPT35TextAgent ;
    rdfs:comment "Primary text generation agent using GPT-4 model for high-quality responses."@en .

chatgpt:GPT35TextAgent a abi:AIAgent ;
    rdfs:label "GPT-3.5 Text Agent"@en ;
    abi:isAIAgentOf chatgpt:TextGenerationSubsystem ;
    abi:hasSpecializedRole abi:FallbackHandlerRole ;
    abi:collaborationPriority "6" ;
    abi:collaboratesWith chatgpt:GPT4TextAgent ;
    rdfs:comment "Fallback text generation agent using GPT-3.5 model for cost-effective responses."@en .

### Image Generation Agents
chatgpt:DALLE3Agent a abi:AIAgent ;
    rdfs:label "DALL-E 3 Agent"@en ;
    abi:isAIAgentOf chatgpt:ImageGenerationSubsystem ;
    abi:hasSpecializedRole abi:SpecializedTaskExecutorRole ;
    abi:collaborationPriority "8" ;
    abi:collaboratesWith chatgpt:GPT4TextAgent, chatgpt:ContentValidatorAgent ;
    abi:hasFallbackAgent chatgpt:DALLE2Agent ;
    rdfs:comment "Advanced image generation agent using DALL-E 3 for high-quality visual content."@en .

chatgpt:DALLE2Agent a abi:AIAgent ;
    rdfs:label "DALL-E 2 Agent"@en ;
    abi:isAIAgentOf chatgpt:ImageGenerationSubsystem ;
    abi:hasSpecializedRole abi:FallbackHandlerRole ;
    abi:collaborationPriority "5" ;
    abi:collaboratesWith chatgpt:DALLE3Agent ;
    rdfs:comment "Fallback image generation agent using DALL-E 2 for reliable visual content."@en .

### Validation Agent
chatgpt:ContentValidatorAgent a abi:AIAgent ;
    rdfs:label "Content Validator Agent"@en ;
    abi:isAIAgentOf chatgpt:ValidationSubsystem ;
    abi:hasSpecializedRole abi:ValidatorRole ;
    abi:collaborationPriority "7" ;
    abi:collaboratesWith chatgpt:GPT4TextAgent, chatgpt:DALLE3Agent ;
    rdfs:comment "Content validation agent that ensures safety and quality of generated content."@en .

#################################################################
#    Load Balancer
#################################################################

chatgpt:ChatGPTLoadBalancer a abi:LoadBalancer ;
    rdfs:label "ChatGPT Load Balancer"@en ;
    rdfs:comment "Intelligent load balancer that distributes requests across ChatGPT agents based on capability and availability."@en .

#################################################################
#    AI Model Instances
#################################################################

chatgpt:GPT4ModelInstance a abi:AIModelInstance ;
    rdfs:label "GPT-4 Model Instance"@en ;
    abi:modelName "gpt-4" ;
    abi:providerName "OpenAI" ;
    abi:hasQuality chatgpt:GPT4Accuracy, chatgpt:GPT4Latency, chatgpt:GPT4TokenCapacity ;
    rdfs:comment "Specific instance of GPT-4 model running on OpenAI's infrastructure."@en .

chatgpt:GPT35ModelInstance a abi:AIModelInstance ;
    rdfs:label "GPT-3.5 Model Instance"@en ;
    abi:modelName "gpt-3.5-turbo" ;
    abi:providerName "OpenAI" ;
    abi:hasQuality chatgpt:GPT35Accuracy, chatgpt:GPT35Latency, chatgpt:GPT35TokenCapacity ;
    rdfs:comment "Specific instance of GPT-3.5 Turbo model running on OpenAI's infrastructure."@en .

chatgpt:DALLE3ModelInstance a abi:AIModelInstance ;
    rdfs:label "DALL-E 3 Model Instance"@en ;
    abi:modelName "dall-e-3" ;
    abi:providerName "OpenAI" ;
    abi:hasQuality chatgpt:DALLE3Accuracy, chatgpt:DALLE3Latency, chatgpt:DALLE3TokenCapacity ;
    rdfs:comment "Specific instance of DALL-E 3 model running on OpenAI's infrastructure."@en .

#################################################################
#    Qualities
#################################################################

### GPT-4 Qualities
chatgpt:GPT4Accuracy a abi:ModelAccuracy ;
    rdfs:label "GPT-4 Accuracy"@en ;
    abi:accuracyValue "0.95"^^xsd:decimal ;
    rdfs:comment "GPT-4 model accuracy quality with 95% performance."@en .

chatgpt:GPT4Latency a abi:ResponseLatency ;
    rdfs:label "GPT-4 Latency"@en ;
    abi:latencyValue "2000"^^xsd:integer ;
    rdfs:comment "GPT-4 response latency quality of 2000ms."@en .

chatgpt:GPT4TokenCapacity a abi:TokenCapacity ;
    rdfs:label "GPT-4 Token Capacity"@en ;
    abi:tokenLimit "8192"^^xsd:integer ;
    rdfs:comment "GPT-4 token capacity quality of 8192 tokens."@en .

### GPT-3.5 Qualities
chatgpt:GPT35Accuracy a abi:ModelAccuracy ;
    rdfs:label "GPT-3.5 Accuracy"@en ;
    abi:accuracyValue "0.88"^^xsd:decimal ;
    rdfs:comment "GPT-3.5 model accuracy quality with 88% performance."@en .

chatgpt:GPT35Latency a abi:ResponseLatency ;
    rdfs:label "GPT-3.5 Latency"@en ;
    abi:latencyValue "800"^^xsd:integer ;
    rdfs:comment "GPT-3.5 response latency quality of 800ms."@en .

chatgpt:GPT35TokenCapacity a abi:TokenCapacity ;
    rdfs:label "GPT-3.5 Token Capacity"@en ;
    abi:tokenLimit "4096"^^xsd:integer ;
    rdfs:comment "GPT-3.5 token capacity quality of 4096 tokens."@en .

### DALL-E 3 Qualities
chatgpt:DALLE3Accuracy a abi:ModelAccuracy ;
    rdfs:label "DALL-E 3 Accuracy"@en ;
    abi:accuracyValue "0.92"^^xsd:decimal ;
    rdfs:comment "DALL-E 3 model accuracy quality with 92% performance."@en .

chatgpt:DALLE3Latency a abi:ResponseLatency ;
    rdfs:label "DALL-E 3 Latency"@en ;
    abi:latencyValue "5000"^^xsd:integer ;
    rdfs:comment "DALL-E 3 response latency quality of 5000ms."@en .

chatgpt:DALLE3TokenCapacity a abi:TokenCapacity ;
    rdfs:label "DALL-E 3 Token Capacity"@en ;
    abi:tokenLimit "4000"^^xsd:integer ;
    rdfs:comment "DALL-E 3 token capacity quality of 4000 tokens."@en .

#################################################################
#    Processes with Temporal Coordination
#################################################################

chatgpt:TextPreprocessingProcess a abi:TextGenerationProcess ;
    rdfs:label "Text Preprocessing Process"@en ;
    abi:realizesCapability abi:TextGenerationCapability ;
    abi:hasParticipant chatgpt:GPT4ModelInstance ;
    abi:hasTemporalRegion chatgpt:PreprocessingSession ;
    abi:temporalSequence "1" ;
    abi:triggersProcess chatgpt:TextGenerationProcess ;
    rdfs:comment "Process that preprocesses user input before text generation."@en .

chatgpt:TextGenerationProcess a abi:TextGenerationProcess ;
    rdfs:label "Text Generation Process"@en ;
    abi:realizesCapability abi:TextGenerationCapability ;
    abi:hasParticipant chatgpt:GPT4ModelInstance ;
    abi:hasTemporalRegion chatgpt:GenerationSession ;
    abi:temporalSequence "2" ;
    abi:precedesInTime chatgpt:ContentValidationProcess ;
    abi:triggersProcess chatgpt:ContentValidationProcess ;
    rdfs:comment "Process that generates text response using GPT-4 model."@en .

chatgpt:ContentValidationProcess a abi:TextGenerationProcess ;
    rdfs:label "Content Validation Process"@en ;
    abi:realizesCapability abi:TextGenerationCapability ;
    abi:hasParticipant chatgpt:GPT4ModelInstance ;
    abi:hasTemporalRegion chatgpt:ValidationSession ;
    abi:temporalSequence "3" ;
    abi:precedesInTime chatgpt:ResponseDeliveryProcess ;
    abi:triggersProcess chatgpt:ResponseDeliveryProcess ;
    rdfs:comment "Process that validates generated content for safety and quality."@en .

chatgpt:ResponseDeliveryProcess a abi:TextGenerationProcess ;
    rdfs:label "Response Delivery Process"@en ;
    abi:realizesCapability abi:TextGenerationCapability ;
    abi:hasParticipant chatgpt:GPT4ModelInstance ;
    abi:hasTemporalRegion chatgpt:DeliverySession ;
    abi:temporalSequence "4" ;
    rdfs:comment "Process that delivers the final validated response to the user."@en .

#################################################################
#    Temporal Regions
#################################################################

chatgpt:PreprocessingSession a abi:InferenceSession ;
    rdfs:label "Preprocessing Session"@en ;
    rdfs:comment "Temporal region during which text preprocessing occurs."@en .

chatgpt:GenerationSession a abi:InferenceSession ;
    rdfs:label "Generation Session"@en ;
    rdfs:comment "Temporal region during which text generation occurs."@en .

chatgpt:ValidationSession a abi:InferenceSession ;
    rdfs:label "Validation Session"@en ;
    rdfs:comment "Temporal region during which content validation occurs."@en .

chatgpt:DeliverySession a abi:InferenceSession ;
    rdfs:label "Delivery Session"@en ;
    rdfs:comment "Temporal region during which response delivery occurs."@en .

#################################################################
#    Information Content Entities
#################################################################

chatgpt:GPT4Specification a abi:ModelSpecification ;
    rdfs:label "GPT-4 Model Specification"@en ;
    abi:specificationContent "GPT-4 is a large multimodal model that can solve complex problems with greater accuracy than its predecessors. It can handle both text and image inputs." ;
    rdfs:comment "Information content describing GPT-4 model parameters and capabilities."@en .

chatgpt:DALLE3Specification a abi:ModelSpecification ;
    rdfs:label "DALL-E 3 Model Specification"@en ;
    abi:specificationContent "DALL-E 3 is an advanced image generation model that creates highly detailed and creative images from text descriptions." ;
    rdfs:comment "Information content describing DALL-E 3 model parameters and capabilities."@en .
