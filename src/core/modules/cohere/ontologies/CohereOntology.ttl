@prefix abi: <http://naas.ai/ontology/abi#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix capability: <http://ontology.naas.ai/abi/capability/> .
@prefix dc: <http://purl.org/dc/terms/> .

<http://naas.ai/ontology/abi/CohereOntology> a owl:Ontology ;
    owl:imports <http://ontology.naas.ai/abi/AIAgentOntology> ;
    dc:title "Cohere AI Agent Ontology"@en ;
    dc:description "BFO-grounded ontology for Cohere AI models and processes"@en ;
    dc:created "20250812T101446"@en .


#################################################################
#    BFO Bucket 1: Material Entities (WHAT/WHO)
#################################################################

abi:CohereAgent a abi:AIAgent ;
    rdfs:label "Cohere AI Agent"@en ;
    rdfs:comment "AI Agent capable of utilizing Cohere models"@en ;
    abi:hasSpecializedRole "Multi-purpose AI processing"@en .


#################################################################
#    BFO Bucket 4: Processes (HOW-IT-HAPPENS)
#################################################################

abi:CohereBusinessProposalProcess a abi:BusinessProposalCreationProcess ;
    rdfs:label "Cohere Business Proposal Process"@en ;
    abi:hasParticipant abi:CohereAgent ;
    abi:realizesCapability capability:TextGenerationCapability ;
    abi:hasTemporalRegion abi:CohereBusinessProposalSession ;
    abi:hasQuality abi:CohereBusinessProposalQuality .

abi:CohereCreativeWritingProcess a abi:CreativeWritingProcess ;
    rdfs:label "Cohere Creative Writing Process"@en ;
    abi:hasParticipant abi:CohereAgent ;
    abi:realizesCapability capability:TextGenerationCapability ;
    abi:hasTemporalRegion abi:CohereCreativeWritingSession ;
    abi:hasQuality abi:CohereCreativeWritingQuality .

abi:CohereCodeGenerationProcess a abi:CodeGenerationProcess ;
    rdfs:label "Cohere Code Generation Process"@en ;
    abi:hasParticipant abi:CohereAgent ;
    abi:realizesCapability capability:CodeGenerationCapability ;
    abi:hasTemporalRegion abi:CohereCodeGenerationSession ;
    abi:hasQuality abi:CohereCodeGenerationQuality .

#################################################################
#    BFO Bucket 5: Temporal Regions (WHEN)
#################################################################

abi:CohereBusinessProposalSession a abi:InferenceSession ;
    rdfs:label "Cohere Business Proposal Session"@en .

abi:CohereCreativeWritingSession a abi:InferenceSession ;
    rdfs:label "Cohere Creative Writing Session"@en .

abi:CohereCodeGenerationSession a abi:InferenceSession ;
    rdfs:label "Cohere Code Generation Session"@en .


#################################################################
#    BFO Bucket 1: Material Entity - Command A
#################################################################

abi:Command_A a abi:AIModelInstance ;
    rdfs:label "Command A"@en ;
    abi:modelSlug "command-a"@en ;
    abi:provider "Cohere"@en ;
    abi:sourceAPI "artificial_analysis"@en .

#################################################################
#    BFO Bucket 2: Qualities - Performance & Cost Metrics
#################################################################

abi:Command_A abi:inputTokenCost 2.5 ;
    abi:inputTokenCostCurrency "USD"@en ;
    abi:outputTokenCost 10 ;
    abi:outputTokenCostCurrency "USD"@en ;
    abi:blendedCost 4.375 ;
    abi:blendedCostCurrency "USD"@en ;
    abi:outputSpeed 155.976 ;
    abi:outputSpeedUnit "tokens_per_second"@en ;
    abi:timeToFirstToken 0.225 ;
    abi:timeToFirstTokenUnit "seconds"@en ;
    abi:timeToFirstAnswerToken 0.225 ;
    abi:timeToFirstAnswerTokenUnit "seconds"@en ;
    abi:intelligenceIndex 31.6 ;
    abi:codingIndex 28.4 ;
    abi:mathIndex 45.8 .

#################################################################
#    Relationships - Agent/Process/Model Network
#################################################################

abi:CohereAgent abi:canUtilizeModel abi:Command_A .
abi:CohereBusinessProposalProcess abi:utilizesModel abi:Command_A .
abi:CohereCreativeWritingProcess abi:utilizesModel abi:Command_A .
abi:CohereCodeGenerationProcess abi:utilizesModel abi:Command_A .

