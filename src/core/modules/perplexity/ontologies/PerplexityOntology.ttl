@prefix dc: <http://purl.org/dc/terms/> .

<http://naas.ai/ontology/abi/PerplexityOntology> a owl:Ontology ;
    owl:imports <http://ontology.naas.ai/abi/AIAgentOntology> ;
    dc:title "Perplexity AI Agent Ontology"@en ;
    dc:description "BFO-grounded ontology for Perplexity AI models and processes"@en ;
    dc:created "20250812T101446"@en .


#################################################################
#    BFO Bucket 1: Material Entities (WHAT/WHO)
#################################################################

abi:PerplexityAgent a abi:AIAgent ;
    rdfs:label "Perplexity AI Agent"@en ;
    rdfs:comment "AI Agent capable of utilizing Perplexity models"@en ;
    abi:hasSpecializedRole "Multi-purpose AI processing"@en .


#################################################################
#    BFO Bucket 4: Processes (HOW-IT-HAPPENS)
#################################################################

abi:PerplexityBusinessProposalProcess a abi:BusinessProposalCreationProcess ;
    rdfs:label "Perplexity Business Proposal Process"@en ;
    abi:hasParticipant abi:PerplexityAgent ;
    abi:realizesCapability capability:TextGenerationCapability ;
    abi:hasTemporalRegion abi:PerplexityBusinessProposalSession ;
    abi:hasQuality abi:PerplexityBusinessProposalQuality .

abi:PerplexityCreativeWritingProcess a abi:CreativeWritingProcess ;
    rdfs:label "Perplexity Creative Writing Process"@en ;
    abi:hasParticipant abi:PerplexityAgent ;
    abi:realizesCapability capability:TextGenerationCapability ;
    abi:hasTemporalRegion abi:PerplexityCreativeWritingSession ;
    abi:hasQuality abi:PerplexityCreativeWritingQuality .

abi:PerplexityCodeGenerationProcess a abi:CodeGenerationProcess ;
    rdfs:label "Perplexity Code Generation Process"@en ;
    abi:hasParticipant abi:PerplexityAgent ;
    abi:realizesCapability capability:CodeGenerationCapability ;
    abi:hasTemporalRegion abi:PerplexityCodeGenerationSession ;
    abi:hasQuality abi:PerplexityCodeGenerationQuality .

#################################################################
#    BFO Bucket 5: Temporal Regions (WHEN)
#################################################################

abi:PerplexityBusinessProposalSession a abi:InferenceSession ;
    rdfs:label "Perplexity Business Proposal Session"@en .

abi:PerplexityCreativeWritingSession a abi:InferenceSession ;
    rdfs:label "Perplexity Creative Writing Session"@en .

abi:PerplexityCodeGenerationSession a abi:InferenceSession ;
    rdfs:label "Perplexity Code Generation Session"@en .


