@prefix abi: <http://naas.ai/ontology/abi#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix capability: <http://ontology.naas.ai/abi/capability/> .
@prefix dc: <http://purl.org/dc/terms/> .

<http://naas.ai/ontology/abi/KimiOntology> a owl:Ontology ;
    owl:imports <http://ontology.naas.ai/abi/AIAgentOntology> ;
    dc:title "Kimi AI Agent Ontology"@en ;
    dc:description "BFO-grounded ontology for Kimi AI models and processes"@en ;
    dc:created "20250812T101446"@en .


#################################################################
#    BFO Bucket 1: Material Entities (WHAT/WHO)
#################################################################

abi:KimiAgent a abi:AIAgent ;
    rdfs:label "Kimi AI Agent"@en ;
    rdfs:comment "AI Agent capable of utilizing Kimi models"@en ;
    abi:hasSpecializedRole "Multi-purpose AI processing"@en .


#################################################################
#    BFO Bucket 4: Processes (HOW-IT-HAPPENS)
#################################################################

abi:KimiBusinessProposalProcess a abi:BusinessProposalCreationProcess ;
    rdfs:label "Kimi Business Proposal Process"@en ;
    abi:hasParticipant abi:KimiAgent ;
    abi:realizesCapability capability:TextGenerationCapability ;
    abi:hasTemporalRegion abi:KimiBusinessProposalSession ;
    abi:hasQuality abi:KimiBusinessProposalQuality .

abi:KimiCreativeWritingProcess a abi:CreativeWritingProcess ;
    rdfs:label "Kimi Creative Writing Process"@en ;
    abi:hasParticipant abi:KimiAgent ;
    abi:realizesCapability capability:TextGenerationCapability ;
    abi:hasTemporalRegion abi:KimiCreativeWritingSession ;
    abi:hasQuality abi:KimiCreativeWritingQuality .

abi:KimiCodeGenerationProcess a abi:CodeGenerationProcess ;
    rdfs:label "Kimi Code Generation Process"@en ;
    abi:hasParticipant abi:KimiAgent ;
    abi:realizesCapability capability:CodeGenerationCapability ;
    abi:hasTemporalRegion abi:KimiCodeGenerationSession ;
    abi:hasQuality abi:KimiCodeGenerationQuality .

#################################################################
#    BFO Bucket 5: Temporal Regions (WHEN)
#################################################################

abi:KimiBusinessProposalSession a abi:InferenceSession ;
    rdfs:label "Kimi Business Proposal Session"@en .

abi:KimiCreativeWritingSession a abi:InferenceSession ;
    rdfs:label "Kimi Creative Writing Session"@en .

abi:KimiCodeGenerationSession a abi:InferenceSession ;
    rdfs:label "Kimi Code Generation Session"@en .


#################################################################
#    BFO Bucket 1: Material Entity - Kimi K2
#################################################################

abi:Kimi_K2 a abi:AIModelInstance ;
    rdfs:label "Kimi K2"@en ;
    abi:modelSlug "kimi-k2"@en ;
    abi:provider "Moonshot AI"@en ;
    abi:sourceAPI "artificial_analysis"@en .

#################################################################
#    BFO Bucket 2: Qualities - Performance & Cost Metrics
#################################################################

abi:Kimi_K2 abi:inputTokenCost 0.6 ;
    abi:inputTokenCostCurrency "USD"@en ;
    abi:outputTokenCost 2.5 ;
    abi:outputTokenCostCurrency "USD"@en ;
    abi:blendedCost 1.075 ;
    abi:blendedCostCurrency "USD"@en ;
    abi:outputSpeed 45.297 ;
    abi:outputSpeedUnit "tokens_per_second"@en ;
    abi:timeToFirstToken 0.522 ;
    abi:timeToFirstTokenUnit "seconds"@en ;
    abi:timeToFirstAnswerToken 0.522 ;
    abi:timeToFirstAnswerTokenUnit "seconds"@en ;
    abi:intelligenceIndex 48.6 ;
    abi:codingIndex 36.5 ;
    abi:mathIndex 83.2 .

#################################################################
#    Relationships - Agent/Process/Model Network
#################################################################

abi:KimiAgent abi:canUtilizeModel abi:Kimi_K2 .
abi:KimiBusinessProposalProcess abi:utilizesModel abi:Kimi_K2 .
abi:KimiCreativeWritingProcess abi:utilizesModel abi:Kimi_K2 .
abi:KimiCodeGenerationProcess abi:utilizesModel abi:Kimi_K2 .
