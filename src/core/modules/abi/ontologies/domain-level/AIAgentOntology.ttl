@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dc11: <http://purl.org/dc/elements/1.1/> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix cco: <https://www.commoncoreontologies.org/> .
@prefix abi: <http://ontology.naas.ai/abi/> .
@prefix capability: <http://ontology.naas.ai/abi/capability/> .

<http://ontology.naas.ai/abi/AIAgentOntology> rdf:type owl:Ontology ;
    owl:imports <https://www.commoncoreontologies.org/AgentOntology> ;
    owl:imports <http://ontology.naas.ai/abi/CapabilityOntology> ;
    owl:versionIRI <https://github.com/jupyter-naas/abi/tree/cli/src/core/modules/abi/ontologies/domain-level/AIAgentOntology.ttl> ;
    dc11:contributor "Jeremy Ravenel" , "Maxime Jublou" , "Florent Ravenel" ;
    dc:description "Ontology for AI agents and AI systems using BFO and CCO standards, integrated with capability ontology."@en ;
    dc:title "AI Agent Ontology (BFO+CCO)" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/contributor
dc11:contributor rdf:type owl:AnnotationProperty .

###  http://purl.org/dc/terms/description
dc:description rdf:type owl:AnnotationProperty .

###  http://purl.org/dc/terms/title
dc:title rdf:type owl:AnnotationProperty .

###  http://www.w3.org/2004/02/skos/core#definition
skos:definition rdf:type owl:AnnotationProperty .

###  http://www.w3.org/2004/02/skos/core#example
skos:example rdf:type owl:AnnotationProperty .

###  http://www.w3.org/2004/02/skos/core#prefLabel
skos:prefLabel rdf:type owl:AnnotationProperty .

#################################################################
#    Object Properties
#################################################################

###  http://ontology.naas.ai/abi/hasCapability
abi:hasCapability a owl:ObjectProperty ;
    rdfs:label "has capability"@en ;
    rdfs:domain abi:AIAgent ;
    rdfs:range capability:Capability ;
    skos:definition "A relation between an AI agent and its capabilities."@en .

###  http://ontology.naas.ai/abi/realizesCapability
abi:realizesCapability a owl:ObjectProperty ;
    rdfs:label "realizes capability"@en ;
    rdfs:domain abi:AIProcess ;
    rdfs:range capability:Capability ;
    skos:definition "A relation between an AI process and the capability it realizes."@en .

###  http://ontology.naas.ai/abi/hasAIAgent
abi:hasAIAgent a owl:ObjectProperty ;
    rdfs:label "has AI agent"@en ;
    rdfs:domain abi:AISystem ;
    rdfs:range abi:AIAgent ;
    skos:definition "A relation between an AI system and the AI agent it implements."@en .

###  http://ontology.naas.ai/abi/canUtilizeModel
abi:canUtilizeModel a owl:ObjectProperty ;
    rdfs:label "can utilize model"@en ;
    rdfs:domain abi:AIAgent ;
    rdfs:range abi:AIModelInstance ;
    skos:definition "A relation between an AI agent and the AI model instances it can dynamically select for different processes."@en ;
    rdfs:comment "AI agents can choose different models based on process requirements, cost optimization, and capability matching."@en .

###  http://ontology.naas.ai/abi/isAIAgentOf
abi:isAIAgentOf a owl:ObjectProperty ;
    rdfs:label "is AI agent of"@en ;
    rdfs:domain abi:AIAgent ;
    rdfs:range abi:AISystem ;
    owl:inverseOf abi:hasAIAgent ;
    skos:definition "A relation between an AI agent and the AI system that implements it."@en .

###  http://ontology.naas.ai/abi/hasQuality
abi:hasQuality a owl:ObjectProperty ;
    rdfs:label "has quality"@en ;
    rdfs:domain bfo:BFO_0000040 ;
    rdfs:range bfo:BFO_0000019 ;
    skos:definition "A relation between a material entity and its qualities."@en .

###  http://ontology.naas.ai/abi/hasParticipant
abi:hasParticipant a owl:ObjectProperty ;
    rdfs:label "has participant"@en ;
    rdfs:domain bfo:BFO_0000015 ;
    rdfs:range bfo:BFO_0000040 ;
    skos:definition "A relation between a process and the material entities that participate in it."@en .

###  http://ontology.naas.ai/abi/occursIn
abi:occursIn a owl:ObjectProperty ;
    rdfs:label "occurs in"@en ;
    rdfs:domain bfo:BFO_0000015 ;
    rdfs:range bfo:BFO_0000006 ;
    skos:definition "A relation between a process and the spatial region where it occurs."@en .

###  http://ontology.naas.ai/abi/hasTemporalRegion
abi:hasTemporalRegion a owl:ObjectProperty ;
    rdfs:label "has temporal region"@en ;
    rdfs:domain bfo:BFO_0000015 ;
    rdfs:range bfo:BFO_0000008 ;
    skos:definition "A relation between a process and its temporal region."@en .

###  http://ontology.naas.ai/abi/isDocumentedBy
abi:isDocumentedBy a owl:ObjectProperty ;
    rdfs:label "is documented by"@en ;
    rdfs:domain bfo:BFO_0000015 ;
    rdfs:range bfo:BFO_0000031 ;
    skos:definition "A relation between a process and the information content that documents it."@en .

###  http://ontology.naas.ai/abi/hasSpecification
abi:hasSpecification a owl:ObjectProperty ;
    rdfs:label "has specification"@en ;
    rdfs:domain bfo:BFO_0000040 ;
    rdfs:range bfo:BFO_0000031 ;
    skos:definition "A relation between a material entity and its specification."@en .

###  http://ontology.naas.ai/abi/collaboratesWith
abi:collaboratesWith a owl:ObjectProperty ;
    rdfs:label "collaborates with"@en ;
    rdfs:domain abi:AIAgent ;
    rdfs:range abi:AIAgent ;
    skos:definition "A relation between AI agents that work together to achieve a common goal."@en ;
    rdfs:comment "This relationship models agent collaboration where multiple AI agents coordinate their efforts, such as when a text generation agent works with an image analysis agent to create multimodal content."@en .

###  http://ontology.naas.ai/abi/hasSpecializedRole
abi:hasSpecializedRole a owl:ObjectProperty ;
    rdfs:label "has specialized role"@en ;
    rdfs:domain abi:AIAgent ;
    rdfs:range abi:AgentRole ;
    skos:definition "A relation between an AI agent and its specialized role within a system."@en ;
    rdfs:comment "This relationship captures the specific function or role an AI agent plays, such as 'primary responder', 'fallback handler', or 'specialized task executor'."@en .

###  http://ontology.naas.ai/abi/isSubsystemOf
abi:isSubsystemOf a owl:ObjectProperty ;
    rdfs:label "is subsystem of"@en ;
    rdfs:domain abi:AISystem ;
    rdfs:range abi:AISystem ;
    skos:definition "A relation between AI systems where one system is a component of another larger system."@en ;
    rdfs:comment "This relationship models hierarchical system composition, such as when a microservice architecture contains multiple specialized subsystems that work together."@en .

###  http://ontology.naas.ai/abi/hasSubsystem
abi:hasSubsystem a owl:ObjectProperty ;
    rdfs:label "has subsystem"@en ;
    rdfs:domain abi:AISystem ;
    rdfs:range abi:AISystem ;
    owl:inverseOf abi:isSubsystemOf ;
    skos:definition "A relation between AI systems where one system contains other systems as components."@en ;
    rdfs:comment "This relationship models system composition where a larger AI system is composed of multiple smaller, specialized subsystems."@en .

###  http://ontology.naas.ai/abi/precedesInTime
abi:precedesInTime a owl:ObjectProperty ;
    rdfs:label "precedes in time"@en ;
    rdfs:domain bfo:BFO_0000015 ;
    rdfs:range bfo:BFO_0000015 ;
    skos:definition "A relation between processes where one process occurs before another in a temporal sequence."@en ;
    rdfs:comment "This relationship models temporal coordination between AI processes, such as when data preprocessing must occur before model inference."@en .

###  http://ontology.naas.ai/abi/triggersProcess
abi:triggersProcess a owl:ObjectProperty ;
    rdfs:label "triggers process"@en ;
    rdfs:domain bfo:BFO_0000015 ;
    rdfs:range bfo:BFO_0000015 ;
    skos:definition "A relation between processes where the completion of one process initiates another process."@en ;
    rdfs:comment "This relationship models causal dependencies between AI processes, such as when a text analysis process triggers a response generation process."@en .

###  http://ontology.naas.ai/abi/hasFallbackAgent
abi:hasFallbackAgent a owl:ObjectProperty ;
    rdfs:label "has fallback agent"@en ;
    rdfs:domain abi:AIAgent ;
    rdfs:range abi:AIAgent ;
    skos:definition "A relation between AI agents where one agent serves as a backup when the primary agent fails."@en ;
    rdfs:comment "This relationship models fault tolerance in AI systems, ensuring continuous service availability through redundant agent configurations."@en .

###  http://ontology.naas.ai/abi/hasLoadBalancer
abi:hasLoadBalancer a owl:ObjectProperty ;
    rdfs:label "has load balancer"@en ;
    rdfs:domain abi:AISystem ;
    rdfs:range abi:LoadBalancer ;
    skos:definition "A relation between an AI system and its load balancing component."@en ;
    rdfs:comment "This relationship models how AI systems distribute workload across multiple agents to optimize performance and resource utilization."@en .

###  http://ontology.naas.ai/abi/utilizesModel
abi:utilizesModel a owl:ObjectProperty ;
    rdfs:label "utilizes model"@en ;
    rdfs:domain bfo:BFO_0000015 ;
    rdfs:range abi:AIModelInstance ;
    skos:definition "A relation between an AI process and the specific AI model instance actually used in that process execution."@en ;
    rdfs:comment "This captures the actual model selection made by an AI agent for a specific process instance."@en .

