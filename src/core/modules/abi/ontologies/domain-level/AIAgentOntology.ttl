#################################################################
#    General axioms
#################################################################

# Class Axioms
abi:AIModelInstance rdfs:subClassOf abi:AISystem ;
    rdfs:comment "Every AI model instance is a type of AI system."@en .

abi:TextGenerationProcess rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty abi:realizesCapability ;
    owl:someValuesFrom capability:TextGenerationCapability ;
    rdfs:comment "Every text generation process must realize a text generation capability."@en
] .

abi:ImageAnalysisProcess rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty abi:realizesCapability ;
    owl:someValuesFrom capability:ImageAnalysisCapability ;
    rdfs:comment "Every image analysis process must realize an image analysis capability."@en
] .

abi:CodeGenerationProcess rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty abi:realizesCapability ;
    owl:someValuesFrom capability:CodeGenerationCapability ;
    rdfs:comment "Every code generation process must realize a code generation capability."@en
] .

# Advanced Relationship Axioms
abi:AIAgent rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty abi:hasSpecializedRole ;
    owl:someValuesFrom abi:AgentRole ;
    rdfs:comment "Every AI agent must have at least one specialized role within a system."@en
] .

abi:AIAgent rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty abi:canUtilizeModel ;
    owl:someValuesFrom abi:AIModelInstance ;
    rdfs:comment "Every AI agent must be able to utilize at least one AI model instance."@en
] .

abi:AISystem rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty abi:hasLoadBalancer ;
    owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
    rdfs:comment "Every AI system can have at most one load balancer for workload distribution."@en
] .

abi:TextGenerationProcess rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty abi:triggersProcess ;
    owl:someValuesFrom abi:TextGenerationProcess ;
    rdfs:comment "Text generation processes can trigger other text generation processes in workflows."@en
] .

# Model-Process Relationship Axioms
bfo:BFO_0000015 rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty abi:utilizesModel ;
    owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
    rdfs:comment "Each process instance utilizes exactly one AI model instance."@en
] .

# Property Axioms
abi:hasAIAgent owl:inverseOf abi:isAIAgentOf ;
    rdfs:comment "If an AI system has an AI agent, then that AI agent is an agent of that AI system."@en .

abi:hasSubsystem owl:inverseOf abi:isSubsystemOf ;
    rdfs:comment "If an AI system has a subsystem, then that subsystem is a component of that AI system."@en .

abi:hasCapability owl:inverseOf [
    rdf:type owl:ObjectProperty ;
    rdfs:label "is capability of"@en ;
    rdfs:domain bfo:BFO_0000017 ;
    rdfs:range bfo:BFO_0000040 ;
    rdfs:comment "If a material entity has a capability, then that capability belongs to that material entity."@en
] .

abi:realizesCapability owl:inverseOf [
    rdf:type owl:ObjectProperty ;
    rdfs:label "is realized by"@en ;
    rdfs:domain bfo:BFO_0000017 ;
    rdfs:range bfo:BFO_0000015 ;
    rdfs:comment "If a process realizes a capability, then that capability is realized by that process."@en
] .

# Advanced Property Characteristics
abi:collaboratesWith a owl:SymmetricProperty ;
    rdfs:comment "Collaboration is symmetric - if agent A collaborates with agent B, then agent B collaborates with agent A."@en .

abi:precedesInTime a owl:TransitiveProperty ;
    rdfs:comment "Temporal precedence is transitive - if process A precedes B and B precedes C, then A precedes C."@en .

abi:triggersProcess a owl:IrreflexiveProperty ;
    rdfs:comment "A process cannot trigger itself - this prevents infinite loops in workflow definitions."@en .

# Disjoint Classes
capability:TextGenerationCapability owl:disjointWith capability:ImageAnalysisCapability .
capability:TextGenerationCapability owl:disjointWith capability:CodeGenerationCapability .
