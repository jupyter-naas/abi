@prefix dc: <http://purl.org/dc/terms/> .

<http://naas.ai/ontology/abi/JambaOntology> a owl:Ontology ;
    owl:imports <http://ontology.naas.ai/abi/AIAgentOntology> ;
    dc:title "Jamba AI Agent Ontology"@en ;
    dc:description "BFO-grounded ontology for Jamba AI models and processes"@en ;
    dc:created "20250812T101446"@en .


#################################################################
#    BFO Bucket 1: Material Entities (WHAT/WHO)
#################################################################

abi:JambaAgent a abi:AIAgent ;
    rdfs:label "Jamba AI Agent"@en ;
    rdfs:comment "AI Agent capable of utilizing Jamba models"@en ;
    abi:hasSpecializedRole "Multi-purpose AI processing"@en .


#################################################################
#    BFO Bucket 4: Processes (HOW-IT-HAPPENS)
#################################################################

abi:JambaBusinessProposalProcess a abi:BusinessProposalCreationProcess ;
    rdfs:label "Jamba Business Proposal Process"@en ;
    abi:hasParticipant abi:JambaAgent ;
    abi:realizesCapability capability:TextGenerationCapability ;
    abi:hasTemporalRegion abi:JambaBusinessProposalSession ;
    abi:hasQuality abi:JambaBusinessProposalQuality .

abi:JambaCreativeWritingProcess a abi:CreativeWritingProcess ;
    rdfs:label "Jamba Creative Writing Process"@en ;
    abi:hasParticipant abi:JambaAgent ;
    abi:realizesCapability capability:TextGenerationCapability ;
    abi:hasTemporalRegion abi:JambaCreativeWritingSession ;
    abi:hasQuality abi:JambaCreativeWritingQuality .

abi:JambaCodeGenerationProcess a abi:CodeGenerationProcess ;
    rdfs:label "Jamba Code Generation Process"@en ;
    abi:hasParticipant abi:JambaAgent ;
    abi:realizesCapability capability:CodeGenerationCapability ;
    abi:hasTemporalRegion abi:JambaCodeGenerationSession ;
    abi:hasQuality abi:JambaCodeGenerationQuality .

#################################################################
#    BFO Bucket 5: Temporal Regions (WHEN)
#################################################################

abi:JambaBusinessProposalSession a abi:InferenceSession ;
    rdfs:label "Jamba Business Proposal Session"@en .

abi:JambaCreativeWritingSession a abi:InferenceSession ;
    rdfs:label "Jamba Creative Writing Session"@en .

abi:JambaCodeGenerationSession a abi:InferenceSession ;
    rdfs:label "Jamba Code Generation Session"@en .


