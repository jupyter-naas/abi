#    BFO Bucket 2: Qualities - Performance & Cost Metrics
#################################################################

abi:Phi_4 abi:inputTokenCost 0.125 ;
    abi:inputTokenCostCurrency "USD"@en ;
    abi:outputTokenCost 0.5 ;
    abi:outputTokenCostCurrency "USD"@en ;
    abi:blendedCost 0.219 ;
    abi:blendedCostCurrency "USD"@en ;
    abi:outputSpeed 44.527 ;
    abi:outputSpeedUnit "tokens_per_second"@en ;
    abi:timeToFirstToken 0.393 ;
    abi:timeToFirstTokenUnit "seconds"@en ;
    abi:timeToFirstAnswerToken 0.393 ;
    abi:timeToFirstAnswerTokenUnit "seconds"@en ;
    abi:intelligenceIndex 27.9 ;
    abi:codingIndex 24.6 ;
    abi:mathIndex 47.7 .

#################################################################
#    Relationships - Agent/Process/Model Network
#################################################################

abi:PhiAgent abi:canUtilizeModel abi:Phi_4 .
abi:PhiBusinessProposalProcess abi:utilizesModel abi:Phi_4 .
abi:PhiCreativeWritingProcess abi:utilizesModel abi:Phi_4 .
abi:PhiCodeGenerationProcess abi:utilizesModel abi:Phi_4 .

