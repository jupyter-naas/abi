@prefix abi: <http://naas.ai/ontology/abi#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix capability: <http://ontology.naas.ai/abi/capability/> .
@prefix dc: <http://purl.org/dc/terms/> .

<http://naas.ai/ontology/abi/PhiOntology> a owl:Ontology ;
    owl:imports <http://ontology.naas.ai/abi/AIAgentOntology> ;
    dc:title "Phi AI Agent Ontology"@en ;
    dc:description "BFO-grounded ontology for Phi AI models and processes"@en ;
    dc:created "20250812T101446"@en .


#################################################################
#    BFO Bucket 1: Material Entities (WHAT/WHO)
#################################################################

abi:PhiAgent a abi:AIAgent ;
    rdfs:label "Phi AI Agent"@en ;
    rdfs:comment "AI Agent capable of utilizing Phi models"@en ;
    abi:hasSpecializedRole "Multi-purpose AI processing"@en .


#################################################################
#    BFO Bucket 4: Processes (HOW-IT-HAPPENS)
#################################################################

abi:PhiBusinessProposalProcess a abi:BusinessProposalCreationProcess ;
    rdfs:label "Phi Business Proposal Process"@en ;
    abi:hasParticipant abi:PhiAgent ;
    abi:realizesCapability capability:TextGenerationCapability ;
    abi:hasTemporalRegion abi:PhiBusinessProposalSession ;
    abi:hasQuality abi:PhiBusinessProposalQuality .

abi:PhiCreativeWritingProcess a abi:CreativeWritingProcess ;
    rdfs:label "Phi Creative Writing Process"@en ;
    abi:hasParticipant abi:PhiAgent ;
    abi:realizesCapability capability:TextGenerationCapability ;
    abi:hasTemporalRegion abi:PhiCreativeWritingSession ;
    abi:hasQuality abi:PhiCreativeWritingQuality .

abi:PhiCodeGenerationProcess a abi:CodeGenerationProcess ;
    rdfs:label "Phi Code Generation Process"@en ;
    abi:hasParticipant abi:PhiAgent ;
    abi:realizesCapability capability:CodeGenerationCapability ;
    abi:hasTemporalRegion abi:PhiCodeGenerationSession ;
    abi:hasQuality abi:PhiCodeGenerationQuality .

#################################################################
#    BFO Bucket 5: Temporal Regions (WHEN)
#################################################################

abi:PhiBusinessProposalSession a abi:InferenceSession ;
    rdfs:label "Phi Business Proposal Session"@en .

abi:PhiCreativeWritingSession a abi:InferenceSession ;
    rdfs:label "Phi Creative Writing Session"@en .

abi:PhiCodeGenerationSession a abi:InferenceSession ;
    rdfs:label "Phi Code Generation Session"@en .


#################################################################
#    BFO Bucket 1: Material Entity - Phi-4
#################################################################

abi:Phi_4 a abi:AIModelInstance ;
    rdfs:label "Phi-4"@en ;
    abi:modelSlug "phi-4"@en ;
    abi:provider "Microsoft Azure"@en ;
    abi:sourceAPI "artificial_analysis"@en .

