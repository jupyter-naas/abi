#################################################################
#    Relationships - Agent/Process/Model Network
#################################################################

abi:SolarAgent abi:canUtilizeModel abi:Solar_Pro_2 .
abi:SolarBusinessProposalProcess abi:utilizesModel abi:Solar_Pro_2 .
abi:SolarCreativeWritingProcess abi:utilizesModel abi:Solar_Pro_2 .
abi:SolarCodeGenerationProcess abi:utilizesModel abi:Solar_Pro_2 .

