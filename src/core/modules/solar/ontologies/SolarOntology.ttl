@prefix abi: <http://naas.ai/ontology/abi#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix capability: <http://ontology.naas.ai/abi/capability/> .
@prefix dc: <http://purl.org/dc/terms/> .

<http://naas.ai/ontology/abi/SolarOntology> a owl:Ontology ;
    owl:imports <http://ontology.naas.ai/abi/AIAgentOntology> ;
    dc:title "Solar AI Agent Ontology"@en ;
    dc:description "BFO-grounded ontology for Solar AI models and processes"@en ;
    dc:created "20250812T101446"@en .


#################################################################
#    BFO Bucket 1: Material Entities (WHAT/WHO)
#################################################################

abi:SolarAgent a abi:AIAgent ;
    rdfs:label "Solar AI Agent"@en ;
    rdfs:comment "AI Agent capable of utilizing Solar models"@en ;
    abi:hasSpecializedRole "Multi-purpose AI processing"@en .


#################################################################
#    BFO Bucket 4: Processes (HOW-IT-HAPPENS)
#################################################################

abi:SolarBusinessProposalProcess a abi:BusinessProposalCreationProcess ;
    rdfs:label "Solar Business Proposal Process"@en ;
    abi:hasParticipant abi:SolarAgent ;
    abi:realizesCapability capability:TextGenerationCapability ;
    abi:hasTemporalRegion abi:SolarBusinessProposalSession ;
    abi:hasQuality abi:SolarBusinessProposalQuality .

abi:SolarCreativeWritingProcess a abi:CreativeWritingProcess ;
    rdfs:label "Solar Creative Writing Process"@en ;
    abi:hasParticipant abi:SolarAgent ;
    abi:realizesCapability capability:TextGenerationCapability ;
    abi:hasTemporalRegion abi:SolarCreativeWritingSession ;
    abi:hasQuality abi:SolarCreativeWritingQuality .

abi:SolarCodeGenerationProcess a abi:CodeGenerationProcess ;
    rdfs:label "Solar Code Generation Process"@en ;
    abi:hasParticipant abi:SolarAgent ;
    abi:realizesCapability capability:CodeGenerationCapability ;
    abi:hasTemporalRegion abi:SolarCodeGenerationSession ;
    abi:hasQuality abi:SolarCodeGenerationQuality .

#################################################################
#    BFO Bucket 5: Temporal Regions (WHEN)
#################################################################

abi:SolarBusinessProposalSession a abi:InferenceSession ;
    rdfs:label "Solar Business Proposal Session"@en .

abi:SolarCreativeWritingSession a abi:InferenceSession ;
    rdfs:label "Solar Creative Writing Session"@en .

abi:SolarCodeGenerationSession a abi:InferenceSession ;
    rdfs:label "Solar Code Generation Session"@en .


