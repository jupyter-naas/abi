@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dc11: <http://purl.org/dc/elements/1.1/> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix cco: <https://www.commoncoreontologies.org/> .
@prefix abi: <http://ontology.naas.ai/abi/> .
@prefix capability: <http://ontology.naas.ai/abi/capability/> .

<http://ontology.naas.ai/abi/CapabilityOntology> rdf:type owl:Ontology ;
    owl:imports <https://www.commoncoreontologies.org/AgentOntology> ;
    owl:imports <http://ontology.naas.ai/abi/AIAgentOntology> ;
    owl:versionIRI <https://github.com/jupyter-naas/abi/tree/cli/src/ontologies/domain-level/CapabilityOntology.ttl> ;
    dc11:contributor "Jeremy Ravenel" , "Maxime Jublou" , "Florent Ravenel" ;
    dc11:contributor "Barry Smith" , "John Beverley" ;
    dc:description "Capability ontology based on Barry Smith and John Beverley's work, following BFO standards and integrating with AIAgent ontology."@en ;
    dc:title "Capability Ontology (BFO+CCO)" ;
    dc:source "Capabilities: An Ontology - John Beverley, David Limbaugh, Eric Merrell, Peter M. Koch, Barry Smith" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/contributor
dc11:contributor rdf:type owl:AnnotationProperty .

###  http://purl.org/dc/terms/description
dc:description rdf:type owl:AnnotationProperty .

###  http://purl.org/dc/terms/title
dc:title rdf:type owl:AnnotationProperty .

###  http://purl.org/dc/terms/source
dc:source rdf:type owl:AnnotationProperty .

###  http://www.w3.org/2004/02/skos/core#definition
skos:definition rdf:type owl:AnnotationProperty .

###  http://www.w3.org/2004/02/skos/core#example
skos:example rdf:type owl:AnnotationProperty .

###  http://www.w3.org/2004/02/skos/core#prefLabel
skos:prefLabel rdf:type owl:AnnotationProperty .

#################################################################
#    Classes
#################################################################

###  http://ontology.naas.ai/abi/capability/Capability
capability:Capability rdf:type owl:Class ;
    rdfs:subClassOf bfo:BFO_0000017 ;
    rdfs:label "Capability"@en ;
    skos:definition "A disposition in whose realization some organism or group of organisms has or had an interest."@en ;
    skos:example "A student's capability to code in C++, a car's capability to ferry passengers, an AI agent's capability to generate text."@en .

###  http://ontology.naas.ai/abi/capability/AICapability
capability:AICapability rdf:type owl:Class ;
    rdfs:subClassOf capability:Capability ;
    rdfs:label "AI Capability"@en ;
    skos:definition "A capability specifically borne by AI agents or AI systems."@en ;
    skos:example "Text generation capability, image analysis capability, reasoning capability."@en .

###  http://ontology.naas.ai/abi/capability/CreativeCapability
capability:CreativeCapability rdf:type owl:Class ;
    rdfs:subClassOf capability:AICapability ;
    rdfs:label "Creative Capability"@en ;
    skos:definition "A capability for creative tasks such as writing, art generation, or innovative problem-solving."@en ;
    skos:example "Creative writing, image generation, music composition."@en .

###  http://ontology.naas.ai/abi/capability/AnalyticalCapability
capability:AnalyticalCapability rdf:type owl:Class ;
    rdfs:subClassOf capability:AICapability ;
    rdfs:label "Analytical Capability"@en ;
    skos:definition "A capability for analysis, reasoning, and logical processing."@en ;
    skos:example "Data analysis, logical reasoning, mathematical computation."@en .

###  http://ontology.naas.ai/abi/capability/TechnicalCapability
capability:TechnicalCapability rdf:type owl:Class ;
    rdfs:subClassOf capability:AICapability ;
    rdfs:label "Technical Capability"@en ;
    skos:definition "A capability for technical tasks such as coding, debugging, or system design."@en ;
    skos:example "Code generation, debugging, system architecture design."@en .

###  http://ontology.naas.ai/abi/capability/CommunicationCapability
capability:CommunicationCapability rdf:type owl:Class ;
    rdfs:subClassOf capability:AICapability ;
    rdfs:label "Communication Capability"@en ;
    skos:definition "A capability for communication, translation, and language processing."@en ;
    skos:example "Translation, conversation, text summarization."@en .

###  http://ontology.naas.ai/abi/capability/PerceptionCapability
capability:PerceptionCapability rdf:type owl:Class ;
    rdfs:subClassOf capability:AICapability ;
    rdfs:label "Perception Capability"@en ;
    skos:definition "A capability for perceiving and understanding multimodal inputs."@en ;
    skos:example "Image recognition, audio processing, video analysis."@en .

###  http://ontology.naas.ai/abi/capability/ResearchCapability
capability:ResearchCapability rdf:type owl:Class ;
    rdfs:subClassOf capability:AICapability ;
    rdfs:label "Research Capability"@en ;
    skos:definition "A capability for research, information gathering, and knowledge synthesis."@en ;
    skos:example "Web search, fact verification, knowledge synthesis."@en .

###  http://ontology.naas.ai/abi/capability/TruthSeekingCapability
capability:TruthSeekingCapability rdf:type owl:Class ;
    rdfs:subClassOf capability:AICapability ;
    rdfs:label "Truth-Seeking Capability"@en ;
    skos:definition "A capability for truth-seeking, contrarian analysis, and unbiased evaluation."@en ;
    skos:example "Contrarian reasoning, truth verification, unbiased analysis."@en .

###  http://ontology.naas.ai/abi/capability/Interest
capability:Interest rdf:type owl:Class ;
    rdfs:subClassOf bfo:BFO_0000019 ;
    rdfs:label "Interest"@en ;
    skos:definition "A quality of an organism or group of organisms indicating stake in the realization of certain capabilities."@en ;
    skos:example "A user's interest in text generation, an organization's interest in data analysis."@en .

###  http://ontology.naas.ai/abi/capability/CapabilityRealization
capability:CapabilityRealization rdf:type owl:Class ;
    rdfs:subClassOf bfo:BFO_0000015 ;
    rdfs:label "Capability Realization"@en ;
    skos:definition "A process that realizes a specific capability."@en ;
    skos:example "The process of generating text, analyzing data, or creating an image."@en .

###  http://ontology.naas.ai/abi/capability/CapabilityEvaluation
capability:CapabilityEvaluation rdf:type owl:Class ;
    rdfs:subClassOf bfo:BFO_0000015 ;
    rdfs:label "Capability Evaluation"@en ;
    skos:definition "A process for evaluating the degree to which a capability is realized."@en ;
    skos:example "Testing code quality, evaluating text coherence, measuring accuracy."@en .

#################################################################
#    Object Properties
#################################################################

###  http://ontology.naas.ai/abi/capability/hasInterestIn
capability:hasInterestIn rdf:type owl:ObjectProperty ;
    rdfs:label "has interest in"@en ;
    rdfs:domain capability:Interest ;
    rdfs:range capability:Capability ;
    skos:definition "A relation between an interest and the capability it concerns."@en .

###  http://ontology.naas.ai/abi/capability/bearerOfInterest
capability:bearerOfInterest rdf:type owl:ObjectProperty ;
    rdfs:label "bearer of interest"@en ;
    rdfs:domain capability:Interest ;
    rdfs:range bfo:BFO_0000040 ;
    skos:definition "A relation between an interest and the organism or group that bears it."@en .

###  http://ontology.naas.ai/abi/capability/realizesCapability
capability:realizesCapability rdf:type owl:ObjectProperty ;
    rdfs:label "realizes capability"@en ;
    rdfs:domain capability:CapabilityRealization ;
    rdfs:range capability:Capability ;
    skos:definition "A relation between a capability realization process and the capability it realizes."@en .

###  http://ontology.naas.ai/abi/capability/evaluatesCapability
capability:evaluatesCapability rdf:type owl:ObjectProperty ;
    rdfs:label "evaluates capability"@en ;
    rdfs:domain capability:CapabilityEvaluation ;
    rdfs:range capability:Capability ;
    skos:definition "A relation between a capability evaluation process and the capability it evaluates."@en .

###  http://ontology.naas.ai/abi/capability/hasCapabilityType
capability:hasCapabilityType rdf:type owl:ObjectProperty ;
    rdfs:label "has capability type"@en ;
    rdfs:domain capability:Capability ;
    rdfs:range owl:Class ;
    skos:definition "A relation between a capability and its specific type classification."@en .

###  http://ontology.naas.ai/abi/capability/requiresCapability
capability:requiresCapability rdf:type owl:ObjectProperty ;
    rdfs:label "requires capability"@en ;
    rdfs:domain capability:Capability ;
    rdfs:range capability:Capability ;
    skos:definition "A relation indicating that one capability requires another capability for its realization."@en .

###  http://ontology.naas.ai/abi/capability/enablesCapability
capability:enablesCapability rdf:type owl:ObjectProperty ;
    rdfs:label "enables capability"@en ;
    rdfs:domain capability:Capability ;
    rdfs:range capability:Capability ;
    skos:definition "A relation indicating that one capability enables the realization of another capability."@en .

###  http://ontology.naas.ai/abi/capability/hasComplexity
capability:hasComplexity rdf:type owl:ObjectProperty ;
    rdfs:label "has complexity"@en ;
    rdfs:domain capability:Capability ;
    rdfs:range capability:ComplexityLevel ;
    skos:definition "A relation between a capability and its complexity level."@en .

###  http://ontology.naas.ai/abi/capability/ComplexityLevel
capability:ComplexityLevel rdf:type owl:Class ;
    rdfs:subClassOf bfo:BFO_0000019 ;
    rdfs:label "Complexity Level"@en ;
    skos:definition "A quality indicating the complexity level of a capability."@en .

###  http://ontology.naas.ai/abi/capability/LowComplexity
capability:LowComplexity rdf:type owl:Class ;
    rdfs:subClassOf capability:ComplexityLevel ;
    rdfs:label "Low Complexity"@en ;
    skos:definition "A low complexity level for capabilities."@en .

###  http://ontology.naas.ai/abi/capability/MediumComplexity
capability:MediumComplexity rdf:type owl:Class ;
    rdfs:subClassOf capability:ComplexityLevel ;
    rdfs:label "Medium Complexity"@en ;
    skos:definition "A medium complexity level for capabilities."@en .

###  http://ontology.naas.ai/abi/capability/HighComplexity
capability:HighComplexity rdf:type owl:Class ;
    rdfs:subClassOf capability:ComplexityLevel ;
    rdfs:label "High Complexity"@en ;
    skos:definition "A high complexity level for capabilities."@en .

#################################################################
#    Data Properties
#################################################################

###  http://ontology.naas.ai/abi/capability/capabilityName
capability:capabilityName rdf:type owl:DatatypeProperty ;
    rdfs:label "capability name"@en ;
    rdfs:domain capability:Capability ;
    rdfs:range xsd:string ;
    skos:definition "The name or identifier of a capability."@en .

###  http://ontology.naas.ai/abi/capability/capabilityDescription
capability:capabilityDescription rdf:type owl:DatatypeProperty ;
    rdfs:label "capability description"@en ;
    rdfs:domain capability:Capability ;
    rdfs:range xsd:string ;
    skos:definition "A description of what the capability enables."@en .

###  http://ontology.naas.ai/abi/capability/realizationFrequency
capability:realizationFrequency rdf:type owl:DatatypeProperty ;
    rdfs:label "realization frequency"@en ;
    rdfs:domain capability:Capability ;
    rdfs:range xsd:decimal ;
    skos:definition "The frequency with which a capability is realized."@en .

###  http://ontology.naas.ai/abi/capability/successRate
capability:successRate rdf:type owl:DatatypeProperty ;
    rdfs:label "success rate"@en ;
    rdfs:domain capability:Capability ;
    rdfs:range xsd:decimal ;
    skos:definition "The success rate of capability realizations."@en .

###  http://ontology.naas.ai/abi/capability/interestStrength
capability:interestStrength rdf:type owl:DatatypeProperty ;
    rdfs:label "interest strength"@en ;
    rdfs:domain capability:Interest ;
    rdfs:range xsd:decimal ;
    skos:definition "The strength of interest in a capability realization."@en .

###  http://ontology.naas.ai/abi/capability/creationDate
capability:creationDate rdf:type owl:DatatypeProperty ;
    rdfs:label "creation date"@en ;
    rdfs:domain capability:Capability ;
    rdfs:range xsd:dateTime ;
    skos:definition "The date when a capability was created or discovered."@en .

###  http://ontology.naas.ai/abi/capability/expirationDate
capability:expirationDate rdf:type owl:DatatypeProperty ;
    rdfs:label "expiration date"@en ;
    rdfs:domain capability:Capability ;
    rdfs:range xsd:dateTime ;
    skos:definition "The date when a capability expires or becomes obsolete."@en .

#################################################################
#    Axioms
#################################################################

# All functions are capabilities (as per Smith & Beverley)
bfo:BFO_0000034 rdfs:subClassOf capability:Capability .

# Capabilities are dispositions (as per Smith & Beverley)
capability:Capability rdfs:subClassOf bfo:BFO_0000017 .

# Capabilities require material entities as bearers
capability:Capability rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty bfo:BFO_0000052 ;
    owl:someValuesFrom bfo:BFO_0000040
] .

# Capabilities are realized through processes
capability:Capability rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty bfo:BFO_0000053 ;
    owl:someValuesFrom bfo:BFO_0000015
] .

# AI Capabilities are borne by AI agents or systems
capability:AICapability rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty bfo:BFO_0000052 ;
    owl:someValuesFrom abi:AIAgent
] .

# Capability realizations have participants
capability:CapabilityRealization rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty abi:hasParticipant ;
    owl:someValuesFrom bfo:BFO_0000040
] .

# Interests are qualities of organisms or groups
capability:Interest rdfs:subClassOf [
    rdf:type owl:Restriction ;
    owl:onProperty bfo:BFO_0000052 ;
    owl:someValuesFrom bfo:BFO_0000040
] .

