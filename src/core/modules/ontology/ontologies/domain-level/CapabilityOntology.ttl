#    Examples
#################################################################

###  http://ontology.naas.ai/abi/capability/TextGenerationCapability
capability:TextGenerationCapability rdf:type capability:CreativeCapability ;
    rdfs:label "Text Generation Capability"@en ;
    capability:capabilityName "text_generation" ;
    capability:capabilityDescription "The capability to generate coherent and contextually appropriate text."@en ;
    capability:hasComplexity capability:MediumComplexity ;
    capability:realizationFrequency "0.85"^^xsd:decimal ;
    capability:successRate "0.92"^^xsd:decimal ;
    skos:example "Generating articles, writing emails, creating stories."@en .

