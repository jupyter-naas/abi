@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dc11: <http://purl.org/dc/elements/1.1/> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix cco: <https://www.commoncoreontologies.org/> .
@prefix abi: <http://ontology.naas.ai/abi/> .

<http://ontology.naas.ai/abi/AbiOntology> a owl:Ontology ;
    dc11:contributor "Jeremy Ravenel" , "Maxime Jublou" , "Florent Ravenel" ;
    dc:description "Domain ontoloy for Abi project."@en ;
    dc:title "Abi Ontology" .

#################################################################
#    Data Properties
#################################################################

abi:is_curated_in_foundry a owl:DatatypeProperty ;
    rdfs:label "is curated in foundry"@en ;
    skos:definition "Relates a class to the foundry it is curated in."@en ;
    skos:example "The class cco:ont00001262 is curated in the foundry 'entreprise_management_foundry' and 'personal_ai_foundry'."@en ;
    rdfs:domain bfo:BFO_0000001 ;
    rdfs:range xsd:string .

#################################################################
#    Mapping Foundry Ontology to Existing Classes
#################################################################

# Organization
cco:ont00001180 abi:is_curated_in_foundry "entreprise_management_foundry" .

# Person
cco:ont00001262 abi:is_curated_in_foundry "entreprise_management_foundry", "personal_ai_foundry" .

# Act of Employment
cco:ont00001226 abi:is_curated_in_foundry "entreprise_management_foundry", "personal_ai_foundry" .

# Act of Volunteering
cco:ont00001031 abi:is_curated_in_foundry "entreprise_management_foundry", "personal_ai_foundry" .

# Legal Name
cco:ont00001331 abi:is_curated_in_foundry "entreprise_management_foundry" .

