@prefix abi: <http://naas.ai/ontology/abi#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix capability: <http://ontology.naas.ai/abi/capability/> .
@prefix dc: <http://purl.org/dc/terms/> .

<http://naas.ai/ontology/abi/MinimaxOntology> a owl:Ontology ;
    owl:imports <http://ontology.naas.ai/abi/AIAgentOntology> ;
    dc:title "Minimax AI Agent Ontology"@en ;
    dc:description "BFO-grounded ontology for Minimax AI models and processes"@en ;
    dc:created "20250812T101446"@en .


#################################################################
#    BFO Bucket 1: Material Entities (WHAT/WHO)
#################################################################

abi:MinimaxAgent a abi:AIAgent ;
    rdfs:label "Minimax AI Agent"@en ;
    rdfs:comment "AI Agent capable of utilizing Minimax models"@en ;
    abi:hasSpecializedRole "Multi-purpose AI processing"@en .


#################################################################
#    BFO Bucket 4: Processes (HOW-IT-HAPPENS)
#################################################################

abi:MinimaxBusinessProposalProcess a abi:BusinessProposalCreationProcess ;
    rdfs:label "Minimax Business Proposal Process"@en ;
    abi:hasParticipant abi:MinimaxAgent ;
    abi:realizesCapability capability:TextGenerationCapability ;
    abi:hasTemporalRegion abi:MinimaxBusinessProposalSession ;
    abi:hasQuality abi:MinimaxBusinessProposalQuality .

abi:MinimaxCreativeWritingProcess a abi:CreativeWritingProcess ;
    rdfs:label "Minimax Creative Writing Process"@en ;
    abi:hasParticipant abi:MinimaxAgent ;
    abi:realizesCapability capability:TextGenerationCapability ;
    abi:hasTemporalRegion abi:MinimaxCreativeWritingSession ;
    abi:hasQuality abi:MinimaxCreativeWritingQuality .

abi:MinimaxCodeGenerationProcess a abi:CodeGenerationProcess ;
    rdfs:label "Minimax Code Generation Process"@en ;
    abi:hasParticipant abi:MinimaxAgent ;
    abi:realizesCapability capability:CodeGenerationCapability ;
    abi:hasTemporalRegion abi:MinimaxCodeGenerationSession ;
    abi:hasQuality abi:MinimaxCodeGenerationQuality .

#################################################################
#    BFO Bucket 5: Temporal Regions (WHEN)
#################################################################

abi:MinimaxBusinessProposalSession a abi:InferenceSession ;
    rdfs:label "Minimax Business Proposal Session"@en .

abi:MinimaxCreativeWritingSession a abi:InferenceSession ;
    rdfs:label "Minimax Creative Writing Session"@en .

abi:MinimaxCodeGenerationSession a abi:InferenceSession ;
    rdfs:label "Minimax Code Generation Session"@en .


#################################################################
#    BFO Bucket 1: Material Entity - MiniMax-Text-01
#################################################################

abi:MiniMax_Text_01 a abi:AIModelInstance ;
    rdfs:label "MiniMax-Text-01"@en ;
    abi:modelSlug "minimax-text-01"@en ;
    abi:provider "MiniMax"@en ;
    abi:sourceAPI "artificial_analysis"@en .

#################################################################
#    BFO Bucket 2: Qualities - Performance & Cost Metrics
#################################################################

abi:MiniMax_Text_01 abi:inputTokenCost 0.2 ;
    abi:inputTokenCostCurrency "USD"@en ;
    abi:outputTokenCost 1.1 ;
    abi:outputTokenCostCurrency "USD"@en ;
    abi:blendedCost 0.425 ;
    abi:blendedCostCurrency "USD"@en ;
    abi:outputSpeed 34.064 ;
    abi:outputSpeedUnit "tokens_per_second"@en ;
    abi:timeToFirstToken 0.595 ;
    abi:timeToFirstTokenUnit "seconds"@en ;
    abi:timeToFirstAnswerToken 0.595 ;
    abi:timeToFirstAnswerTokenUnit "seconds"@en ;
    abi:intelligenceIndex 29.1 ;
    abi:codingIndex 24.8 ;
    abi:mathIndex 44.2 .

#################################################################
#    Relationships - Agent/Process/Model Network
#################################################################

abi:MinimaxAgent abi:canUtilizeModel abi:MiniMax_Text_01 .
abi:MinimaxBusinessProposalProcess abi:utilizesModel abi:MiniMax_Text_01 .
abi:MinimaxCreativeWritingProcess abi:utilizesModel abi:MiniMax_Text_01 .
abi:MinimaxCodeGenerationProcess abi:utilizesModel abi:MiniMax_Text_01 .

