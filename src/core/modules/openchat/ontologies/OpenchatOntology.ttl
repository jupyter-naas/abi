@prefix abi: <http://naas.ai/ontology/abi#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix capability: <http://ontology.naas.ai/abi/capability/> .
@prefix dc: <http://purl.org/dc/terms/> .

<http://naas.ai/ontology/abi/OpenchatOntology> a owl:Ontology ;
    owl:imports <http://ontology.naas.ai/abi/AIAgentOntology> ;
    dc:title "Openchat AI Agent Ontology"@en ;
    dc:description "BFO-grounded ontology for Openchat AI models and processes"@en ;
    dc:created "20250812T101446"@en .


#################################################################
#    BFO Bucket 1: Material Entities (WHAT/WHO)
#################################################################

abi:OpenchatAgent a abi:AIAgent ;
    rdfs:label "Openchat AI Agent"@en ;
    rdfs:comment "AI Agent capable of utilizing Openchat models"@en ;
    abi:hasSpecializedRole "Multi-purpose AI processing"@en .


#################################################################
#    BFO Bucket 4: Processes (HOW-IT-HAPPENS)
#################################################################

abi:OpenchatBusinessProposalProcess a abi:BusinessProposalCreationProcess ;
    rdfs:label "Openchat Business Proposal Process"@en ;
    abi:hasParticipant abi:OpenchatAgent ;
    abi:realizesCapability capability:TextGenerationCapability ;
    abi:hasTemporalRegion abi:OpenchatBusinessProposalSession ;
    abi:hasQuality abi:OpenchatBusinessProposalQuality .

abi:OpenchatCreativeWritingProcess a abi:CreativeWritingProcess ;
    rdfs:label "Openchat Creative Writing Process"@en ;
    abi:hasParticipant abi:OpenchatAgent ;
    abi:realizesCapability capability:TextGenerationCapability ;
    abi:hasTemporalRegion abi:OpenchatCreativeWritingSession ;
    abi:hasQuality abi:OpenchatCreativeWritingQuality .

abi:OpenchatCodeGenerationProcess a abi:CodeGenerationProcess ;
    rdfs:label "Openchat Code Generation Process"@en ;
    abi:hasParticipant abi:OpenchatAgent ;
    abi:realizesCapability capability:CodeGenerationCapability ;
    abi:hasTemporalRegion abi:OpenchatCodeGenerationSession ;
    abi:hasQuality abi:OpenchatCodeGenerationQuality .

#################################################################
#    BFO Bucket 5: Temporal Regions (WHEN)
#################################################################

abi:OpenchatBusinessProposalSession a abi:InferenceSession ;
    rdfs:label "Openchat Business Proposal Session"@en .

abi:OpenchatCreativeWritingSession a abi:InferenceSession ;
    rdfs:label "Openchat Creative Writing Session"@en .

abi:OpenchatCodeGenerationSession a abi:InferenceSession ;
    rdfs:label "Openchat Code Generation Session"@en .


#################################################################
#    BFO Bucket 1: Material Entity - OpenChat 3.5 (1210)
#################################################################

abi:OpenChat_3_5_1210 a abi:AIModelInstance ;
    rdfs:label "OpenChat 3.5 (1210)"@en ;
    abi:modelSlug "openchat-35"@en ;
    abi:provider "OpenChat"@en ;
    abi:sourceAPI "artificial_analysis"@en .

#################################################################
#    BFO Bucket 2: Qualities - Performance & Cost Metrics
#################################################################

abi:OpenChat_3_5_1210 abi:inputTokenCost 0 ;
    abi:inputTokenCostCurrency "USD"@en ;
    abi:outputTokenCost 0 ;
    abi:outputTokenCostCurrency "USD"@en ;
    abi:blendedCost 0 ;
    abi:blendedCostCurrency "USD"@en ;
    abi:outputSpeed 0 ;
    abi:outputSpeedUnit "tokens_per_second"@en ;
    abi:timeToFirstToken 0 ;
    abi:timeToFirstTokenUnit "seconds"@en ;
    abi:timeToFirstAnswerToken 0 ;
    abi:timeToFirstAnswerTokenUnit "seconds"@en ;
    abi:intelligenceIndex 7.7 ;
    abi:codingIndex 0 ;
    abi:mathIndex 15.4 .

#################################################################
#    Relationships - Agent/Process/Model Network
#################################################################

abi:OpenchatAgent abi:canUtilizeModel abi:OpenChat_3_5_1210 .
abi:OpenchatBusinessProposalProcess abi:utilizesModel abi:OpenChat_3_5_1210 .
abi:OpenchatCreativeWritingProcess abi:utilizesModel abi:OpenChat_3_5_1210 .
abi:OpenchatCodeGenerationProcess abi:utilizesModel abi:OpenChat_3_5_1210 .
