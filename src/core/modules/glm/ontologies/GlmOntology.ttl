@prefix abi: <http://naas.ai/ontology/abi#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix capability: <http://ontology.naas.ai/abi/capability/> .
@prefix dc: <http://purl.org/dc/terms/> .

<http://naas.ai/ontology/abi/GlmOntology> a owl:Ontology ;
    owl:imports <http://ontology.naas.ai/abi/AIAgentOntology> ;
    dc:title "Glm AI Agent Ontology"@en ;
    dc:description "BFO-grounded ontology for Glm AI models and processes"@en ;
    dc:created "20250812T101446"@en .


#################################################################
#    BFO Bucket 1: Material Entities (WHAT/WHO)
#################################################################

abi:GlmAgent a abi:AIAgent ;
    rdfs:label "Glm AI Agent"@en ;
    rdfs:comment "AI Agent capable of utilizing Glm models"@en ;
    abi:hasSpecializedRole "Multi-purpose AI processing"@en .


#################################################################
#    BFO Bucket 4: Processes (HOW-IT-HAPPENS)
#################################################################

abi:GlmBusinessProposalProcess a abi:BusinessProposalCreationProcess ;
    rdfs:label "Glm Business Proposal Process"@en ;
    abi:hasParticipant abi:GlmAgent ;
    abi:realizesCapability capability:TextGenerationCapability ;
    abi:hasTemporalRegion abi:GlmBusinessProposalSession ;
    abi:hasQuality abi:GlmBusinessProposalQuality .

abi:GlmCreativeWritingProcess a abi:CreativeWritingProcess ;
    rdfs:label "Glm Creative Writing Process"@en ;
    abi:hasParticipant abi:GlmAgent ;
    abi:realizesCapability capability:TextGenerationCapability ;
    abi:hasTemporalRegion abi:GlmCreativeWritingSession ;
    abi:hasQuality abi:GlmCreativeWritingQuality .

abi:GlmCodeGenerationProcess a abi:CodeGenerationProcess ;
    rdfs:label "Glm Code Generation Process"@en ;
    abi:hasParticipant abi:GlmAgent ;
    abi:realizesCapability capability:CodeGenerationCapability ;
    abi:hasTemporalRegion abi:GlmCodeGenerationSession ;
    abi:hasQuality abi:GlmCodeGenerationQuality .

#################################################################
#    BFO Bucket 5: Temporal Regions (WHEN)
#################################################################

abi:GlmBusinessProposalSession a abi:InferenceSession ;
    rdfs:label "Glm Business Proposal Session"@en .

abi:GlmCreativeWritingSession a abi:InferenceSession ;
    rdfs:label "Glm Creative Writing Session"@en .

abi:GlmCodeGenerationSession a abi:InferenceSession ;
    rdfs:label "Glm Code Generation Session"@en .


#################################################################
#    BFO Bucket 1: Material Entity - GLM-4.5
#################################################################

abi:GLM_4_5 a abi:AIModelInstance ;
    rdfs:label "GLM-4.5"@en ;
    abi:modelSlug "glm-4.5"@en ;
    abi:provider "Z AI"@en ;
    abi:sourceAPI "artificial_analysis"@en .

#################################################################
#    BFO Bucket 2: Qualities - Performance & Cost Metrics
#################################################################

abi:GLM_4_5 abi:inputTokenCost 0.57 ;
    abi:inputTokenCostCurrency "USD"@en ;
    abi:outputTokenCost 2.145 ;
    abi:outputTokenCostCurrency "USD"@en ;
    abi:blendedCost 0.964 ;
    abi:blendedCostCurrency "USD"@en ;
    abi:outputSpeed 61.745 ;
    abi:outputSpeedUnit "tokens_per_second"@en ;
    abi:timeToFirstToken 0.644 ;
    abi:timeToFirstTokenUnit "seconds"@en ;
    abi:timeToFirstAnswerToken 33.036 ;
    abi:timeToFirstAnswerTokenUnit "seconds"@en ;
    abi:intelligenceIndex 56.1 ;
    abi:codingIndex 54.3 ;
    abi:mathIndex 92.6 .

#################################################################
#    Relationships - Agent/Process/Model Network
#################################################################

abi:GlmAgent abi:canUtilizeModel abi:GLM_4_5 .
abi:GlmBusinessProposalProcess abi:utilizesModel abi:GLM_4_5 .
abi:GlmCreativeWritingProcess abi:utilizesModel abi:GLM_4_5 .
abi:GlmCodeGenerationProcess abi:utilizesModel abi:GLM_4_5 .

#################################################################
#    BFO Bucket 1: Material Entity - GLM-4.5-Air
#################################################################

abi:GLM_4_5_Air a abi:AIModelInstance ;
    rdfs:label "GLM-4.5-Air"@en ;
    abi:modelSlug "glm-4-5-air"@en ;
    abi:provider "Z AI"@en ;
    abi:sourceAPI "artificial_analysis"@en .

#################################################################
#    BFO Bucket 2: Qualities - Performance & Cost Metrics
#################################################################

abi:GLM_4_5_Air abi:inputTokenCost 0.2 ;
    abi:inputTokenCostCurrency "USD"@en ;
    abi:outputTokenCost 1.1 ;
    abi:outputTokenCostCurrency "USD"@en ;
    abi:blendedCost 0.425 ;
    abi:blendedCostCurrency "USD"@en ;
    abi:outputSpeed 179.877 ;
    abi:outputSpeedUnit "tokens_per_second"@en ;
    abi:timeToFirstToken 0.401 ;
    abi:timeToFirstTokenUnit "seconds"@en ;
    abi:timeToFirstAnswerToken 11.52 ;
    abi:timeToFirstAnswerTokenUnit "seconds"@en ;
    abi:intelligenceIndex 49.5 ;
    abi:codingIndex 49.5 ;
    abi:mathIndex 81.9 .

#################################################################
#    Relationships - Agent/Process/Model Network
#################################################################

abi:GlmAgent abi:canUtilizeModel abi:GLM_4_5_Air .
abi:GlmBusinessProposalProcess abi:utilizesModel abi:GLM_4_5_Air .
abi:GlmCreativeWritingProcess abi:utilizesModel abi:GLM_4_5_Air .
abi:GlmCodeGenerationProcess abi:utilizesModel abi:GLM_4_5_Air .
