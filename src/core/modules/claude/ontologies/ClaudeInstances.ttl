@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dc11: <http://purl.org/dc/elements/1.1/> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix cco: <https://www.commoncoreontologies.org/> .
@prefix abi: <http://ontology.naas.ai/abi/> .
@prefix claude: <http://ontology.naas.ai/abi/claude/> .

<http://ontology.naas.ai/abi/claude/ClaudeInstances> rdf:type owl:Ontology ;
    owl:imports <http://ontology.naas.ai/abi/AIAgentOntology> ;
    dc:title "Claude Module Instances" ;
    dc:description "Concrete instances demonstrating Claude AI agents with constitutional AI approach."@en .

#################################################################
#    Claude AI System (Main System)
#################################################################

claude:ClaudeSystem a abi:AISystem ;
    rdfs:label "Claude AI System"@en ;
    abi:providerName "Anthropic" ;
    abi:systemComplexity "complex" ;
    abi:hasLoadBalancer claude:ClaudeLoadBalancer ;
    abi:hasSubsystem claude:ConstitutionalAISubsystem, claude:ReasoningSubsystem, claude:SafetySubsystem ;
    rdfs:comment "Main Claude system that implements constitutional AI principles with specialized subsystems for reasoning and safety."@en .

#################################################################
#    Claude Subsystems
#################################################################

claude:ConstitutionalAISubsystem a abi:AISystem ;
    rdfs:label "Claude Constitutional AI Subsystem"@en ;
    abi:providerName "Anthropic" ;
    abi:systemComplexity "moderate" ;
    abi:isSubsystemOf claude:ClaudeSystem ;
    abi:hasAIAgent claude:Claude3SonnetAgent, claude:Claude3HaikuAgent ;
    rdfs:comment "Subsystem implementing constitutional AI principles with multiple Claude model variants."@en .

claude:ReasoningSubsystem a abi:AISystem ;
    rdfs:label "Claude Reasoning Subsystem"@en ;
    abi:providerName "Anthropic" ;
    abi:systemComplexity "moderate" ;
    abi:isSubsystemOf claude:ClaudeSystem ;
    abi:hasAIAgent claude:AdvancedReasoningAgent, claude:CodeReasoningAgent ;
    rdfs:comment "Subsystem dedicated to advanced reasoning and problem-solving tasks."@en .

claude:SafetySubsystem a abi:AISystem ;
    rdfs:label "Claude Safety Subsystem"@en ;
    abi:providerName "Anthropic" ;
    abi:systemComplexity "simple" ;
    abi:isSubsystemOf claude:ClaudeSystem ;
    abi:hasAIAgent claude:SafetyValidatorAgent ;
    rdfs:comment "Subsystem responsible for safety checks and constitutional AI compliance."@en .

#################################################################
#    Claude AI Agents
#################################################################

### Constitutional AI Agents
claude:Claude3SonnetAgent a abi:AIAgent ;
    rdfs:label "Claude 3 Sonnet Agent"@en ;
    abi:isAIAgentOf claude:ConstitutionalAISubsystem ;
    abi:hasSpecializedRole abi:PrimaryResponderRole ;
    abi:collaborationPriority "9" ;
    abi:collaboratesWith claude:AdvancedReasoningAgent, claude:SafetyValidatorAgent ;
    abi:hasFallbackAgent claude:Claude3HaikuAgent ;
    rdfs:comment "Primary Claude agent using Claude 3 Sonnet model with constitutional AI principles."@en .

claude:Claude3HaikuAgent a abi:AIAgent ;
    rdfs:label "Claude 3 Haiku Agent"@en ;
    abi:isAIAgentOf claude:ConstitutionalAISubsystem ;
    abi:hasSpecializedRole abi:FallbackHandlerRole ;
    abi:collaborationPriority "6" ;
    abi:collaboratesWith claude:Claude3SonnetAgent ;
    rdfs:comment "Fast Claude agent using Claude 3 Haiku model for quick responses."@en .

### Reasoning Agents
claude:AdvancedReasoningAgent a abi:AIAgent ;
    rdfs:label "Advanced Reasoning Agent"@en ;
    abi:isAIAgentOf claude:ReasoningSubsystem ;
    abi:hasSpecializedRole abi:SpecializedTaskExecutorRole ;
    abi:collaborationPriority "8" ;
    abi:collaboratesWith claude:Claude3SonnetAgent, claude:CodeReasoningAgent ;
    rdfs:comment "Specialized agent for complex reasoning and problem-solving tasks."@en .

claude:CodeReasoningAgent a abi:AIAgent ;
    rdfs:label "Code Reasoning Agent"@en ;
    abi:isAIAgentOf claude:ReasoningSubsystem ;
    abi:hasSpecializedRole abi:SpecializedTaskExecutorRole ;
    abi:collaborationPriority "7" ;
    abi:collaboratesWith claude:AdvancedReasoningAgent, claude:Claude3SonnetAgent ;
    rdfs:comment "Specialized agent for code analysis and programming tasks."@en .

### Safety Agent
claude:SafetyValidatorAgent a abi:AIAgent ;
    rdfs:label "Safety Validator Agent"@en ;
    abi:isAIAgentOf claude:SafetySubsystem ;
    abi:hasSpecializedRole abi:ValidatorRole ;
    abi:collaborationPriority "8" ;
    abi:collaboratesWith claude:Claude3SonnetAgent, claude:AdvancedReasoningAgent ;
    rdfs:comment "Safety validation agent that ensures constitutional AI compliance and ethical behavior."@en .

#################################################################
#    Load Balancer
#################################################################

claude:ClaudeLoadBalancer a abi:LoadBalancer ;
    rdfs:label "Claude Load Balancer"@en ;
    rdfs:comment "Intelligent load balancer that distributes requests across Claude agents based on task complexity and constitutional AI requirements."@en .

#################################################################
#    AI Model Instances
#################################################################

claude:Claude3SonnetModelInstance a abi:AIModelInstance ;
    rdfs:label "Claude 3 Sonnet Model Instance"@en ;
    abi:modelName "claude-3-sonnet" ;
    abi:providerName "Anthropic" ;
    abi:hasQuality claude:Claude3SonnetAccuracy, claude:Claude3SonnetLatency, claude:Claude3SonnetTokenCapacity ;
    rdfs:comment "Specific instance of Claude 3 Sonnet model running on Anthropic's infrastructure."@en .

claude:Claude3HaikuModelInstance a abi:AIModelInstance ;
    rdfs:label "Claude 3 Haiku Model Instance"@en ;
    abi:modelName "claude-3-haiku" ;
    abi:providerName "Anthropic" ;
    abi:hasQuality claude:Claude3HaikuAccuracy, claude:Claude3HaikuLatency, claude:Claude3HaikuTokenCapacity ;
    rdfs:comment "Specific instance of Claude 3 Haiku model running on Anthropic's infrastructure."@en .

#################################################################
#    Qualities
#################################################################

### Claude 3 Sonnet Qualities
claude:Claude3SonnetAccuracy a abi:ModelAccuracy ;
    rdfs:label "Claude 3 Sonnet Accuracy"@en ;
    abi:accuracyValue "0.93"^^xsd:decimal ;
    rdfs:comment "Claude 3 Sonnet model accuracy quality with 93% performance."@en .

claude:Claude3SonnetLatency a abi:ResponseLatency ;
    rdfs:label "Claude 3 Sonnet Latency"@en ;
    abi:latencyValue "1500"^^xsd:integer ;
    rdfs:comment "Claude 3 Sonnet response latency quality of 1500ms."@en .

claude:Claude3SonnetTokenCapacity a abi:TokenCapacity ;
    rdfs:label "Claude 3 Sonnet Token Capacity"@en ;
    abi:tokenLimit "200000"^^xsd:integer ;
    rdfs:comment "Claude 3 Sonnet token capacity quality of 200,000 tokens."@en .

### Claude 3 Haiku Qualities
claude:Claude3HaikuAccuracy a abi:ModelAccuracy ;
    rdfs:label "Claude 3 Haiku Accuracy"@en ;
    abi:accuracyValue "0.89"^^xsd:decimal ;
    rdfs:comment "Claude 3 Haiku model accuracy quality with 89% performance."@en .

claude:Claude3HaikuLatency a abi:ResponseLatency ;
    rdfs:label "Claude 3 Haiku Latency"@en ;
    abi:latencyValue "600"^^xsd:integer ;
    rdfs:comment "Claude 3 Haiku response latency quality of 600ms."@en .

claude:Claude3HaikuTokenCapacity a abi:TokenCapacity ;
    rdfs:label "Claude 3 Haiku Token Capacity"@en ;
    abi:tokenLimit "200000"^^xsd:integer ;
    rdfs:comment "Claude 3 Haiku token capacity quality of 200,000 tokens."@en .

#################################################################
#    Processes with Constitutional AI Workflow
#################################################################

claude:InputAnalysisProcess a abi:TextGenerationProcess ;
    rdfs:label "Input Analysis Process"@en ;
    abi:realizesCapability abi:TextGenerationCapability ;
    abi:hasParticipant claude:Claude3SonnetModelInstance ;
    abi:hasTemporalRegion claude:InputAnalysisSession ;
    abi:temporalSequence "1" ;
    abi:triggersProcess claude:ConstitutionalCheckProcess ;
    rdfs:comment "Process that analyzes user input for constitutional AI compliance."@en .

claude:ConstitutionalCheckProcess a abi:TextGenerationProcess ;
    rdfs:label "Constitutional Check Process"@en ;
    abi:realizesCapability abi:TextGenerationCapability ;
    abi:hasParticipant claude:Claude3SonnetModelInstance ;
    abi:hasTemporalRegion claude:ConstitutionalCheckSession ;
    abi:temporalSequence "2" ;
    abi:precedesInTime claude:ReasoningProcess ;
    abi:triggersProcess claude:ReasoningProcess ;
    rdfs:comment "Process that checks input against constitutional AI principles."@en .

claude:ReasoningProcess a abi:TextGenerationProcess ;
    rdfs:label "Reasoning Process"@en ;
    abi:realizesCapability abi:TextGenerationCapability ;
    abi:hasParticipant claude:Claude3SonnetModelInstance ;
    abi:hasTemporalRegion claude:ReasoningSession ;
    abi:temporalSequence "3" ;
    abi:precedesInTime claude:SafetyValidationProcess ;
    abi:triggersProcess claude:SafetyValidationProcess ;
    rdfs:comment "Process that performs advanced reasoning and problem-solving."@en .

claude:SafetyValidationProcess a abi:TextGenerationProcess ;
    rdfs:label "Safety Validation Process"@en ;
    abi:realizesCapability abi:TextGenerationCapability ;
    abi:hasParticipant claude:Claude3SonnetModelInstance ;
    abi:hasTemporalRegion claude:SafetyValidationSession ;
    abi:temporalSequence "4" ;
    abi:precedesInTime claude:ResponseGenerationProcess ;
    abi:triggersProcess claude:ResponseGenerationProcess ;
    rdfs:comment "Process that validates response for safety and constitutional AI compliance."@en .

claude:ResponseGenerationProcess a abi:TextGenerationProcess ;
    rdfs:label "Response Generation Process"@en ;
    abi:realizesCapability abi:TextGenerationCapability ;
    abi:hasParticipant claude:Claude3SonnetModelInstance ;
    abi:hasTemporalRegion claude:ResponseGenerationSession ;
    abi:temporalSequence "5" ;
    rdfs:comment "Process that generates the final constitutional AI-compliant response."@en .

#################################################################
#    Temporal Regions
#################################################################

claude:InputAnalysisSession a abi:InferenceSession ;
    rdfs:label "Input Analysis Session"@en ;
    rdfs:comment "Temporal region during which input analysis occurs."@en .

claude:ConstitutionalCheckSession a abi:InferenceSession ;
    rdfs:label "Constitutional Check Session"@en ;
    rdfs:comment "Temporal region during which constitutional AI checks occur."@en .

claude:ReasoningSession a abi:InferenceSession ;
    rdfs:label "Reasoning Session"@en ;
    rdfs:comment "Temporal region during which advanced reasoning occurs."@en .

claude:SafetyValidationSession a abi:InferenceSession ;
    rdfs:label "Safety Validation Session"@en ;
    rdfs:comment "Temporal region during which safety validation occurs."@en .

claude:ResponseGenerationSession a abi:InferenceSession ;
    rdfs:label "Response Generation Session"@en ;
    rdfs:comment "Temporal region during which response generation occurs."@en .

#################################################################
#    Information Content Entities
#################################################################

claude:Claude3SonnetSpecification a abi:ModelSpecification ;
    rdfs:label "Claude 3 Sonnet Model Specification"@en ;
    abi:specificationContent "Claude 3 Sonnet is a balanced model with strong reasoning capabilities and 200K context window, implementing constitutional AI principles." ;
    rdfs:comment "Information content describing Claude 3 Sonnet model parameters and capabilities."@en .

claude:Claude3HaikuSpecification a abi:ModelSpecification ;
    rdfs:label "Claude 3 Haiku Model Specification"@en ;
    abi:specificationContent "Claude 3 Haiku is a fast and cost-effective model with 200K context window, optimized for speed while maintaining constitutional AI principles." ;
    rdfs:comment "Information content describing Claude 3 Haiku model parameters and capabilities."@en .

claude:ConstitutionalAIPrinciples a abi:ModelSpecification ;
    rdfs:label "Constitutional AI Principles"@en ;
    abi:specificationContent "Constitutional AI principles include helpfulness, harmlessness, and honesty, ensuring Claude agents behave ethically and safely." ;
    rdfs:comment "Information content describing the constitutional AI principles that guide Claude behavior."@en .
