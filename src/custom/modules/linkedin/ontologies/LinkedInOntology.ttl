    skos:example "John Doe's LinkedIn profile page has properties such as Profile, Education, Position, etc." ;
    abi:is_curated_in_foundry "enterprise_management_foundry", "personal_ai_foundry" .

abi:LinkedInProfile a owl:Class ;
    rdfs:subClassOf abi:LinkedInProperty ;
    rdfs:label "LinkedIn Profile" ;
    skos:definition "Represents a profile of a LinkedIn entity stored in type 'com.linkedin.voyager.identity.profile.Profile'." ;
    skos:example "John Doe's Profile included entityUrn, firstName, lastName, birthDate, headline, summary, etc." ;
    abi:is_curated_in_foundry "enterprise_management_foundry", "personal_ai_foundry" .

