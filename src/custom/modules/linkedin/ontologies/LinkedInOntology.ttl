#################################################################
#    Object Properties
#################################################################

linkedin:hasSocialPage a owl:ObjectProperty ;
    rdfs:domain [ rdf:type owl:Class ;
                  owl:unionOf ( cco:ont00001180
                               cco:ont00001262
                             )
                ] ;
    rdfs:range linkedin:SocialPage ;
    rdfs:label "has social page"@en ;
    skos:definition "Relates an person or organization to its social media pages."@en ;
    skos:example "John Doe's social media pages on various platforms like LinkedIn, Twitter, etc."@en ;
    owl:inverseOf linkedin:isSocialPageOf .

linkedin:isSocialPageOf a owl:ObjectProperty ;
    rdfs:domain linkedin:SocialPage ;
    rdfs:range [ rdf:type owl:Class ;
                 owl:unionOf ( cco:ont00001180
                              cco:ont00001262
                            )
               ] ;
    rdfs:label "is social page of"@en ;
    skos:definition "Relates a social media page to the person or organization it represents."@en ;
    skos:example "A social media page that represents John Doe's online presence."@en ;
    owl:inverseOf linkedin:hasSocialPage .

linkedin:hasProfile a owl:ObjectProperty ;
    rdfs:domain linkedin:LinkedInProfilePage ;
    rdfs:range linkedin:LinkedInProfile ;
    rdfs:label "has profile"@en ;
    skos:definition "Relates a LinkedIn profile page to its profile."@en ;
    skos:example "John Doe's LinkedIn profile page has a profile with a first name, last name, etc."@en ;
    owl:inverseOf linkedin:isProfileOf .

linkedin:isProfileOf a owl:ObjectProperty ;
    rdfs:domain linkedin:LinkedInProfile ;
    rdfs:range linkedin:LinkedInProfilePage ;
    rdfs:label "is profile of"@en ;
    skos:definition "Relates a LinkedIn profile to the LinkedIn profile page it belongs to."@en ;
    skos:example "John Doe's profile is the profile of his LinkedIn profile page."@en ;
    owl:inverseOf linkedin:hasProfile .

