<http://ontology.naas.ai/abi/linkedin#description> a owl:DatatypeProperty ;
    rdfs:label "Description"@en ;
    rdfs:domain <http://ontology.naas.ai/abi/linkedin#LinkedInPage> ;
    rdfs:range xsd:string ;
    skos:definition "The description of the entity."@en ;
