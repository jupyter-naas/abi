    rdfs:subClassOf cco1:ont00001180 ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( cco1:ont00001180 [ a owl:Restriction ;
                        owl:onProperty bfo1:BFO_0000196 ;
                        owl:someValuesFrom cco1:ont00000485 ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( cco1:ont00001180 [ a owl:Restriction ;
                        owl:onProperty bfo1:BFO_0000196 ;
