@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dc11: <http://purl.org/dc/elements/1.1/> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix cco: <https://www.commoncoreontologies.org/> .
@prefix abi: <http://ontology.naas.ai/abi/> .

<http://ontology.naas.ai/abi/EventsOntology> rdf:type owl:Ontology ;
    owl:imports <https://www.commoncoreontologies.org/EventOntology> ;
    owl:versionIRI <https://github.com/jupyter-naas/abi/tree/cli/src/ontologies/domain-level/EventsOntology.ttl> ;
    dc11:contributor "Jeremy Ravenel" , "Maxime Jublou" , "Florent Ravenel" ;
    dc:description "Domain ontology for events and occurrences."@en ;
    dc:license "" ;
    dc:title "Event Ontology" .

#################################################################
#    Classes
#################################################################

abi:Event a owl:Class ;
    rdfs:label "Event"@en ;
    rdfs:subClassOf cco:ont00000821 ; # Processual Entity
    skos:definition "A processual entity that happens, occurs, takes place, or is performed." .

abi:EventType a owl:Class ;
    rdfs:label "Event Type"@en .

# Main Event Categories
abi:UserInteractionEvent a owl:Class ;
    rdfs:label "User Interaction Event"@en ;
    rdfs:subClassOf abi:Event ;
    skos:definition "Events that involve direct interaction between users and the AI system." .

abi:SystemEvent a owl:Class ;
    rdfs:label "System Event"@en ;
    rdfs:subClassOf abi:Event ;
    skos:definition "Events related to system operations and maintenance." .

abi:DataProcessingEvent a owl:Class ;
    rdfs:label "Data Processing Event"@en ;
    rdfs:subClassOf abi:Event ;
    skos:definition "Events related to data handling and processing operations." .

abi:DecisionMakingEvent a owl:Class ;
    rdfs:label "Decision Making Event"@en ;
    rdfs:subClassOf abi:Event ;
    skos:definition "Events involving AI system making decisions or inferences." .

abi:CommunicationEvent a owl:Class ;
    rdfs:label "Communication Event"@en ;
    rdfs:subClassOf abi:Event ;
    skos:definition "Events related to system communication and integration." .

abi:SecurityEvent a owl:Class ;
    rdfs:label "Security Event"@en ;
    rdfs:subClassOf abi:Event ;
    skos:definition "Events related to security and compliance." .

abi:LearningEvent a owl:Class ;
    rdfs:label "Learning Event"@en ;
    rdfs:subClassOf abi:Event ;
    skos:definition "Events related to system learning and adaptation." .

#################################################################
#    Object Properties
#################################################################

abi:hasType a owl:ObjectProperty ;
    rdfs:domain abi:Event ;
    rdfs:range abi:EventType .

abi:relatedToEvent a owl:ObjectProperty ;
    rdfs:domain abi:Event ;
    rdfs:range abi:Event .

#################################################################
#    Data Properties
#################################################################

abi:hasTitle a owl:DatatypeProperty ;
    rdfs:domain abi:Event ;
    rdfs:range xsd:string .

abi:hasTracker a owl:DatatypeProperty ;
    rdfs:domain abi:Event ;
    rdfs:range xsd:string .

abi:hasContent a owl:DatatypeProperty ;
    rdfs:domain abi:Event ;
    rdfs:range xsd:string .

abi:hasValue a owl:DatatypeProperty ;
    rdfs:domain abi:Event ;
    rdfs:range xsd:decimal .

abi:hasUnit a owl:DatatypeProperty ;
    rdfs:domain abi:Event ;
    rdfs:range xsd:string .

#################################################################
#    Individuals
#################################################################

# User Interaction Event Types
abi:InputEvent a abi:EventType ;
    skos:example "User enters search query 'machine learning tutorials'"@en ,
                 "User uploads a CSV file for analysis"@en .

abi:ClickEvent a abi:EventType ;
    skos:example "User clicks 'Run Analysis' button"@en ,
                 "User selects dropdown menu option"@en .

abi:GestureEvent a abi:EventType ;
    skos:example "User swipes left to dismiss notification"@en ,
                 "User pinch-zooms on data visualization"@en .

abi:SpeechEvent a abi:EventType ;
    skos:example "User says 'Hey AI, start new analysis'"@en ,
                 "Voice command to export results"@en .

# System Event Types
abi:StartupEvent a abi:EventType ;
    skos:example "AI system initializes at 2024-03-20T08:00:00Z"@en ,
                 "Model loading completed successfully"@en .

abi:ShutdownEvent a abi:EventType ;
    skos:example "System graceful shutdown after task completion"@en ,
                 "Emergency shutdown due to critical error"@en .

abi:ErrorEvent a abi:EventType ;
    skos:example "Database connection timeout at 15:30:45"@en ,
                 "Invalid input format detected in data pipeline"@en .

abi:LoggingEvent a abi:EventType ;
    skos:example "User session duration: 45 minutes"@en ,
                 "API endpoint response time: 200ms"@en .

abi:PerformanceEvent a abi:EventType ;
    skos:example "CPU usage peaked at 85% during model training"@en ,
                 "Memory consumption reached 4GB threshold"@en .

# Data Processing Event Types
abi:DataIngestionEvent a abi:EventType ;
    skos:example "Received 1000 new customer records from CRM"@en ,
                 "Started importing Twitter feed data"@en .

abi:DataPreprocessingEvent a abi:EventType ;
