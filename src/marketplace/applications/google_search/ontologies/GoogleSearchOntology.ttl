@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dc11: <http://purl.org/dc/elements/1.1/> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix cco: <https://www.commoncoreontologies.org/> .
@prefix abi: <http://ontology.naas.ai/abi/> .

<http://ontology.naas.ai/abi/google-search.ttl> rdf:type owl:Ontology ;
    owl:imports <> ;
    owl:versionIRI <https://github.com/jupyter-naas/abi/tree/cli/src/ontologies/application-level/google-search.ttl> ;
    dc11:contributor "Jeremy Ravenel" , "Maxime Jublou" , "Florent Ravenel" ;
    dc:description "Application ontology for Google Search."@en ;
    dc:license "" ;
    dc:title "Google Search Application Ontology" .

#################################################################
#    Classes
#################################################################

abi:ActofSearchinginGoogle a owl:Class ;
    rdfs:subClassOf cco:ont00000228 ; # Planned Act
    rdfs:label "Act of Searching in Google" ;
    skos:definition "A Planned Act of searching for information using the Google search engine." ;
    skos:example "Searching for 'how to make pasta' on Google." .

#################################################################
#    Object Properties
#################################################################

abi:hasGoogleSearchResult a owl:ObjectProperty ;
    rdfs:subPropertyOf bfo:BFO_0000058 ; # is concretized by
    rdfs:domain abi:ActofSearchinginGoogle ;
    owl:inverseOf abi:isGoogleSearchResultOf ;
    rdfs:label "has google search result"@en ;
    skos:definition "Relates a Google search act to the entities found in the search results."@en ;
    skos:example "A Google search for 'Microsoft' returns the Microsoft company website and Wikipedia page."@en .

abi:isGoogleSearchResultOf a owl:ObjectProperty ;
    rdfs:subPropertyOf bfo:BFO_0000059 ; # concretizes
    rdfs:range abi:ActofSearchinginGoogle ;
    owl:inverseOf abi:hasGoogleSearchResult ;
    rdfs:label "is google search result of"@en ;
    skos:definition "Relates entities found in search results to the Google search act that discovered them."@en ;
    skos:example "The Microsoft company website is a search result of a Google search for 'Microsoft'."@en .

#################################################################
#    Data properties
#################################################################

abi:query a owl:DatatypeProperty ;
    rdfs:domain abi:ActofSearchinginGoogle ;
    rdfs:range xsd:string ;
    rdfs:label "Query" ;
    skos:definition "The query of the Google search." ;
    skos:example "Searching for 'how to make pasta' on Google." .

abi:pattern a owl:DatatypeProperty ;
    rdfs:domain abi:ActofSearchinginGoogle ;
    rdfs:range xsd:string ;
    rdfs:label "Pattern" ;
    skos:definition "The pattern of the Google search." ;
    skos:example "Searching for 'how to make pasta' on Google." .
