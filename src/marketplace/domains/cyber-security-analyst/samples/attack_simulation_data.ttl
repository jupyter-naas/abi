# =============================================================================
# ATTACK SIMULATION DATA - Realistic Cyber Attack Scenarios
# =============================================================================
# This file contains instances representing real-world cyber attacks including:
# - Stuxnet (Industrial Control System attack)
# - SolarStorm (Supply Chain attack) 
# - HeartBleed (OpenSSL vulnerability)
# - APT29 (Russian state-sponsored group)
# - Lazarus Group (North Korean state-sponsored group)
# =============================================================================

# =============================================================================
# AGENTS (Attackers, Systems, Users)
# =============================================================================

# Human Agents
ex:stuxnet_operator a :Agent ;
    rdfs:label "Stuxnet Operator" ;
    :hasRole "Malicious Actor" .

ex:solarstorm_operator a :Agent ;
    rdfs:label "SolarStorm Operator" ;
    :hasRole "Supply Chain Attacker" .

ex:apt29_operator a :Agent ;
    rdfs:label "APT29 Operator" ;
    :hasRole "State-Sponsored Attacker" .

ex:lazarus_operator a :Agent ;
    rdfs:label "Lazarus Group Operator" ;
    :hasRole "Financial Crime Attacker" .

ex:system_admin a :Agent ;
    rdfs:label "System Administrator" ;
    :hasRole "Privileged User" .

ex:regular_user a :Agent ;
    rdfs:label "Regular User" ;
    :hasRole "Standard User" .

# System Agents
ex:stuxnet_worm a :Agent ;
    rdfs:label "Stuxnet Worm" ;
    :hasRole "Malicious Software" .

ex:solarstorm_malware a :Agent ;
