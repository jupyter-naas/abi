# =============================================================================
# OFFENSIVE TECHNIQUES AND TACTICS
# =============================================================================

# Techniques
ex:spear_phishing a :OffensiveTechnique ;
    rdfs:label "Spear Phishing" ;
    :definition "Targeted phishing attacks against specific individuals" .

ex:supply_chain_compromise a :OffensiveTechnique ;
    rdfs:label "Supply Chain Compromise" ;
    :definition "Compromising software supply chain to distribute malware" .

ex:buffer_overflow a :OffensiveTechnique ;
    rdfs:label "Buffer Overflow" ;
    :definition "Exploiting buffer overflow vulnerabilities" .

ex:privilege_escalation a :OffensiveTechnique ;
    rdfs:label "Privilege Escalation" ;
    :definition "Gaining higher privileges on a system" .

ex:lateral_movement a :OffensiveTechnique ;
    rdfs:label "Lateral Movement" ;
    :definition "Moving through network after initial compromise" .

ex:data_exfiltration a :OffensiveTechnique ;
    rdfs:label "Data Exfiltration" ;
    :definition "Stealing sensitive data from target systems" .

ex:ransomware_deployment a :OffensiveTechnique ;
    rdfs:label "Ransomware Deployment" ;
    :definition "Deploying ransomware to encrypt victim files" .

# Tactics
ex:initial_access a :OffensiveTactic ;
    rdfs:label "Initial Access" ;
    :definition "Gaining initial foothold in target environment" .

ex:persistence a :OffensiveTactic ;
    rdfs:label "Persistence" ;
    :definition "Maintaining access to compromised systems" .

ex:defense_evasion a :OffensiveTactic ;
    rdfs:label "Defense Evasion" ;
    :definition "Avoiding detection by security systems" .

ex:credential_access a :OffensiveTactic ;
    rdfs:label "Credential Access" ;
    :definition "Stealing or compromising user credentials" .

ex:impact a :OffensiveTactic ;
    rdfs:label "Impact" ;
    :definition "Causing damage to target systems or data" .

# Technique-Tactic Relationships
ex:spear_phishing :enables ex:initial_access .
ex:supply_chain_compromise :enables ex:initial_access .
ex:buffer_overflow :enables ex:initial_access .
ex:privilege_escalation :enables ex:persistence .
ex:lateral_movement :enables ex:credential_access .
ex:data_exfiltration :enables ex:impact .
ex:ransomware_deployment :enables ex:impact .

# =============================================================================
# OFFENSIVE ACTIONS (Attack Stages)
# =============================================================================

# Stuxnet Attack Actions
ex:stuxnet_initial_infection a :OffensiveAction ;
    rdfs:label "Stuxnet Initial Infection" ;
    :implements ex:supply_chain_compromise ;
    :hasParticipant ex:stuxnet_operator ;
    :hasParticipant ex:stuxnet_payload ;
    :produces ex:stuxnet_driver ;
    :causes ex:stuxnet_file_creation_event .

ex:stuxnet_persistence a :OffensiveAction ;
    rdfs:label "Stuxnet Persistence" ;
    :implements ex:privilege_escalation ;
    :hasParticipant ex:stuxnet_worm ;
    :hasParticipant ex:stuxnet_driver ;
    :produces ex:stuxnet_config ;
    :causes ex:stuxnet_registry_modification_event .

ex:stuxnet_ics_attack a :OffensiveAction ;
    rdfs:label "Stuxnet ICS Attack" ;
    :implements ex:data_exfiltration ;
    :hasParticipant ex:stuxnet_worm ;
    :hasParticipant ex:stuxnet_config ;
    :produces ex:stuxnet_log ;
    :causes ex:stuxnet_network_communication_event .

# SolarStorm Attack Actions
ex:solarstorm_initial_compromise a :OffensiveAction ;
    rdfs:label "SolarStorm Initial Compromise" ;
    :implements ex:supply_chain_compromise ;
    :hasParticipant ex:solarstorm_operator ;
    :hasParticipant ex:solarstorm_backdoor ;
    :produces ex:solarstorm_config ;
    :causes ex:solarstorm_file_creation_event .

ex:solarstorm_credential_theft a :OffensiveAction ;
    rdfs:label "SolarStorm Credential Theft" ;
    :implements ex:credential_access ;
    :hasParticipant ex:solarstorm_malware ;
    :hasParticipant ex:solarstorm_config ;
    :produces ex:solarstorm_credentials ;
    :causes ex:solarstorm_network_communication_event .

# HeartBleed Attack Actions
ex:heartbleed_exploitation a :OffensiveAction ;
    rdfs:label "HeartBleed Exploitation" ;
    :implements ex:buffer_overflow ;
    :hasParticipant ex:heartbleed_exploit ;
    :hasParticipant ex:heartbleed_exploit_code ;
    :produces ex:stolen_private_key ;
    :causes ex:heartbleed_network_communication_event .

ex:heartbleed_certificate_theft a :OffensiveAction ;
    rdfs:label "HeartBleed Certificate Theft" ;
    :implements ex:data_exfiltration ;
    :hasParticipant ex:heartbleed_exploit ;
    :hasParticipant ex:stolen_private_key ;
    :produces ex:stolen_certificate ;
    :causes ex:heartbleed_file_access_event .

# APT29 Attack Actions
ex:apt29_phishing_campaign a :OffensiveAction ;
    rdfs:label "APT29 Phishing Campaign" ;
    :implements ex:spear_phishing ;
    :hasParticipant ex:apt29_operator ;
    :hasParticipant ex:apt29_phishing_email ;
    :produces ex:apt29_macro_doc ;
    :causes ex:apt29_email_send_event .

ex:apt29_lateral_movement a :OffensiveAction ;
    rdfs:label "APT29 Lateral Movement" ;
    :implements ex:lateral_movement ;
    :hasParticipant ex:apt29_toolkit ;
    :hasParticipant ex:apt29_powershell_script ;
    :produces ex:system_logs ;
    :causes ex:apt29_network_connection_event .

# Lazarus Group Attack Actions
ex:lazarus_ransomware_deployment a :OffensiveAction ;
    rdfs:label "Lazarus Ransomware Deployment" ;
    :implements ex:ransomware_deployment ;
    :hasParticipant ex:lazarus_operator ;
    :hasParticipant ex:lazarus_ransomware_binary ;
    :produces ex:lazarus_encrypted_files ;
    :causes ex:lazarus_file_encryption_event .

ex:lazarus_ransom_demand a :OffensiveAction ;
    rdfs:label "Lazarus Ransom Demand" ;
    :implements ex:impact ;
    :hasParticipant ex:lazarus_ransomware ;
    :hasParticipant ex:lazarus_encrypted_files ;
    :produces ex:lazarus_ransom_note ;
    :causes ex:lazarus_file_creation_event .

