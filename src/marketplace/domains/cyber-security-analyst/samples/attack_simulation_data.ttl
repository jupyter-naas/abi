# =============================================================================
# TEMPORAL RELATIONSHIPS (Attack Chain Sequences)
# =============================================================================

# Stuxnet Attack Chain
ex:stuxnet_initial_infection bfo:precedes ex:stuxnet_persistence .
ex:stuxnet_persistence bfo:precedes ex:stuxnet_ics_attack .

ex:stuxnet_file_creation_event bfo:precedes ex:stuxnet_registry_modification_event .
ex:stuxnet_registry_modification_event bfo:precedes ex:stuxnet_network_communication_event .

# SolarStorm Attack Chain
ex:solarstorm_initial_compromise bfo:precedes ex:solarstorm_credential_theft .

ex:solarstorm_file_creation_event bfo:precedes ex:solarstorm_network_communication_event .

# HeartBleed Attack Chain
ex:heartbleed_exploitation bfo:precedes ex:heartbleed_certificate_theft .

ex:heartbleed_network_communication_event bfo:precedes ex:heartbleed_file_access_event .

# APT29 Attack Chain
ex:apt29_phishing_campaign bfo:precedes ex:apt29_lateral_movement .

ex:apt29_email_send_event bfo:precedes ex:apt29_network_connection_event .

# Lazarus Group Attack Chain
ex:lazarus_ransomware_deployment bfo:precedes ex:lazarus_ransom_demand .

ex:lazarus_file_encryption_event bfo:precedes ex:lazarus_file_creation_event .

# =============================================================================
