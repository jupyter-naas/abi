        owl:NamedIndividual ;
    rdfs:label "File Format Verification" ;
    :d3fend-id "D3-FFV" ;
    :definition "Verifying that a file conforms to its expected format specifications" ;
    :kb-reference :Reference-FileSecurityUsingFileFormatValidation_OPSWATInc ;
    rdfs:subClassOf [ ],
        :ContentValidation .

