# ðŸš§ NOT FUNCTIONAL YET - Ontology Template
# Project Management Methodologies Ontology
# Defines project management methodologies, practices, and relationships

@prefix : <http://naas.ai/ontology/project-management/methodologies#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Ontology Declaration
: a owl:Ontology ;
    rdfs:label "Project Management Methodologies Ontology" ;
    rdfs:comment "ðŸš§ NOT FUNCTIONAL YET - Defines project management methodologies and practices" .

# Classes
:Methodology a owl:Class ;
    rdfs:label "Project Management Methodology" ;
    rdfs:comment "A systematic approach to managing projects" .

:Practice a owl:Class ;
    rdfs:label "Project Management Practice" ;
    rdfs:comment "A specific technique or activity in project management" .

:Artifact a owl:Class ;
    rdfs:label "Project Artifact" ;
    rdfs:comment "A deliverable or document produced during project execution" .

:Role a owl:Class ;
    rdfs:label "Project Role" ;
    rdfs:comment "A defined responsibility or position in a project" .

:Phase a owl:Class ;
    rdfs:label "Project Phase" ;
    rdfs:comment "A distinct stage in the project lifecycle" .

# Properties
:usesPractice a owl:ObjectProperty ;
    rdfs:domain :Methodology ;
    rdfs:range :Practice ;
    rdfs:label "uses practice" .

:producesArtifact a owl:ObjectProperty ;
    rdfs:domain :Practice ;
    rdfs:range :Artifact ;
    rdfs:label "produces artifact" .

:hasRole a owl:ObjectProperty ;
    rdfs:domain :Methodology ;
    rdfs:range :Role ;
    rdfs:label "has role" .

:hasPhase a owl:ObjectProperty ;
    rdfs:domain :Methodology ;
    rdfs:range :Phase ;
    rdfs:label "has phase" .

:complexity a owl:DatatypeProperty ;
    rdfs:domain :Methodology ;
    rdfs:range xsd:string ;
    rdfs:label "complexity level" .

:teamSize a owl:DatatypeProperty ;
    rdfs:domain :Methodology ;
    rdfs:range xsd:string ;
    rdfs:label "recommended team size" .

# Methodology Instances
:Agile a :Methodology ;
    rdfs:label "Agile" ;
    :usesPractice :SprintPlanning, :DailyStandup, :Retrospective ;
    :hasRole :ScrumMaster, :ProductOwner, :DeveloperTeam ;
    :hasPhase :Sprint ;
    :complexity "Medium" ;
    :teamSize "5-9 people" .

:Scrum a :Methodology ;
    rdfs:label "Scrum" ;
    :usesPractice :SprintPlanning, :DailyStandup, :SprintReview, :Retrospective ;
    :hasRole :ScrumMaster, :ProductOwner, :DeveloperTeam ;
    :hasPhase :Sprint ;
    :complexity "Medium" ;
    :teamSize "5-9 people" .

:Waterfall a :Methodology ;
    rdfs:label "Waterfall" ;
    :usesPractice :RequirementsAnalysis, :SystemDesign, :Implementation, :Testing ;
    :hasRole :ProjectManager, :BusinessAnalyst, :Developer, :Tester ;
    :hasPhase :Requirements, :Design, :Implementation, :Testing, :Deployment ;
    :complexity "Low" ;
    :teamSize "Any size" .

:Kanban a :Methodology ;
    rdfs:label "Kanban" ;
    :usesPractice :VisualWorkflow, :WorkInProgressLimits, :ContinuousImprovement ;
    :hasRole :TeamMember, :TeamLead ;
    :complexity "Low" ;
    :teamSize "Any size" .

# Practice Instances
:SprintPlanning a :Practice ;
    rdfs:label "Sprint Planning" ;
    :producesArtifact :SprintBacklog .

:DailyStandup a :Practice ;
    rdfs:label "Daily Standup" ;
    rdfs:comment "Daily synchronization meeting" .

:Retrospective a :Practice ;
    rdfs:label "Retrospective" ;
    :producesArtifact :ActionItems .

# Artifact Instances
:SprintBacklog a :Artifact ;
    rdfs:label "Sprint Backlog" ;
    rdfs:comment "List of work items for a sprint" .

:ActionItems a :Artifact ;
    rdfs:label "Action Items" ;
    rdfs:comment "Improvement actions from retrospective" .
