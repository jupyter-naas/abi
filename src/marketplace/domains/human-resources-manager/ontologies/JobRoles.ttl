# Ontology Declaration
: a owl:Ontology ;
    rdfs:label "JobRoles Ontology" ;
    rdfs:comment "ðŸš§ NOT FUNCTIONAL YET - Job roles, responsibilities, and hierarchies ontology" .

# Core Classes
:JobRolesEntity a owl:Class ;
    rdfs:label "JobRoles Entity" ;
    rdfs:comment "Base class for jobroles entities" .

:JobRolesProcess a owl:Class ;
    rdfs:label "JobRoles Process" ;
    rdfs:comment "Processes related to jobroles" .

:JobRolesMetric a owl:Class ;
    rdfs:label "JobRoles Metric" ;
    rdfs:comment "Metrics and measurements for jobroles" .

# Core Properties
:hasType a owl:ObjectProperty ;
    rdfs:domain :JobRolesEntity ;
    rdfs:range xsd:string ;
    rdfs:label "has type" .

:hasValue a owl:DatatypeProperty ;
    rdfs:domain :JobRolesMetric ;
    rdfs:range xsd:decimal ;
    rdfs:label "has value" .

:relatedTo a owl:ObjectProperty ;
    rdfs:domain :JobRolesEntity ;
