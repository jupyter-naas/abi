@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Ontology Declaration
: a owl:Ontology ;
    rdfs:label "RiskFactors Ontology" ;
    rdfs:comment "ðŸš§ NOT FUNCTIONAL YET - Financial risk factors and assessment ontology" .

# Core Classes
:RiskFactorsEntity a owl:Class ;
    rdfs:label "RiskFactors Entity" ;
    rdfs:comment "Base class for riskfactors entities" .

# Core Properties
:hasType a owl:ObjectProperty ;
    rdfs:domain :RiskFactorsEntity ;
    rdfs:range xsd:string ;
    rdfs:label "has type" .

