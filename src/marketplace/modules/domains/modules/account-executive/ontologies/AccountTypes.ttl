@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Ontology Declaration
: a owl:Ontology ;
    rdfs:label "AccountTypes Ontology" ;
    rdfs:comment "ðŸš§ NOT FUNCTIONAL YET - Account types and classification ontology" .

# Core Classes
:AccountTypesEntity a owl:Class ;
    rdfs:label "AccountTypes Entity" ;
    rdfs:comment "Base class for accounttypes entities" .

# Core Properties
:hasType a owl:ObjectProperty ;
    rdfs:domain :AccountTypesEntity ;
    rdfs:range xsd:string ;
    rdfs:label "has type" .

