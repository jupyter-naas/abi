@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Ontology Declaration
: a owl:Ontology ;
    rdfs:label "InvestigationTypes Ontology" ;
    rdfs:comment "ðŸš§ NOT FUNCTIONAL YET - Investigation types and methodologies ontology" .

# Core Classes
:InvestigationTypesEntity a owl:Class ;
    rdfs:label "InvestigationTypes Entity" ;
    rdfs:comment "Base class for investigationtypes entities" .

:InvestigationTypesProcess a owl:Class ;
    rdfs:label "InvestigationTypes Process" ;
    rdfs:comment "Processes related to investigationtypes" .

:InvestigationTypesMetric a owl:Class ;
    rdfs:label "InvestigationTypes Metric" ;
    rdfs:comment "Metrics and measurements for investigationtypes" .

# Core Properties
:hasType a owl:ObjectProperty ;
    rdfs:domain :InvestigationTypesEntity ;
    rdfs:range xsd:string ;
    rdfs:label "has type" .

:hasValue a owl:DatatypeProperty ;
    rdfs:domain :InvestigationTypesMetric ;
    rdfs:range xsd:decimal ;
    rdfs:label "has value" .

:relatedTo a owl:ObjectProperty ;
    rdfs:domain :InvestigationTypesEntity ;
    rdfs:range :InvestigationTypesEntity ;
    rdfs:label "related to" .

