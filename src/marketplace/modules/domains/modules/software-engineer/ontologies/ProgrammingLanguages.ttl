# ðŸš§ NOT FUNCTIONAL YET - Ontology Template
# Programming Languages Ontology for Software Engineering Domain
# Defines programming languages, their characteristics, and relationships

@prefix : <http://naas.ai/ontology/software-engineering/programming-languages#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Ontology Declaration
: a owl:Ontology ;
    rdfs:label "Programming Languages Ontology" ;
    rdfs:comment "ðŸš§ NOT FUNCTIONAL YET - Defines programming languages and their characteristics" .

# Classes
:ProgrammingLanguage a owl:Class ;
    rdfs:label "Programming Language" ;
    rdfs:comment "A formal language for creating computer programs" .

:Paradigm a owl:Class ;
    rdfs:label "Programming Paradigm" ;
    rdfs:comment "A style or approach to programming" .

:Framework a owl:Class ;
    rdfs:label "Framework" ;
    rdfs:comment "A software framework for a programming language" .

:Library a owl:Class ;
    rdfs:label "Library" ;
    rdfs:comment "A collection of pre-written code for a programming language" .

# Properties
:hasParadigm a owl:ObjectProperty ;
    rdfs:domain :ProgrammingLanguage ;
    rdfs:range :Paradigm ;
    rdfs:label "has paradigm" .

:hasFramework a owl:ObjectProperty ;
    rdfs:domain :ProgrammingLanguage ;
    rdfs:range :Framework ;
    rdfs:label "has framework" .

:hasLibrary a owl:ObjectProperty ;
    rdfs:domain :ProgrammingLanguage ;
    rdfs:range :Library ;
    rdfs:label "has library" .

:version a owl:DatatypeProperty ;
    rdfs:domain :ProgrammingLanguage ;
    rdfs:range xsd:string ;
    rdfs:label "version" .

:performanceRating a owl:DatatypeProperty ;
    rdfs:domain :ProgrammingLanguage ;
    rdfs:range xsd:integer ;
    rdfs:label "performance rating" .

# Programming Language Instances
:Python a :ProgrammingLanguage ;
    rdfs:label "Python" ;
    :hasParadigm :ObjectOriented, :Functional, :Procedural ;
    :hasFramework :Django, :Flask, :FastAPI ;
    :hasLibrary :NumPy, :Pandas, :Requests ;
    :version "3.12" ;
    :performanceRating 7 .

:JavaScript a :ProgrammingLanguage ;
    rdfs:label "JavaScript" ;
    :hasParadigm :ObjectOriented, :Functional, :EventDriven ;
    :hasFramework :React, :Vue, :Angular ;
    :hasLibrary :Lodash, :Axios, :Moment ;
    :version "ES2023" ;
    :performanceRating 8 .

:TypeScript a :ProgrammingLanguage ;
    rdfs:label "TypeScript" ;
    :hasParadigm :ObjectOriented, :Functional ;
    :hasFramework :React, :Angular, :NestJS ;
    :version "5.0" ;
    :performanceRating 8 .

# Paradigm Instances
:ObjectOriented a :Paradigm ;
    rdfs:label "Object-Oriented Programming" .

:Functional a :Paradigm ;
    rdfs:label "Functional Programming" .

:Procedural a :Paradigm ;
    rdfs:label "Procedural Programming" .

:EventDriven a :Paradigm ;
    rdfs:label "Event-Driven Programming" .

# Framework Instances
:Django a :Framework ;
    rdfs:label "Django" ;
    rdfs:comment "High-level Python web framework" .

:React a :Framework ;
    rdfs:label "React" ;
    rdfs:comment "JavaScript library for building user interfaces" .

:Flask a :Framework ;
    rdfs:label "Flask" ;
    rdfs:comment "Lightweight Python web framework" .
