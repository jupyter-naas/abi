# SalesMetrics Ontology for Sales Development Representative Domain
# Sales metrics, KPIs, and performance indicators ontology

@prefix : <http://naas.ai/ontology/sales-development-representative/salesmetrics#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Ontology Declaration
: a owl:Ontology ;
    rdfs:label "SalesMetrics Ontology" ;
    rdfs:comment "ðŸš§ NOT FUNCTIONAL YET - Sales metrics, KPIs, and performance indicators ontology" .

# Core Classes
:SalesMetricsEntity a owl:Class ;
    rdfs:label "SalesMetrics Entity" ;
    rdfs:comment "Base class for salesmetrics entities" .

:SalesMetricsProcess a owl:Class ;
    rdfs:label "SalesMetrics Process" ;
    rdfs:comment "Processes related to salesmetrics" .

:SalesMetricsMetric a owl:Class ;
    rdfs:label "SalesMetrics Metric" ;
    rdfs:comment "Metrics and measurements for salesmetrics" .

# Sales Metrics-specific Classes
:ActivityMetric a owl:Class ;
    rdfs:subClassOf :SalesMetricsEntity ;
    rdfs:label "Activity Metric" ;
    rdfs:comment "Sales activity metrics (calls, emails, meetings)" .

:ConversionMetric a owl:Class ;
    rdfs:subClassOf :SalesMetricsEntity ;
    rdfs:label "Conversion Metric" ;
    rdfs:comment "Conversion rate metrics across sales stages" .

:PerformanceKPI a owl:Class ;
    rdfs:subClassOf :SalesMetricsEntity ;
    rdfs:label "Performance KPI" ;
    rdfs:comment "Key performance indicators for sales development" .

# Core Properties
:hasType a owl:ObjectProperty ;
    rdfs:domain :SalesMetricsEntity ;
    rdfs:range xsd:string ;
    rdfs:label "has type" .

:hasValue a owl:DatatypeProperty ;
    rdfs:domain :SalesMetricsMetric ;
    rdfs:range xsd:decimal ;
    rdfs:label "has value" .

:relatedTo a owl:ObjectProperty ;
    rdfs:domain :SalesMetricsEntity ;
    rdfs:range :SalesMetricsEntity ;
    rdfs:label "related to" .

# Sales Metrics-specific Properties
:hasTarget a owl:DatatypeProperty ;
    rdfs:domain :PerformanceKPI ;
    rdfs:range xsd:decimal ;
    rdfs:label "has target value" .

:hasActual a owl:DatatypeProperty ;
    rdfs:domain :PerformanceKPI ;
    rdfs:range xsd:decimal ;
    rdfs:label "has actual value" .

:hasTimePeriod a owl:DatatypeProperty ;
    rdfs:domain :SalesMetricsEntity ;
    rdfs:range xsd:string ;
    rdfs:label "has time period" .

