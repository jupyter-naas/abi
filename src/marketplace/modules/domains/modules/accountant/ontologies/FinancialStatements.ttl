@prefix : <http://naas.ai/ontology/accountant/financialstatements#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Ontology Declaration
: a owl:Ontology ;
    rdfs:label "FinancialStatements Ontology" ;
    rdfs:comment "ðŸš§ NOT FUNCTIONAL YET - Financial statement components and relationships ontology" .

# Core Classes
:FinancialStatementsEntity a owl:Class ;
    rdfs:label "FinancialStatements Entity" ;
    rdfs:comment "Base class for financialstatements entities" .

:FinancialStatementsProcess a owl:Class ;
    rdfs:label "FinancialStatements Process" ;
    rdfs:comment "Processes related to financialstatements" .

:FinancialStatementsMetric a owl:Class ;
    rdfs:label "FinancialStatements Metric" ;
    rdfs:comment "Metrics and measurements for financialstatements" .

# Core Properties
:hasType a owl:ObjectProperty ;
    rdfs:domain :FinancialStatementsEntity ;
    rdfs:range xsd:string ;
    rdfs:label "has type" .

:hasValue a owl:DatatypeProperty ;
    rdfs:domain :FinancialStatementsMetric ;
    rdfs:range xsd:decimal ;
    rdfs:label "has value" .

:relatedTo a owl:ObjectProperty ;
    rdfs:domain :FinancialStatementsEntity ;
    rdfs:range :FinancialStatementsEntity ;
    rdfs:label "related to" .

