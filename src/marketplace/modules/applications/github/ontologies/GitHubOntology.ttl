@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dc11: <http://purl.org/dc/elements/1.1/> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix cco: <https://www.commoncoreontologies.org/> .
@prefix abi: <http://ontology.naas.ai/abi/> .
@prefix github: <http://ontology.naas.ai/abi/github/> .

<http://ontology.naas.ai/abi/github.ttl> rdf:type owl:Ontology ;
    dc11:contributor "Jeremy Ravenel" , "Maxime Jublou" , "Florent Ravenel" ;
    dc:description "Application ontology for GitHub."@en ;
    dc:title "GitHub Application Ontology" .

#################################################################
#    Classes
#################################################################

github:GitHubPage a owl:Class ;
    rdfs:subClassOf abi:SocialPage ;
    rdfs:label "GitHub Page" ;
    skos:definition "A GitHub page that represents an entity's presence on GitHub, containing information specific to that entity type such as repositories, organizations, or user profiles." ;
    skos:example "A GitHub page could be a user profile page showing repositories and contributions, an organization page with team repositories, or a repository page with code and issues." ;
    rdfs:comment "A GitHub page must be registered in https://github.com/" ;
    abi:is_curated_in_foundry "enterprise_management_foundry", "personal_ai_foundry" .

github:UserPage a owl:Class ;
    rdfs:subClassOf github:GitHubPage ;
    rdfs:label "GitHub User Page" ;
    skos:definition "A GitHub user page that includes information such as a person's repositories, contributions, followers, and professional details, representing the individual's development presence on GitHub." ;
    skos:example "John Doe's GitHub user page, which lists his repositories, contribution graph, followers, and professional bio." ;
    rdfs:comment "A GitHub user page must be registered in https://github.com/username" ;
    abi:is_curated_in_foundry "enterprise_management_foundry", "personal_ai_foundry" .

github:OrganizationPage a owl:Class ;
    rdfs:subClassOf github:GitHubPage ;
    rdfs:label "GitHub Organization Page" ;
    skos:definition "A GitHub organization page that includes information such as the organization's repositories, members, teams, and company information, representing the organization's development presence on GitHub." ;
    skos:example "Microsoft's GitHub organization page, which showcases their repositories, team members, and organizational information." ;
    rdfs:comment "A GitHub organization page must be registered in https://github.com/organization" ;
    abi:is_curated_in_foundry "enterprise_management_foundry", "personal_ai_foundry" .

github:RepositoryPage a owl:Class ;
    rdfs:subClassOf github:GitHubPage ;
    rdfs:label "GitHub Repository Page" ;
    skos:definition "A GitHub repository page that includes information about a software project, including code, issues, pull requests, releases, and documentation." ;
    skos:example "A repository page for a web application project, showing the codebase, open issues, recent commits, and contribution guidelines." ;
    rdfs:comment "A GitHub repository page must be registered in https://github.com/owner/repository" ;
    abi:is_curated_in_foundry "enterprise_management_foundry", "personal_ai_foundry" .

github:Property a owl:Class ;
    rdfs:subClassOf cco:ont00001069 ; # Representational Information Content Entity
    rdfs:label "GitHub Property" ;
    skos:definition "Represents a property of a GitHub entity." ;
    skos:example "John Doe's GitHub user page has properties such as User, Repository, Issue, etc." ;
    abi:is_curated_in_foundry "enterprise_management_foundry", "personal_ai_foundry" .

github:User a owl:Class ;
    rdfs:subClassOf github:Property ;
    rdfs:label "GitHub User" ;
    skos:definition "Represents a GitHub user with profile information, repositories, and activity data." ;
    skos:example "John Doe's user profile including username, bio, location, email, public repositories count, followers count, and company information." ;
    abi:is_curated_in_foundry "enterprise_management_foundry", "personal_ai_foundry" .

github:Repository a owl:Class ;
    rdfs:subClassOf github:Property ;
    rdfs:label "GitHub Repository" ;
    skos:definition "Represents a GitHub repository containing source code, documentation, and project management features." ;
    skos:example "A repository with name, description, programming language, star count, fork count, and privacy settings." ;
    abi:is_curated_in_foundry "enterprise_management_foundry", "personal_ai_foundry" .

github:Issue a owl:Class ;
    rdfs:subClassOf github:Property ;
    rdfs:label "GitHub Issue" ;
    skos:definition "Represents an issue in a GitHub repository for tracking bugs, feature requests, or other tasks." ;
    skos:example "An issue with title, body, labels, assignees, state (open/closed), and creation date." ;
    abi:is_curated_in_foundry "enterprise_management_foundry", "personal_ai_foundry" .

github:IssueComment a owl:Class ;
    rdfs:subClassOf github:Property ;
    rdfs:label "GitHub Issue Comment" ;
    skos:definition "Represents a comment on a GitHub issue or pull request." ;
    skos:example "A comment with body text, author information, and timestamp on an issue discussion." ;
    abi:is_curated_in_foundry "enterprise_management_foundry", "personal_ai_foundry" .

github:PullRequest a owl:Class ;
    rdfs:subClassOf github:Property ;
    rdfs:label "GitHub Pull Request" ;
    skos:definition "Represents a pull request in a GitHub repository for proposing changes to the codebase." ;
    skos:example "A pull request with title, body, source branch, target branch, and review status." ;
    abi:is_curated_in_foundry "enterprise_management_foundry", "personal_ai_foundry" .

github:Contributor a owl:Class ;
    rdfs:subClassOf github:Property ;
    rdfs:label "GitHub Contributor" ;
    skos:definition "Represents a contributor to a GitHub repository with their contribution statistics." ;
    skos:example "A contributor with username, commit count, additions, deletions, and contribution timeline." ;
    abi:is_curated_in_foundry "enterprise_management_foundry", "personal_ai_foundry" .

github:RepositoryActivity a owl:Class ;
    rdfs:subClassOf github:Property ;
    rdfs:label "GitHub Repository Activity" ;
    skos:definition "Represents activity events in a GitHub repository such as pushes, merges, and branch operations." ;
    skos:example "A push activity with commit information, actor, timestamp, and affected files." ;
    abi:is_curated_in_foundry "enterprise_management_foundry", "personal_ai_foundry" .

github:Assignee a owl:Class ;
    rdfs:subClassOf github:Property ;
    rdfs:label "GitHub Assignee" ;
    skos:definition "Represents a user assigned to work on a GitHub issue or pull request." ;
    skos:example "A user assigned to fix a bug issue with their username and assignment timestamp." ;
    abi:is_curated_in_foundry "enterprise_management_foundry", "personal_ai_foundry" .

github:Secret a owl:Class ;
    rdfs:subClassOf github:Property ;
    rdfs:label "GitHub Repository Secret" ;
    skos:definition "Represents an encrypted secret stored in a GitHub repository for use in workflows and automation." ;
    skos:example "An API key or password stored securely for use in GitHub Actions workflows." ;
    abi:is_curated_in_foundry "enterprise_management_foundry" .

github:Organization a owl:Class ;
    rdfs:subClassOf github:Property ;
    rdfs:label "GitHub Organization" ;
    skos:definition "Represents a GitHub organization that groups users and repositories under a common entity." ;
    skos:example "A company organization with multiple team members and repositories for different projects." ;
    abi:is_curated_in_foundry "enterprise_management_foundry", "personal_ai_foundry" .

#################################################################
#    Object Properties
#################################################################

github:hasGitHubPage a owl:ObjectProperty ;
    rdfs:domain [ rdf:type owl:Class ;
                  owl:unionOf ( cco:ont00001180  # Person
                               cco:ont00001262   # Organization
                             )
                ] ;
    rdfs:range github:GitHubPage ;
    rdfs:label "has GitHub page"@en ;
    skos:definition "Relates a person or organization to its GitHub pages."@en ;
    skos:example "John Doe's GitHub pages including user profile and repository pages."@en ;
    owl:inverseOf github:isGitHubPageOf .

github:isGitHubPageOf a owl:ObjectProperty ;
    rdfs:domain github:GitHubPage ;
    rdfs:range [ rdf:type owl:Class ;
                 owl:unionOf ( cco:ont00001180  # Person
                              cco:ont00001262   # Organization
                            )
               ] ;
    rdfs:label "is GitHub page of"@en ;
    skos:definition "Relates a GitHub page to the person or organization it represents."@en ;
    skos:example "A GitHub page that represents John Doe's development presence."@en ;
    owl:inverseOf github:hasGitHubPage .

github:hasUser a owl:ObjectProperty ;
    rdfs:domain github:UserPage ;
    rdfs:range github:User ;
    rdfs:label "has user"@en ;
    skos:definition "Relates a GitHub user page to its user profile."@en ;
    skos:example "John Doe's GitHub user page has a user profile with username, bio, location, etc."@en ;
    owl:inverseOf github:isUserOf .

github:isUserOf a owl:ObjectProperty ;
    rdfs:domain github:User ;
    rdfs:range github:UserPage ;
    rdfs:label "is user of"@en ;
    skos:definition "Relates a GitHub user to the GitHub user page it belongs to."@en ;
    skos:example "John Doe's user profile is the user of his GitHub user page."@en ;
    owl:inverseOf github:hasUser .

github:hasRepository a owl:ObjectProperty ;
    rdfs:domain [ rdf:type owl:Class ;
                  owl:unionOf ( github:UserPage
                               github:OrganizationPage
                             )
                ] ;
    rdfs:range github:Repository ;
    rdfs:label "has repository"@en ;
    skos:definition "Relates a GitHub user or organization page to its repositories."@en ;
    skos:example "John Doe's GitHub page has repositories listed."@en ;
    owl:inverseOf github:isRepositoryOf .

github:isRepositoryOf a owl:ObjectProperty ;
    rdfs:domain github:Repository ;
    rdfs:range [ rdf:type owl:Class ;
                 owl:unionOf ( github:UserPage
                              github:OrganizationPage
                            )
               ] ;
    rdfs:label "is repository of"@en ;
    skos:definition "Relates a repository to the GitHub user or organization page it belongs to."@en ;
    skos:example "A repository is owned by John Doe's GitHub page."@en ;
    owl:inverseOf github:hasRepository .

github:hasIssue a owl:ObjectProperty ;
    rdfs:domain github:Repository ;
    rdfs:range github:Issue ;
    rdfs:label "has issue"@en ;
    skos:definition "Relates a GitHub repository to its issues."@en ;
    skos:example "A repository has issues for bug tracking and feature requests."@en ;
    owl:inverseOf github:isIssueOf .

github:isIssueOf a owl:ObjectProperty ;
    rdfs:domain github:Issue ;
    rdfs:range github:Repository ;
    rdfs:label "is issue of"@en ;
    skos:definition "Relates an issue to the GitHub repository it belongs to."@en ;
    skos:example "An issue is tracked in a specific repository."@en ;
    owl:inverseOf github:hasIssue .

github:hasIssueComment a owl:ObjectProperty ;
    rdfs:domain github:Issue ;
    rdfs:range github:IssueComment ;
    rdfs:label "has issue comment"@en ;
    skos:definition "Relates a GitHub issue to its comments."@en ;
    skos:example "An issue has comments from contributors discussing the problem."@en ;
    owl:inverseOf github:isIssueCommentOf .

github:isIssueCommentOf a owl:ObjectProperty ;
    rdfs:domain github:IssueComment ;
    rdfs:range github:Issue ;
    rdfs:label "is issue comment of"@en ;
    skos:definition "Relates a comment to the GitHub issue it belongs to."@en ;
    skos:example "A comment is posted on a specific issue."@en ;
    owl:inverseOf github:hasIssueComment .

github:hasPullRequest a owl:ObjectProperty ;
    rdfs:domain github:Repository ;
    rdfs:range github:PullRequest ;
    rdfs:label "has pull request"@en ;
    skos:definition "Relates a GitHub repository to its pull requests."@en ;
    skos:example "A repository has pull requests for code review and merging."@en ;
    owl:inverseOf github:isPullRequestOf .

github:isPullRequestOf a owl:ObjectProperty ;
    rdfs:domain github:PullRequest ;
    rdfs:range github:Repository ;
    rdfs:label "is pull request of"@en ;
    skos:definition "Relates a pull request to the GitHub repository it belongs to."@en ;
    skos:example "A pull request is submitted to a specific repository."@en ;
    owl:inverseOf github:hasPullRequest .

github:hasContributor a owl:ObjectProperty ;
    rdfs:domain github:Repository ;
    rdfs:range github:Contributor ;
    rdfs:label "has contributor"@en ;
    skos:definition "Relates a GitHub repository to its contributors."@en ;
    skos:example "A repository has contributors who have submitted code."@en ;
    owl:inverseOf github:isContributorOf .

github:isContributorOf a owl:ObjectProperty ;
    rdfs:domain github:Contributor ;
    rdfs:range github:Repository ;
    rdfs:label "is contributor of"@en ;
    skos:definition "Relates a contributor to the GitHub repository they have contributed to."@en ;
    skos:example "A contributor has made commits to a specific repository."@en ;
    owl:inverseOf github:hasContributor .

github:hasActivity a owl:ObjectProperty ;
    rdfs:domain github:Repository ;
    rdfs:range github:RepositoryActivity ;
    rdfs:label "has activity"@en ;
    skos:definition "Relates a GitHub repository to its activity events."@en ;
    skos:example "A repository has activity events like pushes and merges."@en ;
    owl:inverseOf github:isActivityOf .

github:isActivityOf a owl:ObjectProperty ;
    rdfs:domain github:RepositoryActivity ;
    rdfs:range github:Repository ;
    rdfs:label "is activity of"@en ;
    skos:definition "Relates an activity event to the GitHub repository it occurred in."@en ;
    skos:example "A push activity occurred in a specific repository."@en ;
    owl:inverseOf github:hasActivity .

github:hasAssignee a owl:ObjectProperty ;
    rdfs:domain github:Issue ;
    rdfs:range github:Assignee ;
    rdfs:label "has assignee"@en ;
    skos:definition "Relates a GitHub issue to its assignees."@en ;
    skos:example "An issue has assignees responsible for resolving it."@en ;
    owl:inverseOf github:isAssigneeOf .

github:isAssigneeOf a owl:ObjectProperty ;
    rdfs:domain github:Assignee ;
    rdfs:range github:Issue ;
    rdfs:label "is assignee of"@en ;
    skos:definition "Relates an assignee to the GitHub issue they are assigned to."@en ;
    skos:example "A user is assigned to work on a specific issue."@en ;
    owl:inverseOf github:hasAssignee .

github:hasSecret a owl:ObjectProperty ;
    rdfs:domain github:Repository ;
    rdfs:range github:Secret ;
    rdfs:label "has secret"@en ;
    skos:definition "Relates a GitHub repository to its stored secrets."@en ;
    skos:example "A repository has secrets for API keys and passwords."@en ;
    owl:inverseOf github:isSecretOf .

github:isSecretOf a owl:ObjectProperty ;
    rdfs:domain github:Secret ;
    rdfs:range github:Repository ;
    rdfs:label "is secret of"@en ;
    skos:definition "Relates a secret to the GitHub repository it is stored in."@en ;
    skos:example "An API key secret is stored in a specific repository."@en ;
    owl:inverseOf github:hasSecret .

github:hasOrganization a owl:ObjectProperty ;
    rdfs:domain github:OrganizationPage ;
    rdfs:range github:Organization ;
    rdfs:label "has organization"@en ;
    skos:definition "Relates a GitHub organization page to its organization profile."@en ;
    skos:example "An organization page has organization details and member information."@en ;
    owl:inverseOf github:isOrganizationOf .

github:isOrganizationOf a owl:ObjectProperty ;
    rdfs:domain github:Organization ;
    rdfs:range github:OrganizationPage ;
    rdfs:label "is organization of"@en ;
    skos:definition "Relates an organization to the GitHub organization page it belongs to."@en ;
    skos:example "An organization profile belongs to a specific organization page."@en ;
    owl:inverseOf github:hasOrganization .

#################################################################
#    Data properties
#################################################################

github:id a owl:DatatypeProperty ;
    rdfs:domain github:GitHubPage ;
    rdfs:range xsd:string ;
    rdfs:label "GitHub ID"@en ;
    skos:definition "The unique identifier of the GitHub entity."@en ;
    skos:example "The unique ID of John Doe's GitHub user is '12345678'."@en .

github:url a owl:DatatypeProperty ;
    rdfs:domain github:GitHubPage ;
    rdfs:range xsd:string ;
    rdfs:label "GitHub URL"@en ;
    skos:definition "The URL of the GitHub page."@en ;
    skos:example "The URL of John Doe's GitHub user page is 'https://github.com/johndoe'."@en .

github:name a owl:DatatypeProperty ;
    rdfs:domain [ rdf:type owl:Class ;
                  owl:unionOf ( github:User
                               github:Repository
                               github:Organization
                             )
                ] ;
    rdfs:range xsd:string ;
    rdfs:label "Name"@en ;
    skos:definition "The name of the GitHub entity."@en ;
    skos:example "The name of the user is 'John Doe' or the repository name is 'my-project'."@en .

github:login a owl:DatatypeProperty ;
    rdfs:domain github:User ;
    rdfs:range xsd:string ;
    rdfs:label "Login Username"@en ;
    skos:definition "The login username of the GitHub user."@en ;
    skos:example "The login username is 'johndoe'."@en .

github:bio a owl:DatatypeProperty ;
    rdfs:domain github:User ;
    rdfs:range xsd:string ;
    rdfs:label "Bio"@en ;
    skos:definition "The biography or description of the GitHub user."@en ;
    skos:example "The bio is 'Software engineer passionate about open source'."@en .

github:location a owl:DatatypeProperty ;
    rdfs:domain github:User ;
    rdfs:range xsd:string ;
    rdfs:label "Location"@en ;
    skos:definition "The location of the GitHub user."@en ;
    skos:example "The location is 'San Francisco, CA'."@en .

github:email a owl:DatatypeProperty ;
    rdfs:domain github:User ;
    rdfs:range xsd:string ;
    rdfs:label "Email"@en ;
    skos:definition "The email address of the GitHub user."@en ;
    skos:example "The email is 'john.doe@example.com'."@en .

github:company a owl:DatatypeProperty ;
    rdfs:domain github:User ;
    rdfs:range xsd:string ;
    rdfs:label "Company"@en ;
    skos:definition "The company or organization the GitHub user works for."@en ;
    skos:example "The company is 'Example Corp'."@en .

github:blog a owl:DatatypeProperty ;
    rdfs:domain github:User ;
    rdfs:range xsd:string ;
    rdfs:label "Blog URL"@en ;
    skos:definition "The blog or website URL of the GitHub user."@en ;
    skos:example "The blog URL is 'https://johndoe.com'."@en .

github:publicRepos a owl:DatatypeProperty ;
    rdfs:domain github:User ;
    rdfs:range xsd:nonNegativeInteger ;
    rdfs:label "Public Repositories Count"@en ;
    skos:definition "The number of public repositories owned by the GitHub user."@en ;
    skos:example "The user has 25 public repositories."@en .

github:followers a owl:DatatypeProperty ;
    rdfs:domain github:User ;
    rdfs:range xsd:nonNegativeInteger ;
    rdfs:label "Followers Count"@en ;
    skos:definition "The number of followers of the GitHub user."@en ;
    skos:example "The user has 150 followers."@en .

github:following a owl:DatatypeProperty ;
    rdfs:domain github:User ;
    rdfs:range xsd:nonNegativeInteger ;
    rdfs:label "Following Count"@en ;
    skos:definition "The number of users that the GitHub user is following."@en ;
    skos:example "The user is following 75 other users."@en .

github:description a owl:DatatypeProperty ;
    rdfs:domain github:Repository ;
    rdfs:range xsd:string ;
    rdfs:label "Description"@en ;
    skos:definition "The description of the GitHub repository."@en ;
    skos:example "The description is 'A web application for task management'."@en .

github:private a owl:DatatypeProperty ;
    rdfs:domain github:Repository ;
    rdfs:range xsd:boolean ;
    rdfs:label "Private"@en ;
    skos:definition "Whether the GitHub repository is private or public."@en ;
    skos:example "The repository is private (true) or public (false)."@en .

github:language a owl:DatatypeProperty ;
    rdfs:domain github:Repository ;
    rdfs:range xsd:string ;
    rdfs:label "Primary Language"@en ;
    skos:definition "The primary programming language of the GitHub repository."@en ;
    skos:example "The primary language is 'JavaScript'."@en .

github:stars a owl:DatatypeProperty ;
    rdfs:domain github:Repository ;
    rdfs:range xsd:nonNegativeInteger ;
    rdfs:label "Stars Count"@en ;
    skos:definition "The number of stars the GitHub repository has received."@en ;
    skos:example "The repository has 1,250 stars."@en .

github:forks a owl:DatatypeProperty ;
    rdfs:domain github:Repository ;
    rdfs:range xsd:nonNegativeInteger ;
    rdfs:label "Forks Count"@en ;
    skos:definition "The number of forks of the GitHub repository."@en ;
    skos:example "The repository has 85 forks."@en .

github:title a owl:DatatypeProperty ;
    rdfs:domain [ rdf:type owl:Class ;
                  owl:unionOf ( github:Issue
                               github:PullRequest
                             )
                ] ;
    rdfs:range xsd:string ;
    rdfs:label "Title"@en ;
    skos:definition "The title of the GitHub issue or pull request."@en ;
    skos:example "The issue title is 'Fix login bug' or pull request title is 'Add new feature'."@en .

github:body a owl:DatatypeProperty ;
    rdfs:domain [ rdf:type owl:Class ;
                  owl:unionOf ( github:Issue
                               github:IssueComment
                               github:PullRequest
                             )
                ] ;
    rdfs:range xsd:string ;
    rdfs:label "Body"@en ;
    skos:definition "The body content of the GitHub issue, comment, or pull request."@en ;
    skos:example "The issue body contains detailed description of the problem."@en .

github:state a owl:DatatypeProperty ;
    rdfs:domain [ rdf:type owl:Class ;
                  owl:unionOf ( github:Issue
                               github:PullRequest
                             )
                ] ;
    rdfs:range xsd:string ;
    rdfs:label "State"@en ;
    skos:definition "The state of the GitHub issue or pull request (open, closed, etc.)."@en ;
    skos:example "The issue state is 'open' or 'closed'."@en .

github:number a owl:DatatypeProperty ;
    rdfs:domain [ rdf:type owl:Class ;
                  owl:unionOf ( github:Issue
                               github:PullRequest
                             )
                ] ;
    rdfs:range xsd:nonNegativeInteger ;
    rdfs:label "Number"@en ;
    skos:definition "The sequential number of the GitHub issue or pull request."@en ;
    skos:example "The issue number is 42."@en .

github:labels a owl:DatatypeProperty ;
    rdfs:domain github:Issue ;
    rdfs:range xsd:string ;
    rdfs:label "Labels"@en ;
    skos:definition "The labels assigned to the GitHub issue."@en ;
    skos:example "The issue has labels 'bug', 'high-priority'."@en .

github:headBranch a owl:DatatypeProperty ;
    rdfs:domain github:PullRequest ;
    rdfs:range xsd:string ;
    rdfs:label "Head Branch"@en ;
    skos:definition "The source branch of the GitHub pull request."@en ;
    skos:example "The head branch is 'feature/new-login'."@en .

github:baseBranch a owl:DatatypeProperty ;
    rdfs:domain github:PullRequest ;
    rdfs:range xsd:string ;
    rdfs:label "Base Branch"@en ;
    skos:definition "The target branch of the GitHub pull request."@en ;
    skos:example "The base branch is 'main'."@en .

github:contributions a owl:DatatypeProperty ;
    rdfs:domain github:Contributor ;
    rdfs:range xsd:nonNegativeInteger ;
    rdfs:label "Contributions Count"@en ;
    skos:definition "The number of contributions made by the contributor."@en ;
    skos:example "The contributor has made 127 contributions."@en .

github:activityType a owl:DatatypeProperty ;
    rdfs:domain github:RepositoryActivity ;
    rdfs:range xsd:string ;
    rdfs:label "Activity Type"@en ;
    skos:definition "The type of repository activity (push, merge, etc.)."@en ;
    skos:example "The activity type is 'push' or 'pull_request_merge'."@en .

github:actor a owl:DatatypeProperty ;
    rdfs:domain github:RepositoryActivity ;
    rdfs:range xsd:string ;
    rdfs:label "Actor"@en ;
    skos:definition "The user who performed the repository activity."@en ;
    skos:example "The actor is 'johndoe'."@en .

github:secretName a owl:DatatypeProperty ;
    rdfs:domain github:Secret ;
    rdfs:range xsd:string ;
    rdfs:label "Secret Name"@en ;
    skos:definition "The name of the GitHub repository secret."@en ;
    skos:example "The secret name is 'API_KEY'."@en .

github:createdAt a owl:DatatypeProperty ;
    rdfs:domain [ rdf:type owl:Class ;
                  owl:unionOf ( github:User
                               github:Repository
                               github:Issue
                               github:IssueComment
                               github:PullRequest
                               github:RepositoryActivity
                               github:Secret
                             )
                ] ;
    rdfs:range xsd:dateTime ;
    rdfs:label "Created At"@en ;
    skos:definition "The timestamp when the GitHub entity was created."@en ;
    skos:example "The entity was created at '2023-01-15T10:30:00Z'."@en .

github:updatedAt a owl:DatatypeProperty ;
    rdfs:domain [ rdf:type owl:Class ;
                  owl:unionOf ( github:User
                               github:Repository
                               github:Issue
                               github:IssueComment
                               github:PullRequest
                               github:Secret
                             )
                ] ;
    rdfs:range xsd:dateTime ;
    rdfs:label "Updated At"@en ;
    skos:definition "The timestamp when the GitHub entity was last updated."@en ;
    skos:example "The entity was last updated at '2023-01-20T14:45:00Z'."@en .
