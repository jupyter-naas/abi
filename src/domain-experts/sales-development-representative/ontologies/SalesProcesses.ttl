# SalesProcesses Ontology for Sales Development Representative Domain
# Sales processes, methodologies, and frameworks ontology

@prefix : <http://naas.ai/ontology/sales-development-representative/salesprocesses#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Ontology Declaration
: a owl:Ontology ;
    rdfs:label "SalesProcesses Ontology" ;
    rdfs:comment "ðŸš§ NOT FUNCTIONAL YET - Sales processes, methodologies, and frameworks ontology" .

# Core Classes
:SalesProcessesEntity a owl:Class ;
    rdfs:label "SalesProcesses Entity" ;
    rdfs:comment "Base class for salesprocesses entities" .

:SalesProcessesProcess a owl:Class ;
    rdfs:label "SalesProcesses Process" ;
    rdfs:comment "Processes related to salesprocesses" .

:SalesProcessesMetric a owl:Class ;
    rdfs:label "SalesProcesses Metric" ;
    rdfs:comment "Metrics and measurements for salesprocesses" .

# Sales-specific Classes
:SalesStage a owl:Class ;
    rdfs:subClassOf :SalesProcessesEntity ;
    rdfs:label "Sales Stage" ;
    rdfs:comment "Stages in the sales process" .

:QualificationFramework a owl:Class ;
    rdfs:subClassOf :SalesProcessesEntity ;
    rdfs:label "Qualification Framework" ;
    rdfs:comment "Lead qualification frameworks (BANT, MEDDIC, etc.)" .

:ProspectingMethod a owl:Class ;
    rdfs:subClassOf :SalesProcessesEntity ;
    rdfs:label "Prospecting Method" ;
    rdfs:comment "Methods for prospecting and lead generation" .

# Core Properties
:hasType a owl:ObjectProperty ;
    rdfs:domain :SalesProcessesEntity ;
    rdfs:range xsd:string ;
    rdfs:label "has type" .

:hasValue a owl:DatatypeProperty ;
    rdfs:domain :SalesProcessesMetric ;
    rdfs:range xsd:decimal ;
    rdfs:label "has value" .

:relatedTo a owl:ObjectProperty ;
    rdfs:domain :SalesProcessesEntity ;
    rdfs:range :SalesProcessesEntity ;
    rdfs:label "related to" .

# Sales-specific Properties
:hasConversionRate a owl:DatatypeProperty ;
    rdfs:domain :SalesStage ;
    rdfs:range xsd:decimal ;
    rdfs:label "has conversion rate" .

:requiresCriteria a owl:ObjectProperty ;
    rdfs:domain :QualificationFramework ;
    rdfs:range xsd:string ;
    rdfs:label "requires criteria" .

