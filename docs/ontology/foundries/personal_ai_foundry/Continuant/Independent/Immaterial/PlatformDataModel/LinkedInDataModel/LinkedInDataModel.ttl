# Class definition for LinkedInDataModel
pai:LinkedInDataModel rdf:type owl:Class ;
    rdfs:subClassOf pai:PlatformDataModel ;
    rdfs:label "LinkedIn Data Model"@en ;
    rdfs:comment "An independent continuant immaterial entity that represents the platform-specific structure, semantics, and constraints of LinkedIn's data, including its unique connection degrees, recommendations, skills taxonomy, and professional graph concepts."@en .

# Core Profile Classes
pai:LinkedInProfile rdf:type owl:Class ;
    rdfs:subClassOf pai:Contact ;
    rdfs:label "LinkedIn Profile"@en ;
    rdfs:comment "A digital representation of an individual's professional identity on LinkedIn."@en .

pai:LinkedInMiniProfile rdf:type owl:Class ;
    rdfs:subClassOf pai:LinkedInProfile ;
    rdfs:label "LinkedIn Mini Profile"@en ;
    rdfs:comment "A simplified representation of a LinkedIn profile with basic information."@en .

# Company and Organization Classes
pai:LinkedInCompany rdf:type owl:Class ;
    rdfs:subClassOf pai:Organization ;
    rdfs:label "LinkedIn Company"@en ;
    rdfs:comment "A company or organization profile on LinkedIn."@en .

pai:LinkedInMiniCompany rdf:type owl:Class ;
    rdfs:subClassOf pai:LinkedInCompany ;
    rdfs:label "LinkedIn Mini Company"@en ;
    rdfs:comment "A simplified representation of a company on LinkedIn."@en .

# Education Classes
pai:LinkedInEducation rdf:type owl:Class ;
    rdfs:subClassOf pai:Education ;
    rdfs:label "LinkedIn Education"@en ;
    rdfs:comment "Educational background information in a LinkedIn profile."@en .

pai:LinkedInSchool rdf:type owl:Class ;
    rdfs:subClassOf pai:EducationalInstitution ;
    rdfs:label "LinkedIn School"@en ;
    rdfs:comment "An educational institution listed on LinkedIn."@en .

pai:LinkedInMiniSchool rdf:type owl:Class ;
    rdfs:subClassOf pai:LinkedInSchool ;
    rdfs:label "LinkedIn Mini School"@en ;
    rdfs:comment "A simplified representation of a school on LinkedIn."@en .

pai:LinkedInCourse rdf:type owl:Class ;
    rdfs:subClassOf pai:Course ;
    rdfs:label "LinkedIn Course"@en ;
    rdfs:comment "A course associated with a LinkedIn education entry."@en .

# Career Classes
pai:LinkedInPosition rdf:type owl:Class ;
    rdfs:subClassOf pai:Employment ;
    rdfs:label "LinkedIn Position"@en ;
    rdfs:comment "Professional experience or position listed on a LinkedIn profile."@en .

pai:LinkedInCertification rdf:type owl:Class ;
    rdfs:subClassOf pai:Certification ;
    rdfs:label "LinkedIn Certification"@en ;
    rdfs:comment "A professional certification listed on a LinkedIn profile."@en .

pai:LinkedInSkill rdf:type owl:Class ;
    rdfs:subClassOf pai:Skill ;
    rdfs:label "LinkedIn Skill"@en ;
    rdfs:comment "A professional skill listed on a LinkedIn profile."@en .

pai:LinkedInLanguage rdf:type owl:Class ;
    rdfs:subClassOf pai:Language ;
    rdfs:label "LinkedIn Language"@en ;
    rdfs:comment "A language proficiency listed on a LinkedIn profile."@en .

# Connection Classes
pai:LinkedInConnection rdf:type owl:Class ;
    rdfs:subClassOf pai:Relationship ;
    rdfs:label "LinkedIn Connection"@en ;
    rdfs:comment "A professional connection relationship on LinkedIn."@en .

pai:FirstDegreeConnection rdf:type owl:Class ;
    rdfs:subClassOf pai:LinkedInConnection ;
    rdfs:label "First Degree Connection"@en ;
    rdfs:comment "A direct connection with mutual acceptance on LinkedIn."@en .

pai:SecondDegreeConnection rdf:type owl:Class ;
    rdfs:subClassOf pai:LinkedInConnection ;
    rdfs:label "Second Degree Connection"@en ;
    rdfs:comment "A connection of connection (no direct relationship) on LinkedIn."@en .

pai:ThirdDegreePlusConnection rdf:type owl:Class ;
    rdfs:subClassOf pai:LinkedInConnection ;
    rdfs:label "Third Degree Plus Connection"@en ;
    rdfs:comment "A more distant network relationship on LinkedIn."@en .

pai:OutOfNetworkConnection rdf:type owl:Class ;
    rdfs:subClassOf pai:LinkedInConnection ;
    rdfs:label "Out of Network Connection"@en ;
    rdfs:comment "No connection path within visible network on LinkedIn."@en .

pai:FollowOnlyRelationship rdf:type owl:Class ;
    rdfs:subClassOf pai:LinkedInConnection ;
    rdfs:label "Follow-Only Relationship"@en ;
    rdfs:comment "One-way following without connection on LinkedIn."@en .

# Endorsement and Recommendation Classes
pai:LinkedInEndorsement rdf:type owl:Class ;
    rdfs:subClassOf pai:Endorsement ;
    rdfs:label "LinkedIn Endorsement"@en ;
    rdfs:comment "A validation of a specific skill on LinkedIn."@en .

pai:LinkedInRecommendation rdf:type owl:Class ;
    rdfs:subClassOf pai:Endorsement ;
    rdfs:label "LinkedIn Recommendation"@en ;
    rdfs:comment "A professional endorsement with text on LinkedIn."@en .

