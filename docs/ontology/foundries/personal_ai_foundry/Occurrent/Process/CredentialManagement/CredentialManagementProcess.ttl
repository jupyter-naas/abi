# General Process Properties
cs:processId a owl:DatatypeProperty ;
    rdfs:domain cs:CredentialManagementProcess ;
    rdfs:range xsd:string ;
    rdfs:label "process id"@en .

cs:initiatedBy a owl:ObjectProperty ;
    rdfs:domain cs:CredentialManagementProcess ;
    rdfs:range owl:Thing ;
    rdfs:label "initiated by"@en .

cs:executedBy a owl:ObjectProperty ;
    rdfs:domain cs:CredentialManagementProcess ;
    rdfs:range owl:Thing ;
