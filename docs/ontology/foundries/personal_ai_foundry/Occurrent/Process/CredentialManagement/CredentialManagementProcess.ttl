@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix bfo: <http://purl.obolibrary.org/obo/BFO_> .
@prefix paf: <http://ontology.abi.ai/personal_ai_foundry#> .
@prefix cs: <http://ontology.abi.ai/contact_schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

# Process Class Definition
cs:CredentialManagementProcess a owl:Class ;
    rdfs:label "Credential Management Process"@en ;
    rdfs:subClassOf cs:SecurityProcess, bfo:0000015 ;
    skos:definition "An occurrent process that systematically manages the storage, retrieval, verification, application, and rotation of credentials across multiple digital platforms while maintaining security integrity."@en .

# Phase Classes
cs:InitiationPhase a owl:Class ;
    rdfs:label "Initiation Phase"@en ;
    rdfs:subClassOf cs:CredentialManagementProcess ;
    skos:definition "Initial phase for detecting and identifying authentication requests."@en .

cs:RetrievalPhase a owl:Class ;
    rdfs:label "Retrieval Phase"@en ;
    rdfs:subClassOf cs:CredentialManagementProcess ;
    skos:definition "Phase for securely accessing and decrypting stored credentials."@en .

cs:VerificationPhase a owl:Class ;
    rdfs:label "Verification Phase"@en ;
    rdfs:subClassOf cs:CredentialManagementProcess ;
    skos:definition "Phase for validating credential status and requesting additional verification if needed."@en .

cs:ApplicationPhase a owl:Class ;
    rdfs:label "Application Phase"@en ;
    rdfs:subClassOf cs:CredentialManagementProcess ;
    skos:definition "Phase for applying credentials to authentication endpoints and handling multi-factor authentication."@en .

cs:SynchronizationPhase a owl:Class ;
    rdfs:label "Synchronization Phase"@en ;
    rdfs:subClassOf cs:CredentialManagementProcess ;
    skos:definition "Phase for securely updating credential state across authorized devices."@en .

cs:RotationAssessmentPhase a owl:Class ;
    rdfs:label "Rotation Assessment Phase"@en ;
    rdfs:subClassOf cs:CredentialManagementProcess ;
    skos:definition "Phase for evaluating credential health and rotation requirements."@en .

cs:MonitoringPhase a owl:Class ;
    rdfs:label "Monitoring Phase"@en ;
    rdfs:subClassOf cs:CredentialManagementProcess ;
    skos:definition "Phase for security oversight during authentication."@en .

cs:CompletionPhase a owl:Class ;
    rdfs:label "Completion Phase"@en ;
    rdfs:subClassOf cs:CredentialManagementProcess ;
    skos:definition "Final phase for logging usage and transitioning control to authenticated applications."@en .

# Supporting Classes
cs:SecurityProcess a owl:Class ;
    rdfs:label "Security Process"@en ;
    rdfs:subClassOf paf:DataProcessingProcess ;
    skos:definition "A process that manages security-related operations for personal data."@en .

cs:StoredCredential a owl:Class ;
    rdfs:label "Stored Credential"@en ;
    rdfs:subClassOf paf:SecurityEntity ;
    skos:definition "A securely stored authentication credential for a specific platform."@en .

cs:VaultSystem a owl:Class ;
    rdfs:label "Vault System"@en ;
    rdfs:subClassOf paf:SecuritySystem ;
    skos:definition "A system for securely storing and managing encrypted credentials."@en .

cs:CredentialUsageEvent a owl:Class ;
    rdfs:label "Credential Usage Event"@en ;
    rdfs:subClassOf paf:Event ;
    skos:definition "An event representing the use of stored credentials for authentication."@en .

cs:FailedAuthenticationEvent a owl:Class ;
    rdfs:label "Failed Authentication Event"@en ;
    rdfs:subClassOf paf:Event ;
    skos:definition "An event representing a failed authentication attempt."@en .

cs:CredentialRotatedEvent a owl:Class ;
    rdfs:label "Credential Rotated Event"@en ;
    rdfs:subClassOf paf:Event ;
    skos:definition "An event representing the rotation or update of a credential."@en .

cs:SuspiciousActivityEvent a owl:Class ;
    rdfs:label "Suspicious Activity Event"@en ;
    rdfs:subClassOf paf:Event ;
    skos:definition "An event representing detected suspicious authentication activity."@en .

cs:CredentialSyncEvent a owl:Class ;
    rdfs:label "Credential Sync Event"@en ;
    rdfs:subClassOf paf:Event ;
    skos:definition "An event representing the synchronization of credentials across devices."@en .

# Properties for Process Phases

# Initiation Phase Properties
cs:platformIdentification a owl:DatatypeProperty ;
    rdfs:domain cs:InitiationPhase ;
    rdfs:range xsd:boolean ;
    rdfs:label "platform identification"@en .

cs:platform a owl:ObjectProperty ;
    rdfs:domain cs:InitiationPhase ;
    rdfs:range cs:Platform ;
    rdfs:label "platform"@en .

cs:authenticationContext a owl:DatatypeProperty ;
    rdfs:domain cs:InitiationPhase ;
    rdfs:range xsd:string ;
    rdfs:label "authentication context"@en .

cs:requestSource a owl:ObjectProperty ;
    rdfs:domain cs:InitiationPhase ;
    rdfs:range cs:ApplicationSource ;
    rdfs:label "request source"@en .

# Retrieval Phase Properties
cs:vaultAccessInitiated a owl:DatatypeProperty ;
    rdfs:domain cs:RetrievalPhase ;
    rdfs:range xsd:boolean ;
    rdfs:label "vault access initiated"@en .

cs:masterKeyVerification a owl:DatatypeProperty ;
    rdfs:domain cs:RetrievalPhase ;
    rdfs:range xsd:boolean ;
    rdfs:label "master key verification"@en .

cs:decryptionPerformed a owl:DatatypeProperty ;
    rdfs:domain cs:RetrievalPhase ;
    rdfs:range xsd:boolean ;
    rdfs:label "decryption performed"@en .

cs:credentialsRetrieved a owl:ObjectProperty ;
    rdfs:domain cs:RetrievalPhase ;
    rdfs:range cs:StoredCredential ;
    rdfs:label "credentials retrieved"@en .

# Verification Phase Properties
cs:credentialStatus a owl:DatatypeProperty ;
    rdfs:domain cs:VerificationPhase ;
    rdfs:range xsd:string ;
    rdfs:label "credential status"@en .

cs:expirationCheck a owl:DatatypeProperty ;
    rdfs:domain cs:VerificationPhase ;
    rdfs:range xsd:boolean ;
    rdfs:label "expiration check"@en .

cs:strengthVerification a owl:DatatypeProperty ;
    rdfs:domain cs:VerificationPhase ;
    rdfs:range xsd:boolean ;
    rdfs:label "strength verification"@en .

cs:userVerificationRequired a owl:DatatypeProperty ;
    rdfs:domain cs:VerificationPhase ;
    rdfs:range xsd:boolean ;
    rdfs:label "user verification required"@en .

cs:biometricVerificationPerformed a owl:DatatypeProperty ;
    rdfs:domain cs:VerificationPhase ;
    rdfs:range xsd:boolean ;
    rdfs:label "biometric verification performed"@en .

# Application Phase Properties
cs:injectionMethod a owl:DatatypeProperty ;
    rdfs:domain cs:ApplicationPhase ;
    rdfs:range xsd:string ;
    rdfs:label "injection method"@en .

cs:twoFactorRequired a owl:DatatypeProperty ;
    rdfs:domain cs:ApplicationPhase ;
    rdfs:range xsd:boolean ;
    rdfs:label "two factor required"@en .

cs:twoFactorType a owl:DatatypeProperty ;
    rdfs:domain cs:ApplicationPhase ;
    rdfs:range xsd:string ;
    rdfs:label "two factor type"@en .

cs:twoFactorCodeGenerated a owl:DatatypeProperty ;
    rdfs:domain cs:ApplicationPhase ;
    rdfs:range xsd:boolean ;
    rdfs:label "two factor code generated"@en .

cs:authenticationSuccessful a owl:DatatypeProperty ;
    rdfs:domain cs:ApplicationPhase ;
    rdfs:range xsd:boolean ;
    rdfs:label "authentication successful"@en .

# Synchronization Phase Properties
cs:deviceSyncRequired a owl:DatatypeProperty ;
    rdfs:domain cs:SynchronizationPhase ;
    rdfs:range xsd:boolean ;
    rdfs:label "device sync required"@en .

# Rotation Assessment Phase Properties
cs:credentialAge a owl:DatatypeProperty ;
    rdfs:domain cs:RotationAssessmentPhase ;
    rdfs:range xsd:integer ;
    rdfs:label "credential age"@en .

cs:rotationRecommended a owl:DatatypeProperty ;
    rdfs:domain cs:RotationAssessmentPhase ;
    rdfs:range xsd:boolean ;
    rdfs:label "rotation recommended"@en .

cs:rotationRequested a owl:DatatypeProperty ;
