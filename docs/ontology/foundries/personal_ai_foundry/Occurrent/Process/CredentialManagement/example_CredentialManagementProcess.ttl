# Example of a Voice-Initiated Credential Management Process for a Banking Platform

:financialAuthProcess a cs:CredentialManagementProcess ;
    rdfs:label "Financial Platform Authentication Process"@en ;
    cs:processId "cmp-2023-10-22-081542-78901" ;
    cs:initiatedBy :voiceCommand ;
    cs:executedBy :personalAiSystem ;
    cs:timestamp "2023-10-22T08:15:42Z"^^xsd:dateTime ;
    
    # Voice Command that initiated the process
    cs:voiceCommand [
        a paf:VoiceCommand ;
        cs:commandText "Log me into my Chase account" ;
        cs:commandIntent "authentication" ;
        cs:commandConfidence 0.97 ;
        cs:timestamp "2023-10-22T08:15:40Z"^^xsd:dateTime
    ] ;
    
    # Initiation Phase
    cs:initiationPhase [
        a cs:InitiationPhase ;
        cs:timestamp "2023-10-22T08:15:42Z"^^xsd:dateTime ;
        cs:platformIdentification true ;
        cs:platform :chaseBankPlatform ;
        cs:authenticationContext "user-initiated" ;
        cs:requestSource :smartSpeaker ;
        cs:detectedIntent "financial-authentication" ;
        cs:confidenceScore 0.95
    ] ;
    
    # Retrieval Phase
    cs:retrievalPhase [
        a cs:RetrievalPhase ;
        cs:timestamp "2023-10-22T08:15:43Z"^^xsd:dateTime ;
        cs:vaultAccessInitiated true ;
        cs:masterKeyVerification true ;
        cs:masterKeyMethod "voice-biometric" ;
        cs:decryptionPerformed true ;
        cs:credentialsRetrieved :chaseBankCredentials ;
        cs:localCacheUsed false ;
        cs:cloudVaultAccessed true
    ] ;
    
    # Verification Phase
    cs:verificationPhase [
        a cs:VerificationPhase ;
        cs:timestamp "2023-10-22T08:15:44Z"^^xsd:dateTime ;
        cs:credentialStatus "active" ;
        cs:expirationCheck true ;
        cs:credentialExpiration "2024-01-15"^^xsd:date ;
        cs:strengthVerification true ;
        cs:strengthScore 92 ;
        cs:userVerificationRequired true ;
        cs:userVerificationMethod "voice-confirmation" ;
        cs:userPrompt "I found your Chase Bank credentials. Would you like me to log you in on your phone?" ;
        cs:userResponse "Yes, please" ;
        cs:biometricVerificationPerformed true ;
        cs:biometricType "voice" ;
        cs:biometricMatchScore 0.98
    ] ;
    
    # Application Phase
    cs:applicationPhase [
        a cs:ApplicationPhase ;
        cs:timestamp "2023-10-22T08:15:47Z"^^xsd:dateTime ;
        cs:targetDevice :userSmartphone ;
        cs:applicationLaunched "Chase Mobile App" ;
        cs:injectionMethod "autofill-api" ;
        cs:formFieldsMapped true ;
        cs:credentialsApplied true ;
        cs:twoFactorRequired true ;
        cs:twoFactorType "authenticator-app" ;
        cs:twoFactorCodeRetrieved true ;
        cs:twoFactorCodeSource :authenticatorApp ;
        cs:twoFactorCodeApplied true ;
        cs:authenticationSuccessful true ;
        cs:loginScreenshotCaptured false ;
        cs:secureSessionEstablished true
    ] ;
    
    # Synchronization Phase
    cs:synchronizationPhase [
        a cs:SynchronizationPhase ;
        cs:timestamp "2023-10-22T08:15:48Z"^^xsd:dateTime ;
        cs:deviceSyncRequired true ;
        cs:syncTargets :userLaptop, :userTablet ;
        cs:syncType "credential-usage-metadata" ;
        cs:syncEncrypted true ;
        cs:syncCompleted true
    ] ;
    
    # Rotation Assessment Phase
    cs:rotationAssessmentPhase [
        a cs:RotationAssessmentPhase ;
        cs:timestamp "2023-10-22T08:15:49Z"^^xsd:dateTime ;
        cs:credentialAge 72 ;
        cs:rotationPolicy [
            a cs:RotationPolicy ;
            cs:maxAge 90 ;
            cs:weaknessThreshold 65 ;
            cs:breachCheckRequired true
        ] ;
        cs:breachCheckStatus "no-breach-detected" ;
        cs:rotationRecommended true ;
        cs:rotationRequested false ;
        cs:reasonForRecommendation "approaching-expiration" ;
        cs:rotationScheduled false ;
        cs:userNotified true ;
        cs:rotationReminderScheduled "2023-10-29T09:00:00Z"^^xsd:dateTime
    ] ;
    
    # Monitoring Phase
    cs:monitoringPhase [
        a cs:MonitoringPhase ;
        cs:timestamp "2023-10-22T08:15:50Z"^^xsd:dateTime ;
        cs:loginLocationVerified true ;
        cs:geolocation [
            a cs:GeolocationData ;
            cs:latitude 37.7749 ;
            cs:longitude -122.4194 ;
            cs:accuracy 50 ;
            cs:locationName "San Francisco, CA" ;
            cs:isKnownLocation true
        ] ;
        cs:deviceRecognized true ;
        cs:deviceName "User's iPhone 13" ;
        cs:networkVerified true ;
        cs:networkName "Home Wi-Fi" ;
        cs:suspiciousActivityDetected false ;
        cs:loginTimePatternNormal true ;
        cs:behavioralMatchScore 0.97
    ] ;
    
    # Completion Phase
    cs:completionPhase [
        a cs:CompletionPhase ;
        cs:timestamp "2023-10-22T08:15:51Z"^^xsd:dateTime ;
        cs:authenticationComplete true ;
        cs:usageLogged true ;
        cs:sessionHandoff true ;
        cs:handoffDestination "Chase Mobile App" ;
        cs:userNotified true ;
        cs:notificationMethod "voice" ;
        cs:notificationMessage "You're now logged into your Chase account on your phone" ;
        cs:processStatus "completed" ;
        cs:completionSummary [
            a cs:ProcessSummary ;
            cs:successStatus true ;
            cs:totalDuration 9 ;  # in seconds
            cs:userInteractions 2 ;
            cs:securityLevel "enhanced" 
        ]
    ] ;
    
    # Credential Details
    cs:credentials [
        a cs:StoredCredential ;
        cs:credentialId "cred-chase-78901" ;
        cs:platform :chaseBankPlatform ;
        cs:username "encrypted:AES256:8a7b6c5d4e3f2g1h" ;  # Encrypted representation
        cs:passwordLastModified "2023-08-10"^^xsd:date ;
        cs:strengthScore 92 ;
        cs:passwordLength 18 ;
        cs:containsSpecialChars true ;
        cs:containsNumbers true ;
        cs:containsMixedCase true ;
        cs:twoFactorEnabled true ;
        cs:twoFactorMethods "authenticator-app", "sms" ;
        cs:lastUsed "2023-10-20T14:22:51Z"^^xsd:dateTime ;
        cs:accessFrequency "weekly" ;
        cs:securityTier "financial" ;
        cs:failedAttempts 0 ;
        cs:tags "banking", "financial", "bills"
    ] ;
    
    # Platform Details
    cs:targetPlatform [
        a cs:Platform ;
        cs:platformId "chase-banking" ;
        cs:platformName "Chase Bank" ;
        cs:platformType "financial" ;
        cs:platformURL "https://www.chase.com/" ;
        cs:mobileAppId "com.chase.mobile" ;
        cs:authenticationMethods "username-password", "biometric", "2fa" ;
        cs:lockoutPolicy "5-attempts" ;
        cs:sessionTimeout 600 ;  # seconds
        cs:securityRating "high"
    ] ;
    
    # Process Metadata
    cs:processDuration 9 ;  # in seconds
    cs:processStatus "completed" ;
    cs:systemComponents :vaultSystem, :biometricSystem, :securityMonitor, :voiceAssistant ;
    cs:userDevices :userSmartphone, :userLaptop, :userTablet, :smartSpeaker ;
    cs:aiAssistance [
        a paf:AIAssistanceMetadata ;
        cs:intentRecognition true ;
        cs:contextAwareness true ;
        cs:deviceCoordination true ;
        cs:securityAssessment true ;
        cs:voiceAuthentication true ;
        cs:userPreferences [
            cs:preferredDeviceForFinancial :userSmartphone ;
            cs:preferredNotificationMethod "voice" ;
            cs:savedLoginLocations "home", "office" ;
            cs:securityPreferenceLevel "high"
        ]
    ] .

